  <DataSets>
    <DataSet Name="BasicInfoDataSet">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <CommandText>WITH WithoutDetails AS (SELECT        AttendanceWithoutAdjustment.EmployeeAttendanceCard_id AS EmpAttCardID, AttendanceWithoutAdjustment.FinalTotalOvertimeValue, 
                                                                                AttendanceWithoutAdjustmentDetail.NormalOvertimeValue, AttendanceWithoutAdjustmentDetail.ParticularOvertimeValue, 
                                                                                AttendanceWithoutAdjustmentDetail.HolidayOvertimeValue, MONTH(AttendanceRecord.Date) AS monthnumber, 
                                                                                AttendanceRecord.Id AS MonthId, AttendanceWithoutAdjustment.LatenessPenalty_id, 
                                                                                AttendanceWithoutAdjustment.NonAttendancePenalty_id, AttendanceWithoutAdjustment.FinalLatenessTotalValue, 
                                                                                AttendanceWithoutAdjustmentDetail.LatenessHoursValue, AttendanceWithoutAdjustmentDetail.NonAttendanceHoursValue, 
                                                                                AttendanceWithoutAdjustment.FinalNonAttendanceTotalValue
                                                       FROM            AttendanceWithoutAdjustment INNER JOIN
                                                                                AttendanceWithoutAdjustmentDetail ON 
                                                                                AttendanceWithoutAdjustment.Id = AttendanceWithoutAdjustmentDetail.AttendanceWithoutAdjustment_id INNER JOIN
                                                                                AttendanceRecord ON AttendanceRecord.Id = AttendanceWithoutAdjustment.AttendanceRecord_id
                                                       WHERE        (AttendanceWithoutAdjustment.IsVertualDeleted = 0) AND (AttendanceWithoutAdjustmentDetail.IsVertualDeleted = 0) AND 
                                                                                (AttendanceRecord.IsVertualDeleted = 0))
    SELECT        EmpAttCardID,
                                  (SELECT        Employee.FirstName + ' ' + Employee.FatherName + ' ' + Employee.LastName AS FullName
                                     FROM            EmployeeCard INNER JOIN
                                                              Employee ON EmployeeCard.Employee_id = Employee.Id
                                     WHERE        (EmployeeCard.IsVertualDeleted = 0) AND (Employee.IsVertualDeleted = 0) AND (EmployeeCard.Id = WithoutDetails_1.EmpAttCardID)) AS FullName,
                               monthnumber, MonthId, ISNULL
                                  ((SELECT        COUNT(*) AS Expr1
                                      FROM            AttendanceWithoutAdjustment AS AttendanceWithoutAdjustment_1 INNER JOIN
                                                                AttendanceWithoutAdjustmentDetail AS AttendanceWithoutAdjustmentDetail_1 ON 
                                                                AttendanceWithoutAdjustment_1.Id = AttendanceWithoutAdjustmentDetail_1.AttendanceWithoutAdjustment_id
                                      WHERE        (AttendanceWithoutAdjustmentDetail_1.ActualWorkValue &lt;= 0) AND (AttendanceWithoutAdjustmentDetail_1.IsOffDay = 0) AND 
                                                                (AttendanceWithoutAdjustmentDetail_1.HasMission = 0) AND (AttendanceWithoutAdjustmentDetail_1.HasVacation = 0) AND 
                                                                (AttendanceWithoutAdjustmentDetail_1.IsHoliday = 0) AND 
                                                                (AttendanceWithoutAdjustment_1.EmployeeAttendanceCard_id = WithoutDetails_1.EmpAttCardID) AND 
                                                                (MONTH(AttendanceWithoutAdjustmentDetail_1.Date) = WithoutDetails_1.monthnumber)), 0) AS CountAbsence,
                                  (SELECT        RIGHT('00' + CONVERT(varchar(8), FLOOR(ROUND(WithoutDetails_1.FinalTotalOvertimeValue, 2))), CASE WHEN LEN(CONVERT(varchar(8), 
                                                              FLOOR(ROUND(WithoutDetails_1.FinalTotalOvertimeValue, 2)))) &gt; 2 THEN 3 ELSE 2 END) + ':' + RIGHT('00' + CONVERT(varchar(8),
                                                                  (SELECT        CONVERT(int, (ROUND(WithoutDetails_1.FinalTotalOvertimeValue, 2) - FLOOR(ROUND(WithoutDetails_1.FinalTotalOvertimeValue, 
                                                                                              2))) * 60, 0) AS Expr1)), 2) AS Expr1) AS FinalTotalOvertimeValue, LatenessPenalty_id,
                                  (SELECT        Name
                                     FROM            DisciplinarySetting AS N
                                     WHERE        (Id = WithoutDetails_1.LatenessPenalty_id)) AS LatenessPenalty, NonAttendancePenalty_id,
                                  (SELECT        Name
                                     FROM            DisciplinarySetting AS N
                                     WHERE        (Id = WithoutDetails_1.NonAttendancePenalty_id)) AS NonAttendancePenalty,
                                  (SELECT        RIGHT('00' + CONVERT(varchar(8), FLOOR(ROUND(WithoutDetails_1.FinalNonAttendanceTotalValue, 2))), CASE WHEN LEN(CONVERT(varchar(8), 
                                                              FLOOR(ROUND(WithoutDetails_1.FinalNonAttendanceTotalValue, 2)))) &gt; 2 THEN 3 ELSE 2 END) + ':' + RIGHT('00' + CONVERT(varchar(8),
                                                                  (SELECT        CONVERT(int, (ROUND(WithoutDetails_1.FinalNonAttendanceTotalValue, 2) 
                                                                                              - FLOOR(ROUND(WithoutDetails_1.FinalNonAttendanceTotalValue, 2))) * 60, 0) AS Expr1)), 2) AS Expr1) 
                              AS FinalNonAttendanceTotalValue,
                                  (SELECT        RIGHT('00' + CONVERT(varchar(8), FLOOR(ROUND(WithoutDetails_1.FinalLatenessTotalValue, 2))), CASE WHEN LEN(CONVERT(varchar(8), 
                                                              FLOOR(ROUND(WithoutDetails_1.FinalLatenessTotalValue, 2)))) &gt; 2 THEN 3 ELSE 2 END) + ':' + RIGHT('00' + CONVERT(varchar(8),
                                                                  (SELECT        CONVERT(int, (ROUND(WithoutDetails_1.FinalLatenessTotalValue, 2) - FLOOR(ROUND(WithoutDetails_1.FinalLatenessTotalValue, 
                                                                                              2))) * 60, 0) AS Expr1)), 2) AS Expr1) AS FinalLatenessTotalValue,
                                  (SELECT        RIGHT('00' + CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(WithoutDetails_1.NormalOvertimeValue), 0), 2))), 
                                                              CASE WHEN LEN(CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(WithoutDetails_1.NormalOvertimeValue), 0), 2)))) &gt; 2 THEN 3 ELSE 2 END) 
                                                              + ':' + RIGHT('00' + CONVERT(varchar(8),
                                                                  (SELECT        CONVERT(int, (ROUND(ISNULL(SUM(WithoutDetails_1.NormalOvertimeValue), 0), 2) 
                                                                                              - FLOOR(ROUND(ISNULL(SUM(WithoutDetails_1.NormalOvertimeValue), 0), 2))) * 60, 0) AS Expr1)), 2) AS Expr1) 
                              AS TotalNormalOvertimeValue,
                                  (SELECT        RIGHT('00' + CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(WithoutDetails_1.ParticularOvertimeValue), 0), 2))), 
                                                              CASE WHEN LEN(CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(WithoutDetails_1.ParticularOvertimeValue), 0), 2)))) &gt; 2 THEN 3 ELSE 2 END) 
                                                              + ':' + RIGHT('00' + CONVERT(varchar(8),
                                                                  (SELECT        CONVERT(int, (ROUND(ISNULL(SUM(WithoutDetails_1.ParticularOvertimeValue), 0), 2) 
                                                                                              - FLOOR(ROUND(ISNULL(SUM(WithoutDetails_1.ParticularOvertimeValue), 0), 2))) * 60, 0) AS Expr1)), 2) AS Expr1) 
                              AS TotalParticularOvertimeValue,
                                  (SELECT        RIGHT('00' + CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(WithoutDetails_1.HolidayOvertimeValue), 0), 2))), 
                                                              CASE WHEN LEN(CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(WithoutDetails_1.HolidayOvertimeValue), 0), 2)))) &gt; 2 THEN 3 ELSE 2 END) 
                                                              + ':' + RIGHT('00' + CONVERT(varchar(8),
                                                                  (SELECT        CONVERT(int, (ROUND(ISNULL(SUM(WithoutDetails_1.HolidayOvertimeValue), 0), 2) 
                                                                                              - FLOOR(ROUND(ISNULL(SUM(WithoutDetails_1.HolidayOvertimeValue), 0), 2))) * 60, 0) AS Expr1)), 2) AS Expr1) 
                              AS TotalHolidayOvertimeValue,
                                  (SELECT        RIGHT('00' + CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(WithoutDetails_1.LatenessHoursValue), 0), 2))), 
                                                              CASE WHEN LEN(CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(WithoutDetails_1.LatenessHoursValue), 0), 2)))) &gt; 2 THEN 3 ELSE 2 END) 
                                                              + ':' + RIGHT('00' + CONVERT(varchar(8),
                                                                  (SELECT        CONVERT(int, (ROUND(ISNULL(SUM(WithoutDetails_1.LatenessHoursValue), 0), 2) 
                                                                                              - FLOOR(ROUND(ISNULL(SUM(WithoutDetails_1.LatenessHoursValue), 0), 2))) * 60, 0) AS Expr1)), 2) AS Expr1) 
                              AS InitialLatenessTotalValue,
                                  (SELECT        RIGHT('00' + CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(WithoutDetails_1.NonAttendanceHoursValue), 0), 2))), 
                                                              CASE WHEN LEN(CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(WithoutDetails_1.NonAttendanceHoursValue), 0), 2)))) 
                                                              &gt; 2 THEN 3 ELSE 2 END) + ':' + RIGHT('00' + CONVERT(varchar(8),
                                                                  (SELECT        CONVERT(int, (ROUND(ISNULL(SUM(WithoutDetails_1.NonAttendanceHoursValue), 0), 2) 
                                                                                              - FLOOR(ROUND(ISNULL(SUM(WithoutDetails_1.NonAttendanceHoursValue), 0), 2))) * 60, 0) AS Expr1)), 2) AS Expr1) 
                              AS InitialNonAttendanceTotalValue
     FROM            WithoutDetails AS WithoutDetails_1
     GROUP BY monthnumber, EmpAttCardID, MonthId, FinalTotalOvertimeValue, LatenessPenalty_id, NonAttendancePenalty_id, FinalNonAttendanceTotalValue, 
                              FinalLatenessTotalValue</CommandText>
      </Query>
