      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <CommandText>SELECT 0 MilitaryStatus,'أنهى خدمته' MilitaryStatusName
UNION
SELECT 1 MilitaryStatus,'معفى' MilitaryStatusName
UNION
SELECT 2 MilitaryStatus,'مؤجل' MilitaryStatusName
UNION
SELECT 3 MilitaryStatus,'وحيد' MilitaryStatusName
UNION
