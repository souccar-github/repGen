  <DataSets>
    <DataSet Name="BasicInfoDataSet">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        
MonthlyCard.Id AS MonthlyCardId, 
Employee.Code AS EmployeeCode, 
Employee.FirstName + ' ' + ISNULL(Employee.FatherName,'') + ' ' + Employee.LastName AS FullName, 
Month.Name AS MonthName, 
Month.Id AS MonthId, 
MonthlyCard.Salary, 
Employee.Id AS EmployeeId,
Node.Name AS NodeName,

--مجموع تعويضات الموظف
(Select 
ISNULL(SUM(MonthlyEmployeeBenefit.InitialValue), 0) 
From 
MonthlyEmployeeBenefit 
Where 
MonthlyEmployeeBenefit.MonthlyCard_id = MonthlyCard.ID  AND dbo.MonthlyEmployeeBenefit.IsVertualDeleted=0) 
As TotalEmployeeBenefits,
------------------

--مجموع حسميات الموظف
(Select 
ISNULL(SUM(MonthlyEmployeeDeduction.FinalValue), 0) 
From 
MonthlyEmployeeDeduction 
Where 
MonthlyEmployeeDeduction.MonthlyCard_id = MonthlyCard.ID  AND dbo.MonthlyEmployeeDeduction.IsVertualDeleted=0) 
As TotalEmployeeDeductions,
-------------------

--مجموع قروض الموظف
(Select 
ISNULL(SUM(LoanPayment.PaymentValue), 0) 
From 
LoanPayment 
Where 
LoanPayment.MonthlyCard_id = MonthlyCard.ID  AND dbo.LoanPayment.IsVertualDeleted=0)
As TotalEmployeeLoans,
-------------------

--الراتب الصافي للموظف
CASE Month.MonthType WHEN 0 THEN MonthlyCard.Salary +
(SELECT     ISNULL(SUM(MonthlyEmployeeBenefit.InitialValue), 0)
FROM         MonthlyEmployeeBenefit
WHERE     MonthlyEmployeeBenefit.MonthlyCard_id = MonthlyCard.ID AND dbo.MonthlyEmployeeBenefit.IsVertualDeleted = 0) -
(SELECT     ISNULL(SUM(MonthlyEmployeeDeduction.FinalValue), 0)
FROM         MonthlyEmployeeDeduction
WHERE     MonthlyEmployeeDeduction.MonthlyCard_id = MonthlyCard.ID AND dbo.MonthlyEmployeeDeduction.IsVertualDeleted = 0) -
(SELECT     ISNULL(SUM(LoanPayment.PaymentValue), 0)
FROM         LoanPayment
WHERE     LoanPayment.MonthlyCard_id = MonthlyCard.ID 
AND dbo.LoanPayment.IsVertualDeleted = 0 AND dbo.LoanPayment.IsExternalPayment = 0) 
             
WHEN 1 THEN
(SELECT     ISNULL(SUM(MonthlyEmployeeBenefit.InitialValue), 0)
FROM         MonthlyEmployeeBenefit
WHERE     MonthlyEmployeeBenefit.MonthlyCard_id = MonthlyCard.ID AND dbo.MonthlyEmployeeBenefit.IsVertualDeleted = 0) -
(SELECT     ISNULL(SUM(MonthlyEmployeeDeduction.FinalValue), 0)
FROM         MonthlyEmployeeDeduction
WHERE     MonthlyEmployeeDeduction.MonthlyCard_id = MonthlyCard.ID AND dbo.MonthlyEmployeeDeduction.IsVertualDeleted = 0)-
(SELECT     ISNULL(SUM(LoanPayment.PaymentValue), 0)
FROM         LoanPayment
WHERE     LoanPayment.MonthlyCard_id = MonthlyCard.ID 
AND dbo.LoanPayment.IsVertualDeleted = 0 AND dbo.LoanPayment.IsExternalPayment = 0)  END AS ActualFinalSalary
-------------------


, dbo.Month.Date AS MonthDate
FROM            
Month INNER JOIN
MonthlyCard ON MonthlyCard.Month_id = Month.Id INNER JOIN
EmployeeCard ON EmployeeCard.Id = MonthlyCard.PrimaryCard_id INNER JOIN
Employee ON Employee.Id = EmployeeCard.Employee_id LEFT OUTER JOIN
Node INNER JOIN
Position INNER JOIN
AssigningEmployeeToPosition ON Position.Id = AssigningEmployeeToPosition.Position_Id INNER JOIN
JobDescription ON Position.JobDescription_id = JobDescription.Id ON Node.Id = JobDescription.Node_id ON 
Employee.Id = AssigningEmployeeToPosition.Employee_id
WHERE        (MonthlyCard.IsVertualDeleted = 0) AND (Month.IsVertualDeleted = 0) AND (EmployeeCard.IsVertualDeleted = 0) AND (Employee.IsVertualDeleted = 0) </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
