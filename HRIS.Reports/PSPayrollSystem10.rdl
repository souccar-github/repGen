          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="employeeDataSet">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@SearchFullName">
            <Value>=Parameters!SearchFullName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        Id, FirstName + ' ' + ISNULL(FatherName, '') + ' ' + LastName AS FullName
FROM            Employee
WHERE        (IsVertualDeleted = 0) AND (FirstName + ' ' + LastName LIKE '%' + ISNULL(@SearchFullName, N'') + '%')
ORDER BY FullName</CommandText>
      </Query>
