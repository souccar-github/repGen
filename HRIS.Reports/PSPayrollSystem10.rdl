    <DataSet Name="MonthDataSet">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        Id, Format(Date, 'dd-mm-yyyy') + '_' + Name AS Month
FROM            Month
WHERE        (IsVertualDeleted = 0)</CommandText>
      </Query>
