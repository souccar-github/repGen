  <DataSets>
    <DataSet Name="MonthlyEmployeeBenefiDataSet">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>with WithoutDetails AS
(
SELECT        
EmployeeAttendanceCard_id  AS EmpAttCardID,
AttendanceWithoutAdjustment_id,
MONTH (dbo.AttendanceRecord.Date ) as monthnumber ,
FinalTotalOvertimeValue,
NormalOvertimeValue,
ParticularOvertimeValue,
HolidayOvertimeValue,
dbo.AttendanceRecord.Id MonthId,
LatenessPenalty_id,
NonAttendancePenalty_id,
FinalLatenessTotalValue,
LatenessHoursValue,
NonAttendanceHoursValue,
FinalNonAttendanceTotalValue

FROM
dbo.AttendanceWithoutAdjustment 
inner join dbo.AttendanceWithoutAdjustmentDetail ON dbo.AttendanceWithoutAdjustment.Id=dbo.AttendanceWithoutAdjustmentDetail.AttendanceWithoutAdjustment_id
INNER JOIN dbo.AttendanceRecord ON dbo.AttendanceRecord.Id = dbo.AttendanceWithoutAdjustment.AttendanceRecord_id
WHERE	AttendanceWithoutAdjustment.IsVertualDeleted = 0 AND
		AttendanceWithoutAdjustmentDetail.IsVertualDeleted = 0 AND
		AttendanceRecord.IsVertualDeleted = 0
 )

SELECT
EmpAttCardID,
AttendanceWithoutAdjustment_id,
(SELECT dbo.Employee.FirstName + ' ' + dbo.Employee.FatherName + ' ' + dbo.Employee.LastName AS FullName
 FROM	dbo.EmployeeCard INNER JOIN	dbo.Employee ON dbo.EmployeeCard.Employee_id = dbo.Employee.Id
 WHERE	EmployeeCard.IsVertualDeleted = 0 AND Employee.IsVertualDeleted = 0 AND dbo.EmployeeCard.Id = EmpAttCardID) FullName,
(SELECT	dbo.Employee.Id AS EmployeeId
 FROM	dbo.EmployeeCard INNER JOIN	dbo.Employee ON dbo.EmployeeCard.Employee_id = dbo.Employee.Id
 WHERE	EmployeeCard.IsVertualDeleted = 0 AND Employee.IsVertualDeleted = 0 AND dbo.EmployeeCard.Id = EmpAttCardID) EmployeeId,
monthnumber,
MonthId,
ISNULL((SELECT COUNT(*) FROM  dbo.AttendanceWithoutAdjustment 
INNER JOIN dbo.AttendanceWithoutAdjustmentDetail on dbo.AttendanceWithoutAdjustment.Id=dbo.AttendanceWithoutAdjustmentDetail.AttendanceWithoutAdjustment_id 
WHERE ActualWorkValue&lt;=0 AND IsOffDay=0 AND HasMission=0 AND HasVacation=0 AND IsHoliday=0
AND AttendanceWithoutAdjustment.EmployeeAttendanceCard_id=EmpAttCardID AND Month(AttendanceWithoutAdjustmentDetail.Date)=monthnumber),0)AS CountAbsence,
(SELECT        RIGHT('00' + CONVERT(varchar(8), FLOOR(ROUND(FinalTotalOvertimeValue, 2))), CASE WHEN LEN(CONVERT(varchar(8), FLOOR(ROUND(FinalTotalOvertimeValue, 2))))&gt;2 THEN 3 ELSE 2 END) + ':' + RIGHT('00' + CONVERT(varchar(8),
                                                                  (SELECT        CONVERT(int, (ROUND(FinalTotalOvertimeValue, 2) - FLOOR(ROUND(FinalTotalOvertimeValue, 2))) * 60, 0) 
                                                                                              AS Expr1)), 2) AS Expr1) AS FinalTotalOvertimeValue,
LatenessPenalty_id,
(SELECT N.Name FROM DisciplinarySetting N WHERE N.Id = LatenessPenalty_id) AS LatenessPenalty,
NonAttendancePenalty_id,
(SELECT N.Name FROM DisciplinarySetting N WHERE N.Id = NonAttendancePenalty_id) AS NonAttendancePenalty,
(SELECT        RIGHT('00' + CONVERT(varchar(8), FLOOR(ROUND(FinalNonAttendanceTotalValue, 2))), CASE WHEN LEN(CONVERT(varchar(8), FLOOR(ROUND(FinalNonAttendanceTotalValue, 2))))&gt;2 THEN 3 ELSE 2 END) + ':' + RIGHT('00' + CONVERT(varchar(8),
                                                                  (SELECT        CONVERT(int, (ROUND(FinalNonAttendanceTotalValue, 2) - FLOOR(ROUND(FinalNonAttendanceTotalValue, 2))) * 60, 0) 
                                                                                              AS Expr1)), 2) AS Expr1) AS FinalNonAttendanceTotalValue,
(SELECT        RIGHT('00' + CONVERT(varchar(8), FLOOR(ROUND(FinalLatenessTotalValue, 2))), CASE WHEN LEN(CONVERT(varchar(8), FLOOR(ROUND(FinalLatenessTotalValue, 2))))&gt;2 THEN 3 ELSE 2 END) + ':' + RIGHT('00' + CONVERT(varchar(8),
                                                                  (SELECT        CONVERT(int, (ROUND(FinalLatenessTotalValue, 2) - FLOOR(ROUND(FinalLatenessTotalValue, 2))) * 60, 0) 
                                                                                              AS Expr1)), 2) AS Expr1) AS FinalLatenessTotalValue, 


(SELECT        RIGHT('00' + CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(NormalOvertimeValue), 0), 2))), CASE WHEN LEN(CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(NormalOvertimeValue), 0), 2))))&gt;2 THEN 3 ELSE 2 END) 
                                                              + ':' + RIGHT('00' + CONVERT(varchar(8),
                                                                  (SELECT        CONVERT(int, (ROUND(ISNULL(SUM(NormalOvertimeValue), 0), 2) 
                                                                                              - FLOOR(ROUND(ISNULL(SUM(NormalOvertimeValue), 0), 2))) * 60, 0) AS Expr1)), 2) AS Expr1) AS TotalNormalOvertimeValue,

(SELECT        RIGHT('00' + CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(ParticularOvertimeValue), 0), 2))), CASE WHEN LEN(CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(ParticularOvertimeValue), 0), 2))))&gt;2 THEN 3 ELSE 2 END) 
                                                              + ':' + RIGHT('00' + CONVERT(varchar(8),
                                                                  (SELECT        CONVERT(int, (ROUND(ISNULL(SUM(ParticularOvertimeValue), 0), 2) 
                                                                                              - FLOOR(ROUND(ISNULL(SUM(ParticularOvertimeValue), 0), 2))) * 60, 0) AS Expr1)), 2) AS Expr1) AS TotalParticularOvertimeValue,

(SELECT        RIGHT('00' + CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(HolidayOvertimeValue), 0), 2))), CASE WHEN LEN(CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(HolidayOvertimeValue), 0), 2))))&gt;2 THEN 3 ELSE 2 END) 
                                                              + ':' + RIGHT('00' + CONVERT(varchar(8),
                                                                  (SELECT        CONVERT(int, (ROUND(ISNULL(SUM(HolidayOvertimeValue), 0), 2) 
                                                                                              - FLOOR(ROUND(ISNULL(SUM(HolidayOvertimeValue), 0), 2))) * 60, 0) AS Expr1)), 2) AS Expr1) AS TotalHolidayOvertimeValue,

(SELECT        RIGHT('00' + CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(LatenessHoursValue), 0), 2))), CASE WHEN LEN(CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(LatenessHoursValue), 0), 2))))&gt;2 THEN 3 ELSE 2 END) 
                                                              + ':' + RIGHT('00' + CONVERT(varchar(8),
                                                                  (SELECT        CONVERT(int, (ROUND(ISNULL(SUM(LatenessHoursValue), 0), 2) 
                                                                                              - FLOOR(ROUND(ISNULL(SUM(LatenessHoursValue), 0), 2))) * 60, 0) AS Expr1)), 2) AS Expr1) AS InitialLatenessTotalValue,


(SELECT        RIGHT('00' + CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(NonAttendanceHoursValue), 0), 2))), CASE WHEN LEN(CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(NonAttendanceHoursValue), 0), 2))))&gt;2 THEN 3 ELSE 2 END) 
                                                              + ':' + RIGHT('00' + CONVERT(varchar(8),
                                                                  (SELECT        CONVERT(int, (ROUND(ISNULL(SUM(NonAttendanceHoursValue), 0), 2) 
                                                                                              - FLOOR(ROUND(ISNULL(SUM(NonAttendanceHoursValue), 0), 2))) * 60, 0) AS Expr1)), 2) AS Expr1) AS InitialNonAttendanceTotalValue
FROM   WithoutDetails 
GROUP BY 
monthnumber,
EmpAttCardID,
AttendanceWithoutAdjustment_id,
MonthId,
FinalTotalOvertimeValue,
LatenessPenalty_id,
NonAttendancePenalty_id,
FinalNonAttendanceTotalValue,
FinalLatenessTotalValue</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
