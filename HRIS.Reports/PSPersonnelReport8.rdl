    <DataSet Name="MaritalStatusDataSet">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <CommandText>SELECT        0 AS MaritalStatus, 'أعزب' AS MaritalStatusName
UNION
SELECT        1 AS MaritalStatus, 'متزوج' AS MaritalStatusName
UNION
SELECT        2 AS MaritalStatus, 'مطلق' AS MaritalStatusName
UNION
SELECT        3 AS MaritalStatus, 'أرمل' AS MaritalStatusName
UNION
SELECT        4 AS MaritalStatus, 'خاطب' AS MaritalStatusName</CommandText>
      </Query>
