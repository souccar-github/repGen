  <DataSets>
    <DataSet Name="BasicInfoDataSet">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <CommandText>SELECT        
Employee.Id AS EmployeeId, 
Employee.FirstName, 
Employee.LastName, 
Employee.FatherName, 
Employee.FirstName + ' ' + Employee.FatherName + ' ' + Employee.LastName AS FullName, 
Employee.Code, 
Position.Code AS PositionCode,
dbo.JobDescription.Name AS JobDescriptionName,
Employee.MaritalStatus, 
CASE 
WHEN dbo.Employee.MaritalStatus = 0 THEN 'أعزب' 
WHEN dbo.Employee.MaritalStatus = 1 THEN 'متزوج' 
WHEN dbo.Employee.MaritalStatus = 2 THEN 'مطلق' 
WHEN dbo.Employee.MaritalStatus = 3 THEN 'أرمل' 
WHEN dbo.Employee.MaritalStatus = 4 THEN 'خاطب' 
END AS MaritalStatusName,
(SELECT COUNT(DISTINCT Id)
FROM Child
WHERE (Employee_id = Employee.Id) AND (IsVertualDeleted = 0)) AS ChildCount
FROM            
dbo.Employee 
INNER JOIN dbo.AssigningEmployeeToPosition ON dbo.Employee.Id = dbo.AssigningEmployeeToPosition.Employee_id 
INNER JOIN dbo.Position ON dbo.AssigningEmployeeToPosition.Position_Id = dbo.Position.Id --AND dbo.AssigningEmployeeToPosition.Id = dbo.Position.AssigningEmployeeToPosition_id 
INNER JOIN dbo.JobDescription ON dbo.JobDescription.Id = dbo.Position.JobDescription_id
INNER JOIN dbo.EmployeeCard ON dbo.Employee.Id = dbo.EmployeeCard.Employee_id
WHERE        
EmployeeCard.CardStatus = 1 AND 
AssigningEmployeeToPosition.IsPrimary = 'True' AND 
Employee.IsVertualDeleted = 0 AND
AssigningEmployeeToPosition.IsVertualDeleted = 0 AND 
Position.IsVertualDeleted = 0 AND 
EmployeeCard.IsVertualDeleted = 0</CommandText>
      </Query>
