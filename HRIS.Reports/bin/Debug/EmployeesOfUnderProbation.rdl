<CommandText>SELECT     
dbo.Employee.Id,
dbo.Employee.FirstName+' '+Employee.FatherName+' '+dbo.Employee.LastName AS FullName,     
dbo.Employee.FirstName, 
dbo.Employee.LastName, 
dbo.Employee.FatherName,
dbo.Employee.Code AS EmpCode, 
dbo.AssigningEmployeeToPosition.Position_Id, 
dbo.JobDescription.Name AS JobDescriptionName,
dbo.EmployeeCard.StartWorkingDate, 
dbo.EmployeeCard.ProbationPeriodEndDate,
dbo.Node.Name AS NodeName
FROM dbo.Employee INNER JOIN dbo.EmployeeCard 
ON dbo.Employee.Id = dbo.EmployeeCard.Employee_id AND dbo.EmployeeCard.CardStatus = 1 
	AND Employee.IsVertualDeleted = 0 AND EmployeeCard.IsVertualDeleted = 0 
	AND  dbo.EmployeeCard.StartWorkingDate IS NOT NULL 
	AND  dbo.EmployeeCard.ProbationPeriodEndDate IS NOT NULL

INNER JOIN dbo.AssigningEmployeeToPosition ON dbo.Employee.Id = dbo.AssigningEmployeeToPosition.Employee_id 
	AND AssigningEmployeeToPosition.IsVertualDeleted = 0

INNER JOIN dbo.Position ON dbo.AssigningEmployeeToPosition.Position_Id = dbo.Position.Id 
	AND Position.IsVertualDeleted = 0

INNER JOIN dbo.JobDescription ON dbo.JobDescription.Id = dbo.Position.JobDescription_id 
	AND JobDescription.IsVertualDeleted = 0 

INNER JOIN dbo.Node ON dbo.JobDescription.Node_id=dbo.Node.Id 
	AND dbo.Node.IsVertualDeleted=0

where 

dbo.EmployeeCard.StartWorkingDate &lt;= GETDATE()
AND dbo.EmployeeCard.ProbationPeriodEndDate &gt;= GETDATE()

</CommandText></Query><Fields><Field Name="Id"><DataField>Id</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="FullName"><DataField>FullName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="FirstName"><DataField>FirstName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="LastName"><DataField>LastName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="FatherName"><DataField>FatherName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="EmpCode"><DataField>EmpCode</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Position_Id"><DataField>Position_Id</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="JobDescriptionName"><DataField>JobDescriptionName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="StartWorkingDate"><DataField>StartWorkingDate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="ProbationPeriodEndDate"><DataField>ProbationPeriodEndDate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="NodeName"><DataField>NodeName</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet></DataSets><rd:ReportUnitType>Inch</rd:ReportUnitType><rd:ReportID>0bde8672-710b-4832-8349-1e1cdf424472</rd:ReportID></Report>