FROM            Node
WHERE        (IsVertualDeleted = 0)
ORDER BY Name</CommandText></Query><Fields><Field Name="Id"><DataField>Id</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="Name"><DataField>Name</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Parent_Id"><DataField>Parent_Id</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field></Fields></DataSet><DataSet Name="ChildNodeDataSet"><Query><DataSourceName>DataSource</DataSourceName><QueryParameters><QueryParameter Name="@Node"><Value>=Parameters!Node.Value</Value></QueryParameter></QueryParameters><CommandText>SET                
NOCOUNT ON; WITH EntityChildren AS
(SELECT *
FROM 
dbo.Node
WHERE 
Node.Id = @Node

UNION ALL

SELECT e.*
FROM 
dbo.Node e INNER JOIN
EntityChildren e2 ON e.Parent_Id = e2.Id)
SELECT *
FROM EntityChildren

UNION ALL
SELECT 0 AS Id,0,'بلا عقدة' AS Name,null,0,0,0</CommandText></Query><Fields><Field Name="Id"><DataField>Id</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="IsVertualDeleted"><DataField>IsVertualDeleted</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="Name"><DataField>Name</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Code"><DataField>Code</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="IsHistorical"><DataField>IsHistorical</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="Type_id"><DataField>Type_id</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="Parent_Id"><DataField>Parent_Id</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field></Fields></DataSet><DataSet Name="BasicInfoDataSet"><Query><DataSourceName>DataSource</DataSourceName><CommandText>SELECT        
dbo.Node.Id AS NodeId, 
(SELECT N.Name FROM Node N WHERE  N.Id=dbo.Node.Id) NodeName,
dbo.Employee.Id AS EmployeeId, 
dbo.Employee.FirstName, 
dbo.Employee.LastName, 
dbo.Employee.FatherName, 
dbo.Employee.FirstName +' '+ dbo.Employee.FatherName +' '+ dbo.Employee.LastName FullName,
dbo.AppraisalPhase.Id AppraisalPhaseId, 
dbo.AppraisalPhase.CreationDate AS CreationDate, 
dbo.AppraisalPhase.StartDate AS StartDate, 
dbo.AppraisalPhase.EndDate AS EndDate,
dbo.AppraisalPhaseWorkflow.FinalMarkAfterApproval,
dbo.AppraisalPhaseWorkflow.Id AS AppraisalPhaseWorkflowId,
ROUND((SELECT SUM(dbo.Appraisal.AppraisalValue) FROM Appraisal WHERE  dbo.Appraisal.PhaseWorkflow_id=dbo.AppraisalPhaseWorkflow.Id),0) AS FinalMark
FROM            
dbo.JobDescription 
INNER JOIN dbo.Position ON dbo.JobDescription.Id = dbo.Position.JobDescription_id 
INNER JOIN dbo.Node ON dbo.JobDescription.Node_id = dbo.Node.Id 
INNER JOIN dbo.AssigningEmployeeToPosition ON dbo.AssigningEmployeeToPosition.Position_Id = dbo.Position.Id
--dbo.AssigningEmployeeToPosition ON dbo.Position.AssigningEmployeeToPosition_id = dbo.AssigningEmployeeToPosition.Id 
--AND dbo.Position.Id = dbo.AssigningEmployeeToPosition.Position_Id 
INNER JOIN dbo.Employee ON dbo.Employee.Id = dbo.AssigningEmployeeToPosition.Employee_id
INNER JOIN dbo.AppraisalPhaseWorkflow ON dbo.Position.Id = dbo.AppraisalPhaseWorkflow.Position_id 
INNER JOIN dbo.AppraisalPhase ON dbo.AppraisalPhaseWorkflow.Phase_Id = dbo.AppraisalPhase.Id
WHERE 
((SELECT SUM(dbo.Appraisal.AppraisalValue) FROM Appraisal WHERE  dbo.Appraisal.PhaseWorkflow_id=dbo.AppraisalPhaseWorkflow.Id))&lt;&gt;0 AND
dbo.JobDescription.IsVertualDeleted = 0 AND  
dbo.Position.IsVertualDeleted = 0 AND  
dbo.Node.IsVertualDeleted = 0 AND  
dbo.AssigningEmployeeToPosition.IsVertualDeleted = 0 AND  
dbo.Employee.IsVertualDeleted = 0 AND  
dbo.AppraisalPhaseWorkflow.IsVertualDeleted = 0 AND  
dbo.AppraisalPhase.IsVertualDeleted = 0 </CommandText></Query><Fields><Field Name="NodeId"><DataField>NodeId</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="NodeName"><DataField>NodeName</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="EmployeeId"><DataField>EmployeeId</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="FirstName"><DataField>FirstName</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="LastName"><DataField>LastName</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="FatherName"><DataField>FatherName</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="FullName"><DataField>FullName</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="AppraisalPhaseId"><DataField>AppraisalPhaseId</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="CreationDate"><DataField>CreationDate</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.DateTime</rd:TypeName></Field><Field Name="StartDate"><DataField>StartDate</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.DateTime</rd:TypeName></Field><Field Name="EndDate"><DataField>EndDate</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.DateTime</rd:TypeName></Field><Field Name="FinalMarkAfterApproval"><DataField>FinalMarkAfterApproval</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Single</rd:TypeName></Field><Field Name="AppraisalPhaseWorkflowId"><DataField>AppraisalPhaseWorkflowId</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="FinalMark"><DataField>FinalMark</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Double</rd:TypeName></Field></Fields><Filters><Filter><FilterExpression>=Fields!NodeId.Value</FilterExpression><Operator>In</Operator><FilterValues><FilterValue>=Parameters!ChildNode.Value</FilterValue></FilterValues></Filter><Filter><FilterExpression>=Fields!AppraisalPhaseId.Value</FilterExpression><Operator>In</Operator><FilterValues><FilterValue>=Parameters!AppraisalPhase.Value</FilterValue></FilterValues></Filter></Filters></DataSet><DataSet Name="AppraisalPhaseDataSet"><Query><DataSourceName>DataSource</DataSourceName><CommandText>  SELECT 
  A.IsVertualDeleted,
A.Id, A.CreationDate, 
A.StartDate, 
A.EndDate, 
CAST(CONVERT(VARCHAR(11),A.StartDate,106) AS nvarchar) +' - '+CAST(CONVERT(VARCHAR(11),A.EndDate,106) AS nvarchar) AS PhaseTitle,
A.Description, 
A.Period, 
A.Month, 
A.Quarter, 
A.SemiAnnual, 
A.Year, 
A.TemplateSetting_id, 
A.AppraisalPhaseSetting_id
