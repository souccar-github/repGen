FROM            Node
WHERE        (IsVertualDeleted = 0)
ORDER BY Name</CommandText></Query><Fields><Field Name="Id"><DataField>Id</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="Name"><DataField>Name</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Parent_Id"><DataField>Parent_Id</DataField><rd:TypeName>System.Int32</rd:TypeName></Field></Fields><Filters><Filter><FilterExpression>=IIF(InStr(LCase(Fields!Name.Value) , LCase(Parameters!SearchNode.Value)),TRUE,FALSE)</FilterExpression><Operator>Equal</Operator><FilterValues><FilterValue DataType="Boolean">true</FilterValue></FilterValues></Filter></Filters></DataSet><DataSet Name="ChildNodeDataSet"><Query><DataSourceName>DataSource</DataSourceName><QueryParameters><QueryParameter Name="@Node"><Value>=Parameters!JobDesc0JobDescription0Node.Value</Value></QueryParameter></QueryParameters><CommandText>SET                
NOCOUNT ON; WITH EntityChildren AS
(SELECT *
FROM 
dbo.Node
WHERE 
Node.Id = @Node

UNION ALL

SELECT e.*
FROM 
dbo.Node e INNER JOIN
EntityChildren e2 ON e.Parent_Id = e2.Id)
SELECT *
FROM EntityChildren

UNION ALL
SELECT 0 AS Id,0,'بلا عقدة' AS Name,null,0,0,0</CommandText></Query><Fields><Field Name="Id"><DataField>Id</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="IsVertualDeleted"><DataField>IsVertualDeleted</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="Name"><DataField>Name</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Code"><DataField>Code</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="IsHistorical"><DataField>IsHistorical</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="Type_id"><DataField>Type_id</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="Parent_Id"><DataField>Parent_Id</DataField><rd:TypeName>System.Int32</rd:TypeName></Field></Fields><Filters><Filter><FilterExpression>=IIF(InStr(LCase(Fields!Name.Value) , LCase(Parameters!SearchChildNode.Value)),TRUE,FALSE)</FilterExpression><Operator>Equal</Operator><FilterValues><FilterValue DataType="Boolean">true</FilterValue></FilterValues></Filter></Filters></DataSet><DataSet Name="BasicInfoDataSet"><Query><DataSourceName>DataSource</DataSourceName><QueryParameters><QueryParameter Name="@FromDate"><Value>=Parameters!LabelFromLeavesDate.Value</Value></QueryParameter><QueryParameter Name="@ToDate"><Value>=Parameters!LabelToLeavesDate.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT        LeaveRequest.EmployeeCard_id AS EmployeeCardId,
                             (SELECT        TOP (1) Employee_id
                                FROM            EmployeeCard AS EC
                                WHERE        (Id = LeaveRequest.EmployeeCard_id) AND (IsVertualDeleted = 0)) AS EmployeeId, LeaveSetting.Id AS LeaveSettingId, LeaveSetting.Name AS LeaveSettingName, LeaveSetting.Type_id AS LeaveSettingTypeId, 
                         LeaveRequest.IsHourlyLeave,
                             (SELECT        FullName
                                FROM            Souccar_Security_User
                                WHERE        (Id = LeaveRequest.Creator_id) AND (IsVertualDeleted = 0)) AS Creator,
                             (SELECT        TOP (1) Name
                                FROM            LeaveType AS LT
                                WHERE        (Id = LeaveSetting.Type_id) AND (IsVertualDeleted = 0)) AS LeaveTypeName,
                             (SELECT        TOP (1) Id
                                FROM            LeaveType AS LT
                                WHERE        (Id = LeaveSetting.Type_id) AND (IsVertualDeleted = 0)) AS LeaveTypeId, LeaveRequest.LeaveStatus, LeaveRequest.StartDate, LeaveRequest.EndDate, LeaveRequest.RequestDate, 
                         LeaveRequest.LeaveReason_id,
                             (SELECT        TOP (1) Name
                                FROM            LeaveReason
                                WHERE        (Id = LeaveRequest.LeaveReason_id) AND (IsVertualDeleted = 0)) AS LeaveReason, DATEDIFF(DD, LeaveRequest.StartDate, LeaveRequest.EndDate) + 1 AS LeaveDays, 
                         CASE WHEN dbo.LeaveRequest.IsHourlyLeave = 0 THEN 0 ELSE ROUND(CAST(DATEDIFF(MINUTE, dbo.LeaveRequest.FromTime, dbo.LeaveRequest.ToTime) AS float) / 60, 2) END AS HourlyLeave, LeaveSetting.Balance, 
                         ISNULL
                             ((SELECT        TOP (1) Balance
                                 FROM            BalanceSlice AS BS
                                 WHERE        (LeaveSetting_id = LeaveSetting.Id)), 0) AS BalanceSlice, LeaveRequest.SpentDays, CASE WHEN ISNULL
                             ((SELECT        TOP 1 BS.Balance
                                 FROM            dbo.BalanceSlice BS
                                 WHERE        BS.LeaveSetting_id = dbo.LeaveSetting.Id AND BS.IsVertualDeleted = 0), 0) != 0 THEN ISNULL
                             ((SELECT        TOP 1 BS.Balance
                                 FROM            dbo.BalanceSlice BS
                                 WHERE        BS.LeaveSetting_id = dbo.LeaveSetting.Id AND BS.IsVertualDeleted = 0), 0) ELSE dbo.LeaveSetting.Balance END AS LeaveBalance, LeaveSetting.HasMaximumNumber, LeaveSetting.MaximumNumber,
                             (SELECT        TOP (1) COUNT(Id) AS Expr1
                                FROM            LeaveRequest AS LR
                                WHERE        (EmployeeCard_id = LeaveRequest.EmployeeCard_id) AND (LeaveSetting_id = LeaveRequest.LeaveSetting_id) AND (IsVertualDeleted = 0)
                                GROUP BY LeaveSetting_id) AS NumberOfGrantedLeave, LeaveSetting.MaximumNumber -
                             (SELECT        TOP (1) COUNT(Id) AS Expr1
                                FROM            LeaveRequest AS LR
                                WHERE        (EmployeeCard_id = LeaveRequest.EmployeeCard_id) AND (LeaveSetting_id = LeaveRequest.LeaveSetting_id) AND (IsVertualDeleted = 0)
                                GROUP BY LeaveSetting_id) AS NumberOfRemainLeave, 0 AS WorkflowStatus,
                             (SELECT        TOP (1) FirstName + ' ' + FatherName + ' ' + LastName AS Expr1
                                FROM            Employee
                                WHERE        (Id =
                                                             (SELECT        TOP (1) Employee_id
                                                                FROM            EmployeeCard AS EC
                                                                WHERE        (Id = LeaveRequest.EmployeeCard_id) AND (IsVertualDeleted = 0)))) AS FullName,
                             (SELECT        TOP (1) Node.Id
                                FROM            Node INNER JOIN
                                                         JobDescription ON Node.Id = JobDescription.Node_id INNER JOIN
                                                         Position ON JobDescription.Id = Position.JobDescription_id INNER JOIN
                                                         AssigningEmployeeToPosition ON Position.Id = AssigningEmployeeToPosition.Position_Id AND Node.IsVertualDeleted = 0 AND JobDescription.IsVertualDeleted = 0 AND Position.IsVertualDeleted = 0 AND 
                                                         AssigningEmployeeToPosition.IsVertualDeleted = 0 AND AssigningEmployeeToPosition.IsPrimary = 'TRUE' AND AssigningEmployeeToPosition.Employee_id =
                                                             (SELECT        TOP (1) Employee_id
                                                                FROM            EmployeeCard AS EC
                                                                WHERE        (Id = LeaveRequest.EmployeeCard_id) AND (IsVertualDeleted = 0))) AS NodeId,
                             (SELECT        TOP (1) Node_1.Name
                                FROM            Node AS Node_1 INNER JOIN
                                                         JobDescription AS JobDescription_2 ON Node_1.Id = JobDescription_2.Node_id INNER JOIN
                                                         Position AS Position_3 ON JobDescription_2.Id = Position_3.JobDescription_id INNER JOIN
                                                         AssigningEmployeeToPosition AS AssigningEmployeeToPosition_3 ON Position_3.Id = AssigningEmployeeToPosition_3.Position_Id AND Node_1.IsVertualDeleted = 0 AND 
                                                         JobDescription_2.IsVertualDeleted = 0 AND Position_3.IsVertualDeleted = 0 AND AssigningEmployeeToPosition_3.IsVertualDeleted = 0 AND AssigningEmployeeToPosition_3.IsPrimary = 'TRUE' AND 
                                                         AssigningEmployeeToPosition_3.Employee_id =
                                                             (SELECT        TOP (1) Employee_id
                                                                FROM            EmployeeCard AS EC
                                                                WHERE        (Id = LeaveRequest.EmployeeCard_id) AND (IsVertualDeleted = 0))) AS NodeName,
                             (SELECT        TOP (1) JobDescription_1.Name
                                FROM            JobDescription AS JobDescription_1 INNER JOIN
                                                         Position AS Position_2 ON JobDescription_1.Id = Position_2.JobDescription_id INNER JOIN
                                                         AssigningEmployeeToPosition AS AssigningEmployeeToPosition_2 ON Position_2.Id = AssigningEmployeeToPosition_2.Position_Id AND JobDescription_1.IsVertualDeleted = 0 AND 
                                                         Position_2.IsVertualDeleted = 0 AND AssigningEmployeeToPosition_2.IsVertualDeleted = 0 AND AssigningEmployeeToPosition_2.IsPrimary = 'TRUE' AND AssigningEmployeeToPosition_2.Employee_id =
                                                             (SELECT        TOP (1) Employee_id
                                                                FROM            EmployeeCard AS EC
                                                                WHERE        (Id = LeaveRequest.EmployeeCard_id) AND (IsVertualDeleted = 0))) AS JobDescriptionName,
                             (SELECT        TOP (1) Position_1.Code
                                FROM            Position AS Position_1 INNER JOIN
                                                         AssigningEmployeeToPosition AS AssigningEmployeeToPosition_1 ON Position_1.AssigningEmployeeToPosition_id = AssigningEmployeeToPosition_1.Id AND Position_1.IsVertualDeleted = 0 AND 
                                                         AssigningEmployeeToPosition_1.IsVertualDeleted = 0 AND AssigningEmployeeToPosition_1.IsPrimary = 'TRUE' AND AssigningEmployeeToPosition_1.Employee_id =
                                                             (SELECT        TOP (1) Employee_id
                                                                FROM            EmployeeCard AS EC
                                                                WHERE        (Id = LeaveRequest.EmployeeCard_id) AND (IsVertualDeleted = 0))) AS PositionCode
FROM            LeaveRequest INNER JOIN
                         LeaveSetting ON LeaveRequest.LeaveSetting_id = LeaveSetting.Id
WHERE        (LeaveRequest.IsVertualDeleted = 0) AND (LeaveSetting.IsVertualDeleted = 0) AND (LeaveRequest.LeaveStatus &lt;&gt; 1) AND (LeaveRequest.IsVertualDeleted = 0) AND (LeaveSetting.IsVertualDeleted = 0) AND 
                         (LeaveRequest.LeaveStatus &lt;&gt; 1) AND (LeaveRequest.IsVertualDeleted = 0) AND (LeaveSetting.IsVertualDeleted = 0) AND (LeaveRequest.LeaveStatus &lt;&gt; 1) AND (LeaveRequest.StartDate &gt;= @FromDate) AND 
                         (LeaveRequest.StartDate &lt;= @ToDate) OR
                         (LeaveRequest.IsVertualDeleted = 0) AND (LeaveSetting.IsVertualDeleted = 0) AND (LeaveRequest.LeaveStatus &lt;&gt; 1) AND (LeaveRequest.IsVertualDeleted = 0) AND (LeaveSetting.IsVertualDeleted = 0) AND 
                         (LeaveRequest.LeaveStatus &lt;&gt; 1) AND (LeaveRequest.IsVertualDeleted = 0) AND (LeaveSetting.IsVertualDeleted = 0) AND (LeaveRequest.LeaveStatus &lt;&gt; 1) AND (LeaveRequest.EndDate &gt;= @FromDate) AND 
                         (LeaveRequest.EndDate &lt;= @ToDate) OR
                         (LeaveRequest.IsVertualDeleted = 0) AND (LeaveSetting.IsVertualDeleted = 0) AND (LeaveRequest.LeaveStatus &lt;&gt; 1) AND (LeaveRequest.IsVertualDeleted = 0) AND (LeaveSetting.IsVertualDeleted = 0) AND 
                         (LeaveRequest.LeaveStatus &lt;&gt; 1) AND (LeaveRequest.IsVertualDeleted = 0) AND (LeaveSetting.IsVertualDeleted = 0) AND (LeaveRequest.LeaveStatus &lt;&gt; 1) AND (LeaveRequest.StartDate &lt; @FromDate) AND 
                         (LeaveRequest.EndDate &gt; @ToDate)
ORDER BY LeaveSettingTypeId</CommandText></Query><Fields><Field Name="FullName"><DataField>FullName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="EmployeeId"><DataField>EmployeeId</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="LeaveSettingId"><DataField>LeaveSettingId</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="LeaveSettingName"><DataField>LeaveSettingName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="EmployeeCardId"><DataField>EmployeeCardId</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="LeaveSettingTypeId"><DataField>LeaveSettingTypeId</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="IsHourlyLeave"><DataField>IsHourlyLeave</DataField><rd:TypeName>System.Boolean</rd:TypeName></Field><Field Name="Creator"><DataField>Creator</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="LeaveTypeId"><DataField>LeaveTypeId</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="LeaveTypeName"><DataField>LeaveTypeName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="LeaveStatus"><DataField>LeaveStatus</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="LeaveBalance"><DataField>LeaveBalance</DataField><rd:TypeName>System.Double</rd:TypeName></Field><Field Name="StartDate"><DataField>StartDate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="EndDate"><DataField>EndDate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="RequestDate"><DataField>RequestDate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="HasMaximumNumber"><DataField>HasMaximumNumber</DataField><rd:TypeName>System.Boolean</rd:TypeName></Field><Field Name="LeaveReason_id"><DataField>LeaveReason_id</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="MaximumNumber"><DataField>MaximumNumber</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="LeaveReason"><DataField>LeaveReason</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="LeaveDays"><DataField>LeaveDays</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="HourlyLeave"><DataField>HourlyLeave</DataField><rd:TypeName>System.Double</rd:TypeName></Field><Field Name="Balance"><DataField>Balance</DataField><rd:TypeName>System.Double</rd:TypeName></Field><Field Name="BalanceSlice"><DataField>BalanceSlice</DataField><rd:TypeName>System.Double</rd:TypeName></Field><Field Name="SpentDays"><DataField>SpentDays</DataField><rd:TypeName>System.Double</rd:TypeName></Field><Field Name="NumberOfGrantedLeave"><DataField>NumberOfGrantedLeave</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="NumberOfRemainLeave"><DataField>NumberOfRemainLeave</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="WorkflowStatus"><DataField>WorkflowStatus</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="NodeId"><DataField>NodeId</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="JobDescriptionName"><DataField>JobDescriptionName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="NodeName"><DataField>NodeName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="PositionCode"><DataField>PositionCode</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields><Filters><Filter><FilterExpression>=Fields!NodeId.Value</FilterExpression><Operator>In</Operator><FilterValues><FilterValue>=Parameters!ChildNode.Value</FilterValue></FilterValues></Filter><Filter><FilterExpression>=Fields!LeaveTypeId.Value</FilterExpression><Operator>In</Operator><FilterValues><FilterValue>=Parameters!EmployeeRelationServices0Indexes0LeaveType.Value</FilterValue></FilterValues></Filter></Filters></DataSet><DataSet Name="LeaveTypeDataSet"><Query><DataSourceName>DataSource</DataSourceName><CommandText>SELE