SELECT        
dbo.Employee.Id, 
dbo.Employee.FirstName, 
dbo.Employee.LastName, 
dbo.Employee.FatherName, 
dbo.Employee.FirstName +' '+ dbo.Employee.FatherName +' '+ dbo.Employee.LastName FullName,
dbo.Employee.Code,
dbo.CertificationType.Id as CertificationTypeId,
dbo.CertificationType.Name as CertificationTypeName,
REPLACE(ISNULL(CONVERT(DATE, dbo.Certification.DateOfIssuance), ''), '1900-01-01', '') AS DateOfIssuance,
REPLACE(ISNULL(CONVERT(DATE, dbo.Certification.ExpirationDate), ''), '1900-01-01', '') AS ExpirationDate,
dbo.Country.Id as PlaceOfIssuanceId,
dbo.Country.Name as PlaceOfIssuanceName,
dbo.JobDescription.Name AS JobDescriptionName,
dbo.Node.Name AS NodeName
FROM
dbo.Employee 
INNER JOIN dbo.Certification
ON dbo.Certification.Employee_id=dbo.Employee.Id AND dbo.Certification.IsVertualDeleted=0 AND dbo.Employee.IsVertualDeleted=0
INNER JOIN dbo.AssigningEmployeeToPosition ON dbo.Employee.Id = dbo.AssigningEmployeeToPosition.Employee_id And dbo.AssigningEmployeeToPosition.IsPrimary='True' AND Employee.IsVertualDeleted=0 AND AssigningEmployeeToPosition.IsVertualDeleted=0
INNER JOIN dbo.Position ON dbo.AssigningEmployeeToPosition.Position_Id = dbo.Position.Id AND Position.IsVertualDeleted=0
INNER JOIN dbo.JobDescription ON dbo.JobDescription.Id = dbo.Position.JobDescription_id AND JobDescription.IsVertualDeleted=0 
INNER JOIN dbo.Node ON dbo.JobDescription.Node_id=dbo.Node.Id AND dbo.Node.IsVertualDeleted=0
INNER JOIN dbo.CertificationType
ON dbo.CertificationType.Id=dbo.Certification.Type_id AND dbo.CertificationType.Id in (@CertificationType)
INNER JOIN dbo.Country
ON dbo.Country.Id=dbo.Certification.PlaceOfIssuance_id

select Id,Name from CertificationType where IsVertualDeleted=0</CommandText></Query><Fields><Field Name="Id"><DataField>Id</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="FirstName"><DataField>FirstName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="LastName"><DataField>LastName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="FatherName"><DataField>FatherName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="FullName"><DataField>FullName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Code"><DataField>Code</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="CertificationTypeId"><DataField>CertificationTypeId</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="CertificationTypeName"><DataField>CertificationTypeName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="DateOfIssuance"><DataField>DateOfIssuance</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ExpirationDate"><DataField>ExpirationDate</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="PlaceOfIssuanceId"><DataField>PlaceOfIssuanceId</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="PlaceOfIssuanceName"><DataField>PlaceOfIssuanceName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="JobDescriptionName"><DataField>JobDescriptionName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="NodeName"><DataField>NodeName</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="CertificationTypeDataSet"><Query><DataSourceName>CertificateDataSource</DataSourceName><CommandText>select Id,Name from CertificationType where IsVertualDeleted=0</CommandText></Query><Fields><Field Name="Id"><DataField>Id</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="Name"><DataField>Name</DataField><rd: