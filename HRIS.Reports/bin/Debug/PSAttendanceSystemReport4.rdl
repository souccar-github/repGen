><Height>0.91983cm</Height><Width>3.11058cm</Width><Style><Border><Style>None</Style></Border><VerticalAlign>Middle</VerticalAlign><PaddingLeft>2pt</PaddingLeft><PaddingRight>2pt</PaddingRight><PaddingTop>2pt</PaddingTop><PaddingBottom>2pt</PaddingBottom></Style></Textbox><Textbox Name="Textbox0012"><CanGrow>true</CanGrow><KeepTogether>true</KeepTogether><Paragraphs><Paragraph><TextRuns><TextRun><Value>/</Value><Style><FontFamily>Tahoma</FontFamily><FontSize>12pt</FontSize></Style></TextRun><TextRun><Value>=Globals!TotalPages</Value><Style><FontFamily>Tahoma</FontFamily><FontSize>12pt</FontSize></Style></TextRun></TextRuns><Style><TextAlign>Left</TextAlign></Style></Paragraph></Paragraphs><Top>0cm</Top><Left>17.62422cm</Left><Height>0.91983cm</Height><Width>3.22688cm</Width><ZIndex>1</ZIndex><Style><Border><Style>None</Style></Border><VerticalAlign>Middle</VerticalAlign><PaddingLeft>2pt</PaddingLeft><PaddingRight>2pt</PaddingRight><PaddingTop>2pt</PaddingTop><PaddingBottom>2pt</PaddingBottom></Style></Textbox></ReportItems><Style><Border><Style>None</Style></Border></Style></PageFooter><PageHeight>29.7cm</PageHeight><PageWidth>37cm</PageWidth><LeftMargin>0.5cm</LeftMargin><RightMargin>0.5cm</RightMargin><TopMargin>2cm</TopMargin><BottomMargin>2cm</BottomMargin><ColumnSpacing>0.13cm</ColumnSpacing><Style /></Page><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="DataSource"><DataSourceReference>SharedDataSource</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>9a350a56-51d6-40e0-b0cf-07e98984a2ce</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="BasicInfoDataSet"><Query><DataSourceName>DataSource</DataSourceName><CommandText>WITH DailyDetails AS (SELECT        AttendanceDailyAdjustment.EmployeeAttendanceCard_id AS EmpAttCardID, AttendanceDailyAdjustmentDetail.HasVacation, AttendanceDailyAdjustmentDetail.HasMission, 
                                                                           AttendanceDailyAdjustmentDetail.IsWorkDay, AttendanceDailyAdjustmentDetail.ActualWorkHoursValue, AttendanceDailyAdjustmentDetail.HolidayOvertimeValue, 
                                                                           AttendanceDailyAdjustmentDetail.NormalOvertimeValue, AttendanceDailyAdjustment.FinalOvertimeValue, AttendanceDailyAdjustment.FinalNonAttendanceValue, MONTH(AttendanceRecord.Date) 
                                                                           AS monthnumber, AttendanceRecord.Id AS MonthId, 
                                                                           AttendanceDailyAdjustmentDetail.WorkHoursValue - AttendanceDailyAdjustmentDetail.VacationValue - AttendanceDailyAdjustmentDetail.MissionValue AS RequiredWorkHoursValue, 
                                                                           AttendanceDailyAdjustment.Penalty_id
                                                  FROM            AttendanceDailyAdjustment INNER JOIN
                                                                           AttendanceDailyAdjustmentDetail ON AttendanceDailyAdjustment.Id = AttendanceDailyAdjustmentDetail.AttendanceDailyAdjustment_id INNER JOIN
                                                                           AttendanceRecord ON AttendanceRecord.Id = AttendanceDailyAdjustment.AttendanceRecord_id
                                                  WHERE        (AttendanceDailyAdjustment.IsVertualDeleted = 0) AND (AttendanceDailyAdjustmentDetail.IsVertualDeleted = 0) AND (AttendanceRecord.IsVertualDeleted = 0))
    SELECT        ISNULL
                                  ((SELECT        COUNT(*) AS Expr1
                                      FROM            AttendanceDailyAdjustment AS AttendanceDailyAdjustment_2 INNER JOIN
                                                                AttendanceDailyAdjustmentDetail AS AttendanceDailyAdjustmentDetail_2 ON AttendanceDailyAdjustment_2.Id = AttendanceDailyAdjustmentDetail_2.AttendanceDailyAdjustment_id
                                      WHERE        (AttendanceDailyAdjustment_2.IsVertualDeleted = 0) AND (AttendanceDailyAdjustmentDetail_2.IsVertualDeleted = 0) AND (AttendanceDailyAdjustmentDetail_2.ActualWorkHoursValue = 0) AND 
                                                                (AttendanceDailyAdjustmentDetail_2.IsWorkDay = 1) AND (AttendanceDailyAdjustmentDetail_2.HasMission = 0) AND (AttendanceDailyAdjustmentDetail_2.HasVacation = 0) AND 
                                                                (AttendanceDailyAdjustment_2.EmployeeAttendanceCard_id = DailyDetails_1.EmpAttCardID) AND (MONTH(AttendanceDailyAdjustmentDetail_2.Date) = DailyDetails_1.monthnumber)), 0) AS CountAbsence,
                                  (SELECT        RIGHT('00' + CONVERT(varchar(8), FLOOR(ROUND
                                                                  ((SELECT        SUM(AttendanceDailyAdjustmentDetail_1.WorkHoursValue - AttendanceDailyAdjustmentDetail_1.VacationValue - AttendanceDailyAdjustmentDetail_1.MissionValue - AttendanceDailyAdjustmentDetail_1.ActualWorkHoursValue)
                                                                                                 AS Expr1
                                                                      FROM            AttendanceDailyAdjustment AS AttendanceDailyAdjustment_1 INNER JOIN
                                                                                                AttendanceDailyAdjustmentDetail AS AttendanceDailyAdjustmentDetail_1 ON AttendanceDailyAdjustment_1.Id = AttendanceDailyAdjustmentDetail_1.AttendanceDailyAdjustment_id
                                                                      WHERE        (AttendanceDailyAdjustment_1.IsVertualDeleted = 0) AND (AttendanceDailyAdjustmentDetail_1.IsVertualDeleted = 0) AND 
                                                                                                (AttendanceDailyAdjustmentDetail_1.WorkHoursValue - AttendanceDailyAdjustmentDetail_1.VacationValue - AttendanceDailyAdjustmentDetail_1.MissionValue - AttendanceDailyAdjustmentDetail_1.ActualWorkHoursValue
                                                                                                 &gt; 0) AND (AttendanceDailyAdjustment_1.EmployeeAttendanceCard_id = DailyDetails_1.EmpAttCardID) AND (MONTH(AttendanceDailyAdjustmentDetail_1.Date) = DailyDetails_1.monthnumber)), 
                                                              2))), CASE WHEN LEN(CONVERT(varchar(8), FLOOR(ROUND(CONVERT(varchar(8), FLOOR(ROUND
                                                                  ((SELECT        SUM(AttendanceDailyAdjustmentDetail_1.WorkHoursValue - AttendanceDailyAdjustmentDetail_1.VacationValue - AttendanceDailyAdjustmentDetail_1.MissionValue - AttendanceDailyAdjustmentDetail_1.ActualWorkHoursValue)
                                                                                                 AS Expr1
                                                                      FROM            AttendanceDailyAdjustment AS AttendanceDailyAdjustment_1 INNER JOIN
                                                                                                AttendanceDailyAdjustmentDetail AS AttendanceDailyAdjustmentDetail_1 ON AttendanceDailyAdjustment_1.Id = AttendanceDailyAdjustmentDetail_1.AttendanceDailyAdjustment_id
                                                                      WHERE        (AttendanceDailyAdjustment_1.IsVertualDeleted = 0) AND (AttendanceDailyAdjustmentDetail_1.IsVertualDeleted = 0) AND 
                                                                                                (AttendanceDailyAdjustmentDetail_1.WorkHoursValue - AttendanceDailyAdjustmentDetail_1.VacationValue - AttendanceDailyAdjustmentDetail_1.MissionValue - AttendanceDailyAdjustmentDetail_1.ActualWorkHoursValue
                                                                                                 &gt; 0) AND (AttendanceDailyAdjustment_1.EmployeeAttendanceCard_id = DailyDetails_1.EmpAttCardID) AND (MONTH(AttendanceDailyAdjustmentDetail_1.Date) = DailyDetails_1.monthnumber)), 
                                                              2))), 2)))) &gt; 2 THEN 3 ELSE 2 END) + ':' + RIGHT('00' + CONVERT(varchar(8),
                                                                  (SELECT        CONVERT(int, (ROUND
                                                                                                  ((SELECT        SUM(AttendanceDailyAdjustmentDetail_1.WorkHoursValue - AttendanceDailyAdjustmentDetail_1.VacationValue - AttendanceDailyAdjustmentDetail_1.MissionValue - AttendanceDailyAdjustmentDetail_1.ActualWorkHoursValue)
                                                                                                                                 AS Expr1
                                                                                                      FROM            AttendanceDailyAdjustment AS AttendanceDailyAdjustment_1 INNER JOIN
                                                                                                                                AttendanceDailyAdjustmentDetail AS AttendanceDailyAdjustmentDetail_1 ON 
                                                                                                                                AttendanceDailyAdjustment_1.Id = AttendanceDailyAdjustmentDetail_1.AttendanceDailyAdjustment_id
                                                                                                      WHERE        (AttendanceDailyAdjustment_1.IsVertualDeleted = 0) AND (AttendanceDailyAdjustmentDetail_1.IsVertualDeleted = 0) AND 
                                                                                                                                (AttendanceDailyAdjustmentDetail_1.WorkHoursValue - AttendanceDailyAdjustmentDetail_1.VacationValue - AttendanceDailyAdjustmentDetail_1.MissionValue - AttendanceDailyAdjustmentDetail_1.ActualWorkHoursValue
                                                                                                                                 &gt; 0) AND (AttendanceDailyAdjustment_1.EmployeeAttendanceCard_id = DailyDetails_1.EmpAttCardID) AND (MONTH(AttendanceDailyAdjustmentDetail_1.Date) 
                                                                                                                                = DailyDetails_1.monthnumber)), 2) - FLOOR(ROUND
                                                                                                  ((SELECT        SUM(AttendanceDailyAdjustmentDetail_1.WorkHoursValue - AttendanceDailyAdjustmentDetail_1.VacationValue - AttendanceDailyAdjustmentDetail_1.MissionValue - AttendanceDailyAdjustmentDetail_1.ActualWorkHoursValue)
                                                                                                                                 AS Expr1
                                                                                                      FROM            AttendanceDailyAdjustment AS AttendanceDailyAdjustment_1 INNER JOIN
                                                                                                                                AttendanceDailyAdjustmentDetail AS AttendanceDailyAdjustmentDetail_1 ON 
                                                                                                                                AttendanceDailyAdjustment_1.Id = AttendanceDailyAdjustmentDetail_1.AttendanceDailyAdjustment_id
                                                                                                      WHERE        (AttendanceDailyAdjustment_1.IsVertualDeleted = 0) AND (AttendanceDailyAdjustmentDetail_1.IsVertualDeleted = 0) AND 
                                                                                                                                (AttendanceDailyAdjustmentDetail_1.WorkHoursValue - AttendanceDailyAdjustmentDetail_1.VacationValue - AttendanceDailyAdjustmentDetail_1.MissionValue - AttendanceDailyAdjustmentDetail_1.ActualWorkHoursValue
                                                                                                                                 &gt; 0) AND (AttendanceDailyAdjustment_1.EmployeeAttendanceCard_id = DailyDetails_1.EmpAttCardID) AND (MONTH(AttendanceDailyAdjustmentDetail_1.Date) 
                                                                                                                                = DailyDetails_1.monthnumber)), 2))) * 60, 0) AS Expr1)), 2) AS Expr1) AS InitialNonAttendanceValue, MonthId, EmpAttCardID,
                                  (SELECT        Employee.FirstName + ' ' + Employee.FatherName + ' ' + Employee.LastName AS FullName
                                     FROM            EmployeeCard INNER JOIN
                                                              Employee ON EmployeeCard.Employee_id = Employee.Id
                                     WHERE        (EmployeeCard.IsVertualDeleted = 0) AND (Employee.IsVertualDeleted = 0) AND (EmployeeCard.Id = DailyDetails_1.EmpAttCardID)) AS FullName, monthnumber,
                                  (SELECT        RIGHT('00' + CONVERT(varchar(8), FLOOR(ROUND(DailyDetails_1.FinalOvertimeValue, 2))), CASE WHEN LEN(CONVERT(varchar(8), FLOOR(ROUND(DailyDetails_1.FinalOvertimeValue, 2)))) 
                                                              &gt; 2 THEN 3 ELSE 2 END) + ':' + RIGHT('00' + CONVERT(varchar(8),
                                                                  (SELECT        CONVERT(int, (ROUND(DailyDetails_1.FinalOvertimeValue, 2) - FLOOR(ROUND(DailyDetails_1.FinalOvertimeValue, 2))) * 60, 0) AS Expr1)), 2) AS Expr1) AS FinalOvertimeValue,
                                  (SELECT        RIGHT('00' + CONVERT(varchar(8), FLOOR(ROUND(DailyDetails_1.FinalNonAttendanceValue, 2))), CASE WHEN LEN(CONVERT(varchar(8), FLOOR(ROUND(DailyDetails_1.FinalNonAttendanceValue, 2)))) 
                                                              &gt; 2 THEN 3 ELSE 2 END) + ':' + RIGHT('00' + CONVERT(varchar(8),
                                                                  (SELECT        CONVERT(int, (ROUND(DailyDetails_1.FinalNonAttendanceValue, 2) - FLOOR(ROUND(DailyDetails_1.FinalNonAttendanceValue, 2))) * 60, 0) AS Expr1)), 2) AS Expr1) AS FinalNonAttendanceValue,
                               Penalty_id,
                                  (SELECT        Name
                                     FROM            DisciplinarySetting AS N
                                     WHERE        (Id = DailyDetails_1.Penalty_id)) AS Penalty,
                                  (SELECT        RIGHT('00' + CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(DailyDetails_1.HolidayOvertimeValue), 0), 2))), CASE WHEN LEN(CONVERT(varchar(8), 
                                                              FLOOR(ROUND(ISNULL(SUM(DailyDetails_1.HolidayOvertimeValue), 0), 2)))) &gt; 2 THEN 3 ELSE 2 END) + ':' + RIGHT('00' + CONVERT(varchar(8),
                                                                  (SELECT        CONVERT(int, (ROUND(ISNULL(SUM(DailyDetails_1.HolidayOvertimeValue), 0), 2) - FLOOR(ROUND(ISNULL(SUM(DailyDetails_1.HolidayOvertimeValue), 0), 2))) * 60, 0) AS Expr1)), 2) AS Expr1) 
                              AS TotalHolidayOvertimeValue,
                                  (SELECT        RIGHT('00' + CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(DailyDetails_1.NormalOvertimeValue), 0), 2))), CASE WHEN LEN(CONVERT(varchar(8), 
                                                              FLOOR(ROUND(ISNULL(SUM(DailyDetails_1.NormalOvertimeValue), 0), 2)))) &gt; 2 THEN 3 ELSE 2 END) + ':' + RIGHT('00' + CONVERT(varchar(8),
                                                                  (SELECT        CONVERT(int, (ROUND(ISNULL(SUM(DailyDetails_1.NormalOvertimeValue), 0), 2) - FLOOR(ROUND(ISNULL(SUM(DailyDetails_1.NormalOvertimeValue), 0), 2))) * 60, 0) AS Expr1)), 2) AS Expr1) 
                              AS TotalNormalOvertimeValue
     FROM            DailyDetails AS DailyDetails_1
     GROUP BY monthnumber, EmpAttCardID, MonthId, Penalty_id, FinalOvertimeValue, FinalNonAttendanceValue</CommandText></Query><Fields><Field Name="CountAbsence"><DataField>CountAbsence</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="InitialNonAttendanceValue"><DataField>InitialNonAttendanceValue</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="FullName"><DataField>FullName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="monthnumber"><DataField>monthnumber</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="EmpAttCardID"><DataField>EmpAttCardID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="MonthId"><DataField>MonthId</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="FinalOvertimeValue"><DataField>FinalOvertimeValue</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="FinalNonAttendanceValue"><DataField>FinalNonAttendanceValue</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Penalty_id"><DataField>Penalty_id</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="Penalty"><DataField>Penalty</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="TotalHolidayOvertimeValue"><DataField>TotalHolidayOvertimeValue</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="TotalNormalOvertimeValue"><DataField>TotalNormalOvertimeValue</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields><Filters><Filter><FilterExpression>=Fields!MonthId.Value</FilterExpression><Operator>In</Operator><FilterValues><FilterValue>=Parameters!EmployeeRelationServices0FixedHoliday0Month.Value</FilterValue></FilterValues></Filter></Filters></DataSet><DataSet Name="MonthNameDataSet"><Query><DataSourceName>DataSource</DataSourceName><CommandText>SELECT DISTINCT Id, Name AS MonthName
