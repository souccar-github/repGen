lixColumnHierarchy><TablixRowHierarchy><TablixMembers><TablixMember><Group Name="Details3" /></TablixMember></TablixMembers></TablixRowHierarchy><Top>0.49875in</Top><Left>4.01042in</Left><Height>0.42125in</Height><Width>3.99875in</Width><ZIndex>3</ZIndex><Style><Border><Style>None</Style></Border></Style></Tablix></ReportItems><KeepTogether>true</KeepTogether><Style><Border><Style>None</Style></Border></Style></Rectangle></CellContents></TablixCell></TablixCells></TablixRow></TablixRows></TablixBody><TablixColumnHierarchy><TablixMembers><TablixMember /></TablixMembers></TablixColumnHierarchy><TablixRowHierarchy><TablixMembers><TablixMember><Group Name="Details"><GroupExpressions><GroupExpression>=Fields!Name.Value</GroupExpression></GroupExpressions></Group></TablixMember></TablixMembers></TablixRowHierarchy><KeepTogether>true</KeepTogether><DataSetName>DataSet1</DataSetName><Height>1.51723in</Height><Width>8.00917in</Width><Visibility><Hidden>=CountRows("DataSet1")&lt;1</Hidden></Visibility><Style><Border><Style>None</Style></Border></Style></Tablix></ReportItems><Height>1.51723in</Height><Style /></Body><Width>8.00917in</Width><Page><LeftMargin>1in</LeftMargin><RightMargin>1in</RightMargin><TopMargin>1in</TopMargin><BottomMargin>1in</BottomMargin><Style /></Page><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="SharedDataSource"><DataSourceReference>SharedDataSource</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>ce25d2a3-1e3e-4ec4-a18f-4c6c52c5155b</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="DataSet1"><Query><DataSourceName>SharedDataSource</DataSourceName><QueryParameters><QueryParameter Name="@position_id"><Value>=Parameters!position_id.Value</Value></QueryParameter><QueryParameter Name="@Phase_id"><Value>=Parameters!Phase_id.Value</Value></QueryParameter></QueryParameters><CommandText>IF NOT EXISTS(select section.Name from TemplateAppraisalPositions templatePostions left join Position postion on templatePostions.Position_id=postion.Id
left join AppraisalTemplate template on template.Id=templatePostions.AppraisalTemplate_id
left join TemplateSectionWeight temSection on temSection.AppraisalTemplate_id=template.Id
left join AppraisalSection section on section.Id=temSection.AppraisalSection_id
where postion.Id=@position_id   and
postion.IsVertualDeleted=0  and temSection.IsVertualDeleted=0 and template.IsVertualDeleted=0 and section.IsVertualDeleted=0 )
select section.Name , section.Id,section.Name+' Section:' as expName,temSection.Weight weight,
(select SUM((CustomSectionItem.Weight * CustomSectionItem.Rate)/100) from AppraisalPhase phase 

left join AppraisalPhaseWorkflow phaseWorkflow   on phase.Id=phaseWorkflow.Phase_Id
left join Appraisal app  on phaseWorkflow.Id=app.PhaseWorkflow_id
left join AppraisalCustomSection customsection on app.Id=customsection.PhaseAppraisal_id
left join Position pos on pos.Id=phaseWorkflow.Position_id
left join AppraisalCustomSectionItem CustomSectionItem on CustomSectionItem.Section_Id=customsection.Id
where customsection.Section_id=section.Id and pos.Id=@position_id and phase.Id=@Phase_id and phaseWorkflow.IsVertualDeleted=0 and phase.IsVertualDeleted=0 and
 pos.IsVertualDeleted=0  and customsection.IsVertualDeleted=0) sectionValue,(SELECT  COUNT(*)  
                               
FROM            AppraisalPhaseWorkflow AS phaseWorkflow LEFT OUTER JOIN
                         AppraisalPhase AS phase ON phase.Id = phaseWorkflow.Phase_Id LEFT OUTER JOIN
                         Position AS pos ON pos.Id = phaseWorkflow.Position_id LEFT OUTER JOIN
                         JobDescription AS job ON job.Id = pos.JobDescription_id LEFT OUTER JOIN
                         WorkflowItem AS _Workflow ON _Workflow.Id = phaseWorkflow.WorkflowItem_id left outer join
						 WorkflowStep AS step on step.Workflow_id=_Workflow.Id
WHERE        (phaseWorkflow.IsVertualDeleted = 0) AND (phase.IsVertualDeleted = 0) AND (pos.IsVertualDeleted = 0) AND (job.IsVertualDeleted = 0) AND 
                         (_Workflow.IsVertualDeleted = 0) and step.Status=0 and phase.Id=@Phase_id and pos.Id=@position_id) AS steps
 from AppraisalSection section left join TemplateSectionWeight temSection on section.Id=temSection.AppraisalSection_id
left join AppraisalTemplate template on temSection.AppraisalTemplate_id=template.Id
left join AppraisalTemplateSetting templateSetting on template.Id=templateSetting.DefaultTemplate_id
where section.IsVertualDeleted=0 and template.IsVertualDeleted=0 and templateSetting.IsVertualDeleted=0 and temSection.IsVertualDeleted=0 
ELSE
select section.Name ,section.Id,section.Name+' Section:' as expName ,temSection.Weight weight,
(select SUM((CustomSectionItem.Weight * CustomSectionItem.Rate)/100) from AppraisalPhase phase 

left join AppraisalPhaseWorkflow phaseWorkflow   on phase.Id=phaseWorkflow.Phase_Id
left join Appraisal app  on phaseWorkflow.Id=app.PhaseWorkflow_id
left join AppraisalCustomSection customsection on app.Id=customsection.PhaseAppraisal_id
left join Position pos on pos.Id=phaseWorkflow.Position_id
left join AppraisalCustomSectionItem CustomSectionItem on CustomSectionItem.Section_Id=customsection.Id
where customsection.Section_id=section.Id and pos.Id=@position_id and phase.Id=@Phase_id and phaseWorkflow.IsVertualDeleted=0 and phase.IsVertualDeleted=0 and
 pos.IsVertualDeleted=0  and customsection.IsVertualDeleted=0) sectionValue,(SELECT  COUNT(*)  
                               
FROM            AppraisalPhaseWorkflow AS phaseWorkflow LEFT OUTER JOIN
                         AppraisalPhase AS phase ON phase.Id = phaseWorkflow.Phase_Id LEFT OUTER JOIN
                         Position AS pos ON pos.Id = phaseWorkflow.Position_id LEFT OUTER JOIN
                         JobDescription AS job ON job.Id = pos.JobDescription_id LEFT OUTER JOIN
                         WorkflowItem AS _Workflow ON _Workflow.Id = phaseWorkflow.WorkflowItem_id left outer join
						 WorkflowStep AS step on step.Workflow_id=_Workflow.Id
WHERE        (phaseWorkflow.IsVertualDeleted = 0) AND (phase.IsVertualDeleted = 0) AND (pos.IsVertualDeleted = 0) AND (job.IsVertualDeleted = 0) AND 
                         (_Workflow.IsVertualDeleted = 0) and step.Status=0 and phase.Id=@Phase_id and pos.Id=@position_id) AS steps from TemplateAppraisalPositions templatePostions left join Position postion on templatePostions.Position_id=postion.Id
left join AppraisalTemplate template on template.Id=templatePostions.AppraisalTemplate_id
left join TemplateSectionWeight temSection on temSection.AppraisalTemplate_id=template.Id
left join AppraisalSection section on section.Id=temSection.AppraisalSection_id

where postion.Id=@position_id and
postion.IsVertualDeleted=0  and temSection.IsVertualDeleted=0 and template.IsVertualDeleted=0 and section.IsVertualDeleted=0  and templatePostions.IsVertualDeleted=0</CommandText><rd:UseGenericDesigner>true</rd:UseGenericDesigner></Query><Fields><Field Name="Name"><DataField>Name</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Id"><DataField>Id</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="expName"><DataField>expName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="weight"><DataField>weight</DataField><rd:TypeName>System.Single</rd:TypeName></Field><Field Name="sectionValue"><DataField>sectionValue</DataField><rd:TypeName>System.Double</rd:TypeName></Field><Field Name="steps"><DataField>steps</DataField><rd:TypeName>System.Int32</rd:TypeName></Field></Fields></DataSet></DataSets><ReportParameters><ReportParameter Name="position_id"><DataType>Integer</DataType><Prompt>position_id</Prompt></ReportParameter><ReportParameter Name="Phase_id"><DataType>Integer</DataType><Prompt>Phase_id</Prompt></ReportParameter></ReportParameters><Language>en-US</Language><ConsumeContainerWhitespace>true</ConsumeContainerWhitespace><rd:ReportUnitType>Inch</rd:ReportUnitType><rd:ReportID>37f402ba-94b7-468e-bea0-0ee58f6d492c</rd:ReportID></Report>