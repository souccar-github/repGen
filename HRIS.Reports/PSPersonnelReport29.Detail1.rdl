  <DataSets>
    <DataSet Name="BasicInfoDataSet">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <CommandText>SELECT        
Employee_id AS EmpId, 
CompanyName, 
StartDate, 
EndDate,
(SELECT        Name AS IndustryName
FROM            Industry
WHERE        (Id = E.Industry_id) AND (IsVertualDeleted = 0)) AS IndustryName,
(SELECT        Name AS IndustryName
FROM            GlobalJobTitle
WHERE        (Id = E.JobTitle_id) AND (IsVertualDeleted = 0)) AS GlobalJobTitleName, 
--DATEDIFF(YEAR, StartDate, EndDate) AS PerDate,
ROUND(CAST(DATEDIFF(MONTH, StartDate, EndDate)AS FLOAT) /12 ,2) AS PerDate
FROM            Experience AS E
WHERE        (IsVertualDeleted = 0)</CommandText>
      </Query>
      <Fields>
        <Field Name="EmpId">
          <DataField>EmpId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="CompanyName">
          <DataField>CompanyName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="StartDate">
          <DataField>StartDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="EndDate">
          <DataField>EndDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="IndustryName">
          <DataField>IndustryName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="GlobalJobTitleName">
          <DataField>GlobalJobTitleName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PerDate">
          <DataField>PerDate</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
