  <DataSets>
    <DataSet Name="BasicInfoDataSet">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <CommandText>WITH temp AS (SELECT        EmployeeCard_Id AS EmployeeCardId,
                                                               (SELECT        Employee_id
                                                                  FROM            EmployeeCard AS EC
                                                                  WHERE        (Id = EmployeePromotion.EmployeeCard_Id) AND (IsVertualDeleted = 0)) AS EmployeeId,
                                                               (SELECT        Employee.FirstName + ' ' + Employee.FatherName + ' ' + Employee.LastName AS FullName
                                                                  FROM            Employee INNER JOIN
                                                                                           EmployeeCard ON Employee.Id = EmployeeCard.Employee_id
                                                                  WHERE        (EmployeeCard.Id = EmployeePromotion.EmployeeCard_Id) AND (Employee.IsVertualDeleted = 0)) AS FullName,
                                                               (SELECT        Code
                                                                  FROM            Position AS P
                                                                  WHERE        (IsVertualDeleted = 0) AND (Id = EmployeePromotion.Position_id)) AS PositionCode,
                                                               (SELECT        J.Name
                                                                  FROM            Position AS P INNER JOIN
                                                                                           JobDescription AS J ON P.JobDescription_id = J.Id
                                                                  WHERE        (P.IsVertualDeleted = 0) AND (J.IsVertualDeleted = 0) AND (P.Id = EmployeePromotion.Position_id)) AS JobDescriptionName,
                                                               (SELECT        JT.Name
                                                                  FROM            Position AS P INNER JOIN
                                                                                           JobDescription AS JD ON P.JobDescription_id = JD.Id INNER JOIN
                                                                                           JobTitle AS JT ON JD.JobTitle_id = JT.Id
                                                                  WHERE        (P.IsVertualDeleted = 0) AND (JD.IsVertualDeleted = 0) AND (JT.IsVertualDeleted = 0) AND (P.Id = EmployeePromotion.Position_id)) AS JobTitleName,
                                                               (SELECT        G.Name
                                                                  FROM            Position AS P INNER JOIN
                                                                                           JobDescription AS JD ON P.JobDescription_id = JD.Id INNER JOIN
                                                                                           JobTitle AS JT ON JD.JobTitle_id = JT.Id INNER JOIN
                                                                                           Grade AS G ON JT.Grade_id = G.Id
                                                                  WHERE        (P.IsVertualDeleted = 0) AND (JD.IsVertualDeleted = 0) AND (JT.IsVertualDeleted = 0) AND (G.IsVertualDeleted = 0) AND (P.Id = EmployeePromotion.Position_id)) AS GradeName, 
                                                           PositionJoiningDate AS StartingDate, PromotionReason AS Reason, Comment, CreationDate
                                  FROM            EmployeePromotion
                                  WHERE        (IsVertualDeleted = 0)
                                  UNION
                                  SELECT        EmployeeCard_Id AS EmployeeCardId,
                                                               (SELECT        Employee_id
                                                                  FROM            EmployeeCard AS EC
                                                                  WHERE        (Id = EmployeeTransfer.EmployeeCard_Id) AND (IsVertualDeleted = 0)) AS EmployeeId,
                                                               (SELECT        Employee_1.FirstName + ' ' + Employee_1.FatherName + ' ' + Employee_1.LastName AS FullName
                                                                  FROM            Employee AS Employee_1 INNER JOIN
                                                                                           EmployeeCard AS EmployeeCard_1 ON Employee_1.Id = EmployeeCard_1.Employee_id
                                                                  WHERE        (EmployeeCard_1.Id = EmployeeTransfer.EmployeeCard_Id) AND (Employee_1.IsVertualDeleted = 0)) AS FullName,
                                                               (SELECT        Code
                                                                  FROM            Position AS P
                                                                  WHERE        (IsVertualDeleted = 0) AND (Id = EmployeeTransfer.DestinationPosition_id)) AS PositionCode,
                                                               (SELECT        J.Name
                                                                  FROM            Position AS P INNER JOIN
                                                                                           JobDescription AS J ON P.JobDescription_id = J.Id
                                                                  WHERE        (P.IsVertualDeleted = 0) AND (J.IsVertualDeleted = 0) AND (P.Id = EmployeeTransfer.DestinationPosition_id)) AS JobDescriptionName,
                                                               (SELECT        JT.Name
                                                                  FROM            Position AS P INNER JOIN
                                                                                           JobDescription AS JD ON P.JobDescription_id = JD.Id INNER JOIN
                                                                                           JobTitle AS JT ON JD.JobTitle_id = JT.Id
                                                                  WHERE        (P.IsVertualDeleted = 0) AND (JD.IsVertualDeleted = 0) AND (JT.IsVertualDeleted = 0) AND (P.Id = EmployeeTransfer.DestinationPosition_id)) AS JobTitleName,
                                                               (SELECT        G.Name
                                                                  FROM            Position AS P INNER JOIN
                                                                                           JobDescription AS JD ON P.JobDescription_id = JD.Id INNER JOIN
                                                                                           JobTitle AS JT ON JD.JobTitle_id = JT.Id INNER JOIN
                                                                                           Grade AS G ON JT.Grade_id = G.Id
                                                                  WHERE        (P.IsVertualDeleted = 0) AND (JD.IsVertualDeleted = 0) AND (JT.IsVertualDeleted = 0) AND (G.IsVertualDeleted = 0) AND (P.Id = EmployeeTransfer.DestinationPosition_id)) AS GradeName, 
                                                           StartingDate, TransferReason AS Reason, Comment, CreationDate
                                  FROM            EmployeeTransfer
                                  WHERE        (IsVertualDeleted = 0))
    SELECT        EmployeeCardId, EmployeeId, FullName, PositionCode, JobDescriptionName, JobTitleName, GradeName, StartingDate, Reason, Comment, CreationDate
     FROM            temp AS temp_1
     ORDER BY StartingDate, CreationDate</CommandText>
      </Query>
