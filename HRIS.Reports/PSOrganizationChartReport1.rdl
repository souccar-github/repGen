    <DataSet Name="BasicInfoDataSet">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <CommandText>SELECT
(SELECT JobDescription.Name FROM dbo.JobDescription 
WHERE JobDescription.Id=JobDescription_id AND JobDescription.IsVertualDeleted=0) AS JobDescriptionName,
(SELECT dbo.JobTitle.Name
FROM dbo.JobDescription INNER JOIN dbo.JobTitle ON dbo.JobDescription.JobTitle_id = dbo.JobTitle.Id
WHERE JobDescription.Id=JobDescription_id AND JobTitle.IsVertualDeleted=0 AND JobDescription.IsVertualDeleted=0) AS JobTitleName,
(SELECT dbo.Node.Name FROM dbo.JobDescription INNER JOIN dbo.Node ON dbo.JobDescription.Node_id = dbo.Node.Id 
WHERE dbo.JobDescription.Id=JobDescription_id AND JobDescription.IsVertualDeleted=0 AND Node.IsVertualDeleted=0) AS NodeName,
ISNULL((SELECT dbo.JobDescription.Node_id FROM dbo.JobDescription
WHERE dbo.JobDescription.Id=JobDescription_id AND JobDescription.IsVertualDeleted=0),0) AS NodeId,
Manager_id AS ManagerId,
Position.Id AS PositionId,
ISNULL((SELECT top(1) FirstName+' '+ISNULL(FatherName,'')+' '+LastName 
FROM dbo.Employee 
INNER JOIN AssigningEmployeeToPosition ON dbo.Employee.Id=AssigningEmployeeToPosition.Employee_id 
INNER JOIN dbo.Position P ON P.Id=AssigningEmployeeToPosition.Position_Id 
WHERE 
P.Id=dbo.Position.Id AND
P.AssigningEmployeeToPosition_id IS NOT NULL AND 
Employee.IsVertualDeleted=0 AND 
P.IsVertualDeleted=0 AND
AssigningEmployeeToPosition.IsVertualDeleted=0),'') AS EmployeeName,
  Code 
FROM            
dbo.Position 
WHERE
Position.IsVertualDeleted=0
ORDER BY JobDescription_id</CommandText>
      </Query>
