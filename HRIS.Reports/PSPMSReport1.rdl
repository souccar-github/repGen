    <DataSet Name="BasicInfoDataSet">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <CommandText>SELECT        
dbo.Node.Id AS NodeId, 
(SELECT N.Name FROM Node N WHERE  N.Id=dbo.Node.Id) NodeName,
dbo.Employee.Id AS EmployeeId, 
dbo.Employee.FirstName, 
dbo.Employee.LastName, 
dbo.Employee.FatherName, 
dbo.Employee.FirstName +' '+ dbo.Employee.FatherName +' '+ dbo.Employee.LastName FullName,
dbo.AppraisalPhase.Id AppraisalPhaseId, 
dbo.AppraisalPhase.CreationDate AS CreationDate, 
dbo.AppraisalPhase.StartDate AS StartDate, 
dbo.AppraisalPhase.EndDate AS EndDate,
dbo.AppraisalPhaseWorkflow.FinalMarkAfterApproval,
dbo.AppraisalPhaseWorkflow.Id AS AppraisalPhaseWorkflowId,
ROUND((SELECT SUM(dbo.Appraisal.AppraisalValue) FROM Appraisal WHERE  dbo.Appraisal.PhaseWorkflow_id=dbo.AppraisalPhaseWorkflow.Id),0) AS FinalMark
FROM            
dbo.JobDescription 
INNER JOIN dbo.Position ON dbo.JobDescription.Id = dbo.Position.JobDescription_id 
INNER JOIN dbo.Node ON dbo.JobDescription.Node_id = dbo.Node.Id 
INNER JOIN dbo.AssigningEmployeeToPosition ON dbo.AssigningEmployeeToPosition.Position_Id = dbo.Position.Id
--dbo.AssigningEmployeeToPosition ON dbo.Position.AssigningEmployeeToPosition_id = dbo.AssigningEmployeeToPosition.Id 
--AND dbo.Position.Id = dbo.AssigningEmployeeToPosition.Position_Id 
INNER JOIN dbo.Employee ON dbo.Employee.Id = dbo.AssigningEmployeeToPosition.Employee_id
INNER JOIN dbo.AppraisalPhaseWorkflow ON dbo.Position.Id = dbo.AppraisalPhaseWorkflow.Position_id 
INNER JOIN dbo.AppraisalPhase ON dbo.AppraisalPhaseWorkflow.Phase_Id = dbo.AppraisalPhase.Id
WHERE 
((SELECT SUM(dbo.Appraisal.AppraisalValue) FROM Appraisal WHERE  dbo.Appraisal.PhaseWorkflow_id=dbo.AppraisalPhaseWorkflow.Id))&lt;&gt;0 AND
dbo.JobDescription.IsVertualDeleted = 0 AND  
dbo.Position.IsVertualDeleted = 0 AND  
dbo.Node.IsVertualDeleted = 0 AND  
dbo.AssigningEmployeeToPosition.IsVertualDeleted = 0 AND  
dbo.Employee.IsVertualDeleted = 0 AND  
dbo.AppraisalPhaseWorkflow.IsVertualDeleted = 0 AND  
dbo.AppraisalPhase.IsVertualDeleted = 0 </CommandText>
      </Query>
