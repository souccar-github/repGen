    <DataSet Name="AppraisalPhaseDataSet">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <CommandText>  SELECT 
  A.IsVertualDeleted,
A.Id, A.CreationDate, 
A.StartDate, 
A.EndDate, 
CAST(CONVERT(VARCHAR(11),A.StartDate,106) AS nvarchar) +' - '+CAST(CONVERT(VARCHAR(11),A.EndDate,106) AS nvarchar) AS PhaseTitle,
A.Description, 
A.Period, 
A.Month, 
A.Quarter, 
A.SemiAnnual, 
A.Year, 
A.TemplateSetting_id, 
A.AppraisalPhaseSetting_id
FROM "AppraisalPhase" A
where A.IsVertualDeleted = 'False'</CommandText>
      </Query>
