    <DataSet Name="EmployeeDataSet">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <CommandText>WITH Temp AS(
SELECT 
dbo.Employee.Id,
dbo.Employee.FirstName+' '+dbo.Employee.FatherName+' '+ dbo.Employee.LastName  AS FullName,
(SELECT top(1) LeaveTemplateMaster_id FROM EmployeeCard WHERE EmployeeCard.IsVertualDeleted=0 AND EmployeeCard.Employee_id = dbo.Employee.Id) AS EmployeeCardLeaveTemplateMaster,
(SELECT top(1) dbo.Grade.LeaveTemplateMaster_id--, dbo.AssigningEmployeeToPosition.Employee_id
FROM dbo.Grade 
INNER JOIN dbo.JobTitle ON dbo.Grade.Id = dbo.JobTitle.Grade_id 
INNER JOIN dbo.JobDescription ON dbo.JobTitle.Id = dbo.JobDescription.JobTitle_id 
INNER JOIN dbo.Position ON dbo.JobDescription.Id = dbo.Position.JobDescription_id 
INNER JOIN dbo.AssigningEmployeeToPosition ON dbo.Position.Id = dbo.AssigningEmployeeToPosition.Position_Id
WHERE dbo.AssigningEmployeeToPosition.Employee_id = dbo.Employee.Id AND
Grade.IsVertualDeleted = 0 AND
JobTitle.IsVertualDeleted = 0 AND
JobDescription.IsVertualDeleted = 0 AND
Position.IsVertualDeleted = 0 AND
AssigningEmployeeToPosition.IsVertualDeleted = 0
) AS GradeLeaveTemplateMaster
FROM Employee
)
SELECT * 
FROM	Temp
WHERE	EmployeeCardLeaveTemplateMaster IS NOT NULL OR
		GradeLeaveTemplateMaster IS NOT NULL
ORDER BY FullName</CommandText>
      </Query>
      <Fields>
        <Field Name="Id">
          <DataField>Id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="FullName">
          <DataField>FullName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="EmployeeCardLeaveTemplateMaster">
          <DataField>EmployeeCardLeaveTemplateMaster</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="GradeLeaveTemplateMaster">
          <DataField>GradeLeaveTemplateMaster</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=IIF(InStr(LCase(Fields!FullName.Value) , LCase(Parameters!SearchFullName.Value)),TRUE,FALSE)</FilterExpression>
          <Operator>Equal</Operator>
          <FilterValues>
            <FilterValue DataType="Boolean">true</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
    <DataSet Name="YearDataSet">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <CommandText>SELECT        YEAR(StartDate) AS year
FROM            LeaveRequest
WHERE        (IsVertualDeleted = 0)
UNION
SELECT        YEAR(EndDate) AS year
FROM            LeaveRequest AS LeaveRequest_1
WHERE        (IsVertualDeleted = 0)
ORDER BY year DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="year">
          <DataField>year</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
