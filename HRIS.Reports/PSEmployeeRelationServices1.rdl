  <DataSets>
    <DataSet Name="BasicInfoDataSet">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Year_">
            <Value>=Parameters!LabelYear.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @DateNow DATETIME, @StartOfYear DATETIME, @EndOfYear DATETIME;
SET                @DateNow = GETDATE();
SET                @StartOfYear = CAST(DATEADD(yy, DATEDIFF(yy, 0, getdate()), 0) AS date);
SET                @EndOfYear = CAST(DATEADD(yy, DATEDIFF(yy, 0, getdate()) + 1, - 1) AS date);
                             SELECT        dbo.Employee.FirstName + ' ' + dbo.Employee.FatherName + ' ' + dbo.Employee.LastName AS FullName, dbo.Employee.Id AS EmployeeId, dbo.EmployeeCard.Id AS EmployeeCardId, 
                                                       dbo.EmployeeCard.StartWorkingDate, dbo.LeaveSetting.Name AS LeaveName, dbo.LeaveType.Name AS LeaveTypeName, dbo.LeaveType.Id AS LeaveTypeId, 
                                                       (@Year_ - YEAR(dbo.EmployeeCard.StartWorkingDate) + 1) AS YearDiff, (((CAST(CASE WHEN Year(@DateNow) &gt; @Year_ THEN 12 ELSE Month(@DateNow) 
                                                       + 1 END - Month(dbo.EmployeeCard.StartWorkingDate) AS float) / 12) - CAST((@Year_ - YEAR(dbo.EmployeeCard.StartWorkingDate) + 1) AS int)) * 12) AS MonthDiff, 
                                                       /*----------------------------------------------LeaveBalance--------------------------------------------------------------*/ CASE WHEN ((@Year_ - YEAR(dbo.EmployeeCard.StartWorkingDate)))
                                                        &gt;= 1 THEN (ISNULL
                                                           ((SELECT        N .Balance
                                                               FROM            dbo.BalanceSlice N
                                                               WHERE        N .IsVertualDeleted = 0 AND N .LeaveSetting_id = dbo.LeaveSetting.Id AND (CASE WHEN (Month(@DateNow) - Month(dbo.EmployeeCard.StartWorkingDate)) &gt; 0 OR
                                                                                         Year(@DateNow) != @Year_ THEN @Year_ ELSE @Year_ - 1 END - YEAR(dbo.EmployeeCard.StartWorkingDate)) &gt;= N .FromYearOfServices AND (CASE WHEN (Month(@DateNow) 
                                                                                         - Month(dbo.EmployeeCard.StartWorkingDate)) &gt; 0 OR
                                                                                         Year(@DateNow) != @Year_ THEN @Year_ ELSE @Year_ - 1 END - YEAR(dbo.EmployeeCard.StartWorkingDate)) &lt; N .ToYearOfServices), (dbo.LeaveSetting.Balance))) 
                                                       WHEN (@Year_ - YEAR(dbo.EmployeeCard.StartWorkingDate)) = 0 AND dbo.LeaveSetting.IsAffectedByAssigningDate = 1 THEN (round(ISNULL((cast
                                                           ((SELECT        TOP 1 N .Balance
                                                               FROM            dbo.BalanceSlice N
                                                               WHERE        N .IsVertualDeleted = 0 AND N .LeaveSetting_id = dbo.LeaveSetting.Id AND (@Year_ - YEAR(dbo.EmployeeCard.StartWorkingDate)) &gt;= N .FromYearOfServices AND 
                                                                                         (@Year_ - YEAR(dbo.EmployeeCard.StartWorkingDate)) &lt; N .ToYearOfServices) AS float) / cast(12 AS float)) * (CAST((12 - Month(dbo.EmployeeCard.StartWorkingDate) + 1) AS float)), 
                                                       ((cast(dbo.LeaveSetting.Balance AS float) / cast(12 AS float)) * (CAST((12 - Month(dbo.EmployeeCard.StartWorkingDate) + 1) AS float)))), 2)) ELSE ISNULL
                                                           ((SELECT        TOP 1 N .Balance
                                                               FROM            dbo.BalanceSlice N
                                                               WHERE        N .IsVertualDeleted = 0 AND N .LeaveSetting_id = dbo.LeaveSetting.Id AND (CASE WHEN (Month(@DateNow) - Month(dbo.EmployeeCard.StartWorkingDate)) &gt; 0 OR
                                                                                         Year(@DateNow) != @Year_ THEN @Year_ ELSE @Year_ - 1 END - YEAR(dbo.EmployeeCard.StartWorkingDate)) &gt;= N .FromYearOfServices AND (CASE WHEN (Month(@DateNow) 
                                                                                         - Month(dbo.EmployeeCard.StartWorkingDate)) &gt; 0 OR
                                                                                         Year(@DateNow) != @Year_ THEN @Year_ ELSE @Year_ - 1 END - YEAR(dbo.EmployeeCard.StartWorkingDate)) &lt; N .ToYearOfServices), dbo.LeaveSetting.Balance) END AS LeaveBalance, 
                                                       ROUND(ISNULL
                                                           ((SELECT        ISNULL(SUM(M.RoundedBalance), 0)
                                                               FROM            dbo.RecycledLeave M
                                                               WHERE        M.IsVertualDeleted = 0 AND M.EmployeeCard_id = dbo.EmployeeCard.Id AND M.leaveSetting_Id = dbo.LeaveSetting.Id AND M.RecycleType = 0 AND M.Year = (@Year_ - 1)), 0), 2) 
                                                       AS RoundedBalance, /*----------------------------------------------GrantedBalance--------------------------------------------------------------*/ ROUND(isnull
                                                           ((SELECT        SUM(N .SpentDays)
                                                               FROM            dbo.LeaveRequest N
                                                               WHERE        (YEAR(N .StartDate) = @Year_ OR
                                                                                         YEAR(N .EndDate) = @Year_) AND (N .LeaveStatus = 0 OR
                                                                                         N .LeaveStatus = 2) AND N .IsVertualDeleted = 0 AND N .EmployeeCard_id = dbo.EmployeeCard.Id AND N .LeaveSetting_id = dbo.LeaveSetting.Id), 0), 2) AS GrantedBalance, 
                                                       /*----------------------------------------------RemainingBalance--------------------------------------------------------------*/ CASE WHEN (@Year_ - YEAR(dbo.EmployeeCard.StartWorkingDate)
                                                        + 1) &gt;= 1 THEN (ISNULL
                                                           ((SELECT        N .Balance
                                                               FROM            dbo.BalanceSlice N
                                                               WHERE        N .IsVertualDeleted = 0 AND N .LeaveSetting_id = dbo.LeaveSetting.Id AND (@Year_ - YEAR(dbo.EmployeeCard.StartWorkingDate) + 1) &gt;= N .FromYearOfServices AND 
                                                                                         (@Year_ - YEAR(dbo.EmployeeCard.StartWorkingDate) + 1) &lt; N .ToYearOfServices), (dbo.LeaveSetting.Balance))) WHEN (@Year_ - YEAR(dbo.EmployeeCard.StartWorkingDate) + 1) = 0 AND 
                                                       ((((CAST(CASE WHEN Year(@DateNow) &gt; @Year_ THEN 12 ELSE Month(@DateNow) + 1 END - Month(dbo.EmployeeCard.StartWorkingDate) AS float) / 12) 
                                                       - CAST(@Year_ - YEAR(dbo.EmployeeCard.StartWorkingDate) + 1 AS int)) * 12)) &gt; 0 AND dbo.LeaveSetting.IsAffectedByAssigningDate = 1 THEN (ISNULL
                                                           (((SELECT        TOP 1 N .Balance
                                                                 FROM            dbo.BalanceSlice N
                                                                 WHERE        N .IsVertualDeleted = 0 AND N .LeaveSetting_id = dbo.LeaveSetting.Id AND (@Year_ - YEAR(dbo.EmployeeCard.StartWorkingDate) + 1) &gt;= N .FromYearOfServices AND 
                                                                                          (@Year_ - YEAR(dbo.EmployeeCard.StartWorkingDate) + 1) &lt; N .ToYearOfServices) / 12) * ((((CAST(CASE WHEN Year(@DateNow) &gt; @Year_ THEN 12 ELSE Month(@DateNow) 
                                                       + 1 END - Month(dbo.EmployeeCard.StartWorkingDate) AS float) / 12) - CAST(@Year_ - YEAR(dbo.EmployeeCard.StartWorkingDate) + 1 AS int)) * 12)), ((dbo.LeaveSetting.Balance / 12) 
                                                       * ((((CAST(CASE WHEN Year(@DateNow) &gt; @Year_ THEN 12 ELSE Month(@DateNow) + 1 END - Month(dbo.EmployeeCard.StartWorkingDate) AS float) / 12) 
                                                       - CAST(@Year_ - YEAR(dbo.EmployeeCard.StartWorkingDate) + 1 AS int)) * 12))))) +
                                                           (SELECT        ISNULL(SUM(M.RoundedBalance), 0)
                                                              FROM            dbo.RecycledLeave M
                                                              WHERE        M.IsVertualDeleted = 0 AND M.EmployeeCard_id = dbo.EmployeeCard.Id AND M.leaveSetting_Id = dbo.LeaveSetting.Id AND M.RecycleType = 0 AND M.Year = @Year_) END -
                                                           (SELECT        SUM(LR.SpentDays)
                                                              /*SUM(DATEDIFF(DAY, LR.StartDate, LR.EndDate))          */ FROM dbo.LeaveRequest LR INNER JOIN
                                                                                       dbo.EmployeeCard EC ON LR.EmployeeCard_id = EC.Id
                                                              WHERE        LR.IsVertualDeleted = 0 AND EC.IsVertualDeleted = 0 AND LR.LeaveSetting_id = dbo.LeaveSetting.Id AND EC.Id = dbo.EmployeeCard.Id AND (Year(LR.StartDate) = @Year_ AND Month(LR.StartDate) 
                                                                                       &gt;= 1 AND Day(LR.StartDate) &gt;= 1) AND (Year(LR.EndDate) = @Year_ AND Month(LR.EndDate) &lt;= 12 AND Day(LR.EndDate) &lt;= 31) AND LR.LeaveStatus = 2/*Status Approved*/ ) 
                                                       AS RemainingBalance, /*------------------------------------------------------------------------------------------------------------*/ dbo.LeaveSetting.HasMaximumNumber, 
                                                       dbo.LeaveSetting.MaximumNumber, 
                                                       /*----------------------------------------------GivenTheNumberOfTimes------------------------------------------*/ CASE WHEN dbo.LeaveSetting.HasMaximumNumber = 1 THEN
                                                           (SELECT        COUNT(LR.Id)
                                                              FROM            dbo.LeaveRequest LR INNER JOIN
                                                                                       dbo.EmployeeCard EC ON LR.EmployeeCard_id = EC.Id
                                                              WHERE        LR.IsVertualDeleted = 0 AND EC.IsVertualDeleted = 0 AND LR.LeaveSetting_id = dbo.LeaveSetting.Id AND EC.Id = dbo.EmployeeCard.Id AND (Year(LR.StartDate) = @Year_ AND Month(LR.StartDate) 
                                                                                       &gt;= 1 AND Day(LR.StartDate) &gt;= 1) AND (Year(LR.EndDate) = @Year_ AND Month(LR.EndDate) &lt;= 12 AND Day(LR.EndDate) &lt;= 31) AND LR.LeaveStatus = 2/*Status Approved*/ ) 
                                                       WHEN dbo.LeaveSetting.HasMaximumNumber = 0 THEN 0 END AS GivenTheNumberOfTimes, 
                                                       /*----------------------------------------------TheRemainingNumberOfTimes---------------------------------------*/ CASE WHEN dbo.LeaveSetting.HasMaximumNumber = 1 THEN dbo.LeaveSetting.MaximumNumber
                                                        -
                                                           (SELECT        COUNT(LR.Id)
                                                              FROM            dbo.LeaveRequest LR INNER JOIN
                                                                                       dbo.EmployeeCard EC ON LR.EmployeeCard_id = EC.Id
                                                              WHERE        LR.IsVertualDeleted = 0 AND EC.IsVertualDeleted = 0 AND LR.LeaveSetting_id = dbo.LeaveSetting.Id AND EC.Id = dbo.EmployeeCard.Id AND (Year(LR.StartDate) = @Year_ AND Month(LR.StartDate) 
                                                                                       &gt;= 1 AND Day(LR.StartDate) &gt;= 1) AND (Year(LR.EndDate) = @Year_ AND Month(LR.EndDate) &lt;= 12 AND Day(LR.EndDate) &lt;= 31) AND LR.LeaveStatus = 2/*Status Approved*/ ) 
                                                       WHEN dbo.LeaveSetting.HasMaximumNumber = 0 THEN 0 END AS TheRemainingNumberOfTimes, CASE WHEN ISNULL(dbo.EmployeeCard.LeaveTemplateMaster_id, 0) 
                                                       != 0 THEN dbo.EmployeeCard.LeaveTemplateMaster_id END AS TemplateMasterId
                              FROM            dbo.Employee INNER JOIN
                                                       dbo.EmployeeCard ON dbo.Employee.Id = dbo.EmployeeCard.Employee_id INNER JOIN
                                                       dbo.LeaveTemplateMaster ON dbo.LeaveTemplateMaster.Id = dbo.EmployeeCard.LeaveTemplateMaster_id INNER JOIN
                                                       dbo.LeaveTemplateDetail ON dbo.LeaveTemplateDetail.LeaveTemplateMaster_id = dbo.LeaveTemplateMaster.Id AND 
                                                       dbo.LeaveTemplateDetail.LeaveTemplateMaster_id = dbo.LeaveTemplateMaster.Id INNER JOIN
                                                       dbo.LeaveSetting ON dbo.LeaveTemplateDetail.LeaveSetting_id = dbo.LeaveSetting.Id INNER JOIN
                                                       dbo.LeaveType ON dbo.LeaveSetting.Type_id = dbo.LeaveType.Id
                              WHERE        (CASE WHEN ISNULL(dbo.EmployeeCard.LeaveTemplateMaster_id, 0) != 0 THEN dbo.EmployeeCard.LeaveTemplateMaster_id END) = LeaveTemplateMaster.Id AND dbo.EmployeeCard.CardStatus = 1 AND 
                                                       Employee.IsVertualDeleted = 0 AND LeaveSetting.IsVertualDeleted = 0 AND LeaveType.IsVertualDeleted = 0 AND LeaveTemplateDetail.IsVertualDeleted = 0 AND LeaveTemplateMaster.IsVertualDeleted = 0</CommandText>
      </Query>
