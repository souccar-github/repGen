  <DataSets>
    <DataSet Name="BasicInfoDataSet">
      <Query>
        <DataSourceName>DataSource</DataSourceName>
        <CommandText>SELECT
dbo.Certification.Type_id, 
(SELECT Name FROM dbo.CertificationType WHERE dbo.CertificationType.Id=dbo.Certification.Type_id) AS CertificationTypeName,
dbo.Certification.PlaceOfIssuance_id, 
(SELECT Name FROM dbo.Country WHERE dbo.Country.Id=PlaceOfIssuance_id) AS PlaceOfIssuanceName,
dbo.Certification.DateOfIssuance,
dbo.Certification.ExpirationDate, 
dbo.Certification.Employee_id
FROM
dbo.Certification</CommandText>
      </Query>
      <Fields>
        <Field Name="Type_id">
          <DataField>Type_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="CertificationTypeName">
          <DataField>CertificationTypeName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PlaceOfIssuance_id">
          <DataField>PlaceOfIssuance_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="PlaceOfIssuanceName">
          <DataField>PlaceOfIssuanceName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DateOfIssuance">
          <DataField>DateOfIssuance</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="ExpirationDate">
          <DataField>ExpirationDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Employee_id">
          <DataField>Employee_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!Type_id.Value</FilterExpression>
          <Operator>In</Operator>
          <FilterValues>
            <FilterValue>=Parameters!certificationTypeId.Value</FilterValue>
