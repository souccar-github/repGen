    <DataSet Name="MonthlyEmployeeBenefiDataSet">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>with MonthDetails AS
(
SELECT        
AttendanceMonthlyAdjustment.Id AttendanceMonthlyAdjustmentId,
InitialOvertimeValue, 
FinalOvertimeValue, 
FinalNonAttendanceValue, 
(SELECT TOP 1 N.Name FROM DisciplinarySetting N WHERE N.Id = dbo.AttendanceMonthlyAdjustment.Penalty_id) AS Penalty,
EmployeeAttendanceCard_id,
dbo.AttendanceRecord.Number as MonthNumber ,
dbo.AttendanceRecord.Name MonthName,
dbo.AttendanceRecord.Id MonthId,
dbo.AttendanceMonthlyAdjustmentDetail.WorkHoursValue - dbo.AttendanceMonthlyAdjustmentDetail.VacationValue - dbo.AttendanceMonthlyAdjustmentDetail.MissionValue
AS RequiredHoursValue,
CASE WHEN (IsOffDay=1) AND (IsHoliday=1) THEN 0 ELSE (WorkHoursValue - VacationValue - MissionValue) END RequiredWorkHoursValue,--عدد ساعات العمل المطلوبة لهذا اليوم حسب الوردية
WorkHoursValue,
VacationValue,
MissionValue,
ActualWorkHoursValue,
OvertimeOrderValue
FROM            
dbo.AttendanceMonthlyAdjustment INNER JOIN 
dbo.AttendanceMonthlyAdjustmentDetail on dbo.AttendanceMonthlyAdjustment.Id=dbo.AttendanceMonthlyAdjustmentDetail.AttendanceMonthlyAdjustment_id INNER JOIN 
dbo.AttendanceRecord ON dbo.AttendanceRecord.Id = dbo.AttendanceMonthlyAdjustment.AttendanceRecord_id
where	
AttendanceMonthlyAdjustment.IsVertualDeleted = 0 AND
AttendanceRecord.IsVertualDeleted = 0 
)

select
EmployeeAttendanceCard_id,
AttendanceMonthlyAdjustmentId,
(SELECT dbo.Employee.FirstName + ' ' + dbo.Employee.FatherName + ' ' + dbo.Employee.LastName AS FullName
FROM	dbo.EmployeeCard INNER JOIN	dbo.Employee ON dbo.EmployeeCard.Employee_id = dbo.Employee.Id
WHERE	EmployeeCard.IsVertualDeleted = 0 AND Employee.IsVertualDeleted = 0 AND dbo.EmployeeCard.Id = EmployeeAttendanceCard_id) FullName,
(SELECT dbo.Employee.Id AS EmployeeId
FROM	dbo.EmployeeCard INNER JOIN	dbo.Employee ON dbo.EmployeeCard.Employee_id = dbo.Employee.Id
WHERE	EmployeeCard.IsVertualDeleted = 0 AND Employee.IsVertualDeleted = 0 AND dbo.EmployeeCard.Id = EmployeeAttendanceCard_id) EmployeeId,
MonthNumber,
MonthName,
MonthId,


(SELECT        RIGHT('00' + CONVERT(varchar(8), FLOOR(ROUND(InitialOvertimeValue, 2))), CASE WHEN LEN(CONVERT(varchar(8), FLOOR(ROUND(InitialOvertimeValue, 2))))&gt;2 THEN 3 ELSE 2 END) + ':' + RIGHT('00' + CONVERT(varchar(8),
                                                                  (SELECT        CONVERT(int, (ROUND(InitialOvertimeValue, 2) - FLOOR(ROUND(InitialOvertimeValue, 2))) * 60, 0) 
                                                                                              AS Expr1)), 2) AS Expr1) AS InitialOvertimeValue, --الاضافي الشهري المحتسب قبل معامل الضرب


(SELECT        RIGHT('00' + CONVERT(varchar(8), FLOOR(ROUND(FinalOvertimeValue, 2))), CASE WHEN LEN(CONVERT(varchar(8), FLOOR(ROUND(FinalOvertimeValue, 2))))&gt;2 THEN 3 ELSE 2 END) + ':' + RIGHT('00' + CONVERT(varchar(8),
                                                                  (SELECT        CONVERT(int, (ROUND(FinalOvertimeValue, 2) - FLOOR(ROUND(FinalOvertimeValue, 2))) * 60, 0) 
                                                                                              AS Expr1)), 2) AS Expr1) AS FinalOvertimeValue, --الاضافي الشهري المحتسب بعد معامل الضرب


(SELECT        RIGHT('00' + CONVERT(varchar(8), FLOOR(ROUND(FinalNonAttendanceValue, 2))), CASE WHEN LEN(CONVERT(varchar(8), FLOOR(ROUND(FinalNonAttendanceValue, 2))))&gt;2 THEN 3 ELSE 2 END) + ':' + RIGHT('00' + CONVERT(varchar(8),
                                                                  (SELECT        CONVERT(int, (ROUND(FinalNonAttendanceValue, 2) - FLOOR(ROUND(FinalNonAttendanceValue, 2))) * 60, 0) 
                                                                                              AS Expr1)), 2) AS Expr1) AS FinalNonAttendanceValue, --نقص الدوام بعد الضرب بالمعامل
Penalty,


 (SELECT        RIGHT('00' + CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(RequiredHoursValue), 0), 2))), CASE WHEN LEN(CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(RequiredHoursValue), 0), 2))))&gt;2 THEN 3 ELSE 2 END) 
                                                              + ':' + RIGHT('00' + CONVERT(varchar(8),
                                                                  (SELECT        CONVERT(int, (ROUND(ISNULL(SUM(RequiredHoursValue), 0), 2) 
                                                                                              - FLOOR(ROUND(ISNULL(SUM(RequiredHoursValue), 0), 2))) * 60, 0) AS Expr1)), 2) AS Expr1) AS SumRequiredHoursValue,


 (SELECT        RIGHT('00' + CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(RequiredWorkHoursValue), 0), 2))), CASE WHEN LEN(CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(RequiredWorkHoursValue), 0), 2))))&gt;2 THEN 3 ELSE 2 END) 
                                                              + ':' + RIGHT('00' + CONVERT(varchar(8),
                                                                  (SELECT        CONVERT(int, (ROUND(ISNULL(SUM(RequiredWorkHoursValue), 0), 2) 
                                                                                              - FLOOR(ROUND(ISNULL(SUM(RequiredWorkHoursValue), 0), 2))) * 60, 0) AS Expr1)), 2) AS Expr1) AS SumRequiredWorkHoursValue,


 (SELECT        RIGHT('00' + CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(WorkHoursValue), 0), 2))), CASE WHEN LEN(CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(WorkHoursValue), 0), 2))))&gt;2 THEN 3 ELSE 2 END) 
                                                              + ':' + RIGHT('00' + CONVERT(varchar(8),
                                                                  (SELECT        CONVERT(int, (ROUND(ISNULL(SUM(WorkHoursValue), 0), 2) 
                                                                                              - FLOOR(ROUND(ISNULL(SUM(WorkHoursValue), 0), 2))) * 60, 0) AS Expr1)), 2) AS Expr1) AS SumWorkHoursValue,--عدد ساعات العمل المطلوبة بالشهر حسب الوردية



(SELECT        RIGHT('00' + CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(VacationValue), 0), 2))), CASE WHEN LEN(CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(VacationValue), 0), 2))))&gt;2 THEN 3 ELSE 2 END) 
                                                              + ':' + RIGHT('00' + CONVERT(varchar(8),
                                                                  (SELECT        CONVERT(int, (ROUND(ISNULL(SUM(VacationValue), 0), 2) 
                                                                                              - FLOOR(ROUND(ISNULL(SUM(VacationValue), 0), 2))) * 60, 0) AS Expr1)), 2) AS Expr1) AS SumVacationValue,



(SELECT        RIGHT('00' + CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(MissionValue), 0), 2))),CASE WHEN LEN(CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(MissionValue), 0), 2))))&gt;2 THEN 3 ELSE 2 END) 
                                                              + ':' + RIGHT('00' + CONVERT(varchar(8),
                                                                  (SELECT        CONVERT(int, (ROUND(ISNULL(SUM(MissionValue), 0), 2) 
                                                                                              - FLOOR(ROUND(ISNULL(SUM(MissionValue), 0), 2))) * 60, 0) AS Expr1)), 2) AS Expr1) AS SumMissionValue,


(SELECT        RIGHT('00' + CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(ActualWorkHoursValue), 0), 2))), CASE WHEN LEN(CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(ActualWorkHoursValue), 0), 2))))&gt;2 THEN 3 ELSE 2 END) 
                                                              + ':' + RIGHT('00' + CONVERT(varchar(8),
                                                                  (SELECT        CONVERT(int, (ROUND(ISNULL(SUM(ActualWorkHoursValue), 0), 2) 
                                                                                              - FLOOR(ROUND(ISNULL(SUM(ActualWorkHoursValue), 0), 2))) * 60, 0) AS Expr1)), 2) AS Expr1) AS SumActualWorkHoursValue,--عدد ساعات العمل المحققة بالشهر



(SELECT        RIGHT('00' + CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(OvertimeOrderValue), 0), 2))), CASE WHEN LEN(CONVERT(varchar(8), FLOOR(ROUND(ISNULL(SUM(OvertimeOrderValue), 0), 2))))&gt;2 THEN 3 ELSE 2 END) 
                                                              + ':' + RIGHT('00' + CONVERT(varchar(8),
                                                                  (SELECT        CONVERT(int, (ROUND(ISNULL(SUM(OvertimeOrderValue), 0), 2) 
                                                                                              - FLOOR(ROUND(ISNULL(SUM(OvertimeOrderValue), 0), 2))) * 60, 0) AS Expr1)), 2) AS Expr1) AS SumOvertimeOrderValue,--عدد ساعات التكليف
--ISNULL(SUM(RequiredWorkHoursValue),0)AS SumRequiredWorkHoursValue,





(SELECT        RIGHT('00' + CONVERT(varchar(8), FLOOR(ROUND(CASE WHEN ISNULL(SUM(RequiredWorkHoursValue),0) &gt; ISNULL(SUM(ActualWorkHoursValue),0) THEN ISNULL(SUM(RequiredWorkHoursValue),0) - ISNULL(SUM(ActualWorkHoursValue),0) ELSE 0 END, 2))), CASE WHEN LEN(CONVERT(varchar(8), FLOOR(ROUND(CASE WHEN ISNULL(SUM(RequiredWorkHoursValue),0) &gt; ISNULL(SUM(ActualWorkHoursValue),0) THEN ISNULL(SUM(RequiredWorkHoursValue),0) - ISNULL(SUM(ActualWorkHoursValue),0) ELSE 0 END, 2))))&gt;2 THEN 3 ELSE 2 END) + ':' + RIGHT('00' + CONVERT(varchar(8),
                                                                  (SELECT        CONVERT(int, (ROUND(CASE WHEN ISNULL(SUM(RequiredWorkHoursValue),0) &gt; ISNULL(SUM(ActualWorkHoursValue),0) THEN ISNULL(SUM(RequiredWorkHoursValue),0) - ISNULL(SUM(ActualWorkHoursValue),0) ELSE 0 END, 2)
																   - FLOOR(ROUND(CASE WHEN ISNULL(SUM(RequiredWorkHoursValue),0) &gt; ISNULL(SUM(ActualWorkHoursValue),0) THEN ISNULL(SUM(RequiredWorkHoursValue),0) - ISNULL(SUM(ActualWorkHoursValue),0) ELSE 0 END, 2))) * 60, 0) 
                                                                                              AS Expr1)), 2) AS Expr1) AS InitialNonAttendanceValue,--نقص الدوام






(SELECT        RIGHT('00' + CONVERT(varchar(8), FLOOR(ROUND(CASE WHEN ISNULL(SUM(ActualWorkHoursValue),0) &gt; ISNULL(SUM(RequiredWorkHoursValue),0) THEN ISNULL(SUM(ActualWorkHoursValue),0) - ISNULL(SUM(RequiredWorkHoursValue),0) ELSE 0 END, 2))), CASE WHEN LEN(CONVERT(varchar(8), FLOOR(ROUND(CASE WHEN ISNULL(SUM(ActualWorkHoursValue),0) &gt; ISNULL(SUM(RequiredWorkHoursValue),0) THEN ISNULL(SUM(ActualWorkHoursValue),0) - ISNULL(SUM(RequiredWorkHoursValue),0) ELSE 0 END, 2))))&gt;2 THEN 3 ELSE 2 END) + ':' + RIGHT('00' + CONVERT(varchar(8),
                                                                  (SELECT        CONVERT(int, (ROUND(CASE WHEN ISNULL(SUM(ActualWorkHoursValue),0) &gt; ISNULL(SUM(RequiredWorkHoursValue),0) THEN ISNULL(SUM(ActualWorkHoursValue),0) - ISNULL(SUM(RequiredWorkHoursValue),0) ELSE 0 END, 2) 
																  - FLOOR(ROUND(CASE WHEN ISNULL(SUM(ActualWorkHoursValue),0) &gt; ISNULL(SUM(RequiredWorkHoursValue),0) THEN ISNULL(SUM(ActualWorkHoursValue),0) - ISNULL(SUM(RequiredWorkHoursValue),0) ELSE 0 END, 2))) * 60, 0) 
                                                                                              AS Expr1)), 2) AS Expr1) AS ExpectedOvertimeValue--الاضافي الشهري المحتمل
from   MonthDetails 
group by 
AttendanceMonthlyAdjustmentId,
monthnumber,
MonthName,
MonthId,
EmployeeAttendanceCard_id,
Penalty,
InitialOvertimeValue,
FinalOvertimeValue,
FinalNonAttendanceValue</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
