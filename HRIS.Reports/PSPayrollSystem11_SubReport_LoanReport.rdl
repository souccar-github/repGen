  <DataSets>
    <DataSet Name="LoanDataSet">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>Select 
LoanPayment.MonthlyCard_id MonthlyCardId,
DonorLoan.Name DonorLoanName,
LoanPayment.PaymentValue,
LoanPayment.RemainingValueAfterPaymentValue
From
MonthlyCard Inner Join
LoanPayment On(LoanPayment.MonthlyCard_id = MonthlyCard.Id AND LoanPayment.IsExternalPayment = 0) Inner Join
EmployeeLoan On(EmployeeLoan.Id = LoanPayment.EmployeeLoan_id)Inner Join
DonorLoan On(DonorLoan.Id = EmployeeLoan.DonorLoan_id)
where 
(dbo.LoanPayment.IsVertualDeleted=0) AND
(dbo.MonthlyCard.IsVertualDeleted=0) AND
(dbo.EmployeeLoan.IsVertualDeleted=0) AND
(dbo.DonorLoan.IsVertualDeleted=0)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="MonthlyCardId">
          <DataField>MonthlyCardId</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DonorLoanName">
          <DataField>DonorLoanName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PaymentValue">
          <DataField>PaymentValue</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="RemainingValueAfterPaymentValue">
          <DataField>RemainingValueAfterPaymentValue</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!MonthlyCardId.Value</FilterExpression>
          <Operator>In</Operator>
          <FilterValues>
            <FilterValue>=Parameters!mcid.Value</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
