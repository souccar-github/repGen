ht>0.56444cm</Height><Width>21.886cm</Width><ZIndex>2</ZIndex><Style><Border><Style>None</Style></Border><PaddingLeft>2pt</PaddingLeft><PaddingRight>2pt</PaddingRight><PaddingTop>2pt</PaddingTop><PaddingBottom>2pt</PaddingBottom><Direction>RTL</Direction></Style></Textbox></ReportItems><Height>4.09116in</Height><Style /></Body><Width>8.97382in</Width><Page><PageFooter><Height>0.84667cm</Height><PrintOnFirstPage>true</PrintOnFirstPage><PrintOnLastPage>true</PrintOnLastPage><ReportItems><Textbox Name="TotalPages"><CanGrow>true</CanGrow><KeepTogether>true</KeepTogether><Paragraphs><Paragraph><TextRuns><TextRun><Value>= "Page "&amp; Globals!PageNumber &amp; " of " &amp; Globals!TotalPages</Value><Style /></TextRun></TextRuns><Style><TextAlign>Right</TextAlign></Style></Paragraph></Paragraphs><rd:DefaultName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">TotalPages</rd:DefaultName><Top>0.14111cm</Top><Left>0cm</Left><Height>0.6cm</Height><Width>21.886cm</Width><Style><Border><Style>None</Style></Border><PaddingLeft>2pt</PaddingLeft><PaddingRight>2pt</PaddingRight><PaddingTop>2pt</PaddingTop><PaddingBottom>2pt</PaddingBottom></Style></Textbox></ReportItems><Style><Border><Style>None</Style></Border></Style></PageFooter><PageHeight>29.7cm</PageHeight><PageWidth>26cm</PageWidth><LeftMargin>0.5cm</LeftMargin><RightMargin>0.5cm</RightMargin><TopMargin>2cm</TopMargin><BottomMargin>2cm</BottomMargin><ColumnSpacing>0.13cm</ColumnSpacing><Style /></Page><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="DataSource1"><DataSourceReference>SharedDataSource</DataSourceReference><rd:SecurityType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">None</rd:SecurityType><rd:DataSourceID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">d5a02870-23e4-4bd7-9a48-107238f7bd9a</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="BasicInfoDataSet"><Query><DataSourceName>DataSource1</DataSourceName><CommandText>SELECT        
MonthlyCard.Id AS MonthlyCardId, 
Employee.Code AS EmployeeCode, 
Employee.FirstName + ' ' + ISNULL(Employee.FatherName,'') + ' ' + Employee.LastName AS FullName, 
Month.Name AS MonthName, 
Month.Id AS MonthId, 
MonthlyCard.Salary, 
Employee.Id AS EmployeeId,
Node.Name AS NodeName,

--مجموع تعويضات الموظف
(Select 
ISNULL(SUM(MonthlyEmployeeBenefit.InitialValue), 0) 
From 
MonthlyEmployeeBenefit 
Where 
MonthlyEmployeeBenefit.MonthlyCard_id = MonthlyCard.ID  AND dbo.MonthlyEmployeeBenefit.IsVertualDeleted=0) 
As TotalEmployeeBenefits,
------------------

--مجموع حسميات الموظف
(Select 
ISNULL(SUM(MonthlyEmployeeDeduction.FinalValue), 0) 
From 
MonthlyEmployeeDeduction 
Where 
MonthlyEmployeeDeduction.MonthlyCard_id = MonthlyCard.ID  AND dbo.MonthlyEmployeeDeduction.IsVertualDeleted=0) 
As TotalEmployeeDeductions,
-------------------

--مجموع قروض الموظف
(Select 
ISNULL(SUM(LoanPayment.PaymentValue), 0) 
From 
LoanPayment 
Where 
LoanPayment.MonthlyCard_id = MonthlyCard.ID  AND dbo.LoanPayment.IsVertualDeleted=0)
As TotalEmployeeLoans,
-------------------

--الراتب الصافي للموظف
CASE Month.MonthType WHEN 0 THEN MonthlyCard.Salary +
(SELECT     ISNULL(SUM(MonthlyEmployeeBenefit.InitialValue), 0)
FROM         MonthlyEmployeeBenefit
WHERE     MonthlyEmployeeBenefit.MonthlyCard_id = MonthlyCard.ID AND dbo.MonthlyEmployeeBenefit.IsVertualDeleted = 0) -
(SELECT     ISNULL(SUM(MonthlyEmployeeDeduction.FinalValue), 0)
FROM         MonthlyEmployeeDeduction
WHERE     MonthlyEmployeeDeduction.MonthlyCard_id = MonthlyCard.ID AND dbo.MonthlyEmployeeDeduction.IsVertualDeleted = 0) -
(SELECT     ISNULL(SUM(LoanPayment.PaymentValue), 0)
FROM         LoanPayment
WHERE     LoanPayment.MonthlyCard_id = MonthlyCard.ID 
AND dbo.LoanPayment.IsVertualDeleted = 0 AND dbo.LoanPayment.IsExternalPayment = 0) 
             
WHEN 1 THEN
(SELECT     ISNULL(SUM(MonthlyEmployeeBenefit.InitialValue), 0)
FROM         MonthlyEmployeeBenefit
WHERE     MonthlyEmployeeBenefit.MonthlyCard_id = MonthlyCard.ID AND dbo.MonthlyEmployeeBenefit.IsVertualDeleted = 0) -
(SELECT     ISNULL(SUM(MonthlyEmployeeDeduction.FinalValue), 0)
FROM         MonthlyEmployeeDeduction
WHERE     MonthlyEmployeeDeduction.MonthlyCard_id = MonthlyCard.ID AND dbo.MonthlyEmployeeDeduction.IsVertualDeleted = 0)-
(SELECT     ISNULL(SUM(LoanPayment.PaymentValue), 0)
FROM         LoanPayment
WHERE     LoanPayment.MonthlyCard_id = MonthlyCard.ID 
AND dbo.LoanPayment.IsVertualDeleted = 0 AND dbo.LoanPayment.IsExternalPayment = 0)  END AS ActualFinalSalary
-------------------


, dbo.Month.Date AS MonthDate
FROM            
Month INNER JOIN
MonthlyCard ON MonthlyCard.Month_id = Month.Id INNER JOIN
EmployeeCard ON EmployeeCard.Id = MonthlyCard.PrimaryCard_id INNER JOIN
Employee ON Employee.Id = EmployeeCard.Employee_id LEFT OUTER JOIN
Node INNER JOIN
Position INNER JOIN
AssigningEmployeeToPosition ON Position.Id = AssigningEmployeeToPosition.Position_Id INNER JOIN
JobDescription ON Position.JobDescription_id = JobDescription.Id ON Node.Id = JobDescription.Node_id ON 
Employee.Id = AssigningEmployeeToPosition.Employee_id
WHERE        (MonthlyCard.IsVertualDeleted = 0) AND (Month.IsVertualDeleted = 0) AND (EmployeeCard.IsVertualDeleted = 0) AND (Employee.IsVertualDeleted = 0) </CommandText><rd:UseGenericDesigner xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">true</rd:UseGenericDesigner></Query><Fields><Field Name="MonthlyCardId"><DataField>MonthlyCardId</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="EmployeeCode"><DataField>EmployeeCode</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="FullName"><DataField>FullName</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="MonthName"><DataField>MonthName</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="MonthId"><DataField>MonthId</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="Salary"><DataField>Salary</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Single</rd:TypeName></Field><Field Name="EmployeeId"><DataField>EmployeeId</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="NodeName"><DataField>NodeName</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="TotalEmployeeBenefits"><DataField>TotalEmployeeBenefits</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Double</rd:TypeName></Field><Field Name="TotalEmployeeDeductions"><DataField>Tota