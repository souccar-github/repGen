  from EntranceExitRecord as Records  left  join Employee as Emp on Records.Employee_id=Emp.Id
  where Emp.IsVertualDeleted=0 and Emp.IsVertualDeleted=0 
  and Records.LogDate=@Date
  group by Emp.Id ,Emp.FirstName+' '+Emp.LastName
  order by Emp.FirstName+' '+Emp.LastName </CommandText><rd:UseGenericDesigner xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">true</rd:UseGenericDesigner></Query><Fields><Field Name="emp_Id"><DataField>emp_Id</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="min_"><DataField>min_</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="max_"><DataField>max_</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="emp_name"><DataField>emp_name</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field></Fields><Filters><Filter><FilterExpression>=Fields!emp_Id.Value</FilterExpression><Operator>In</Operator><FilterValues><FilterValue>=Parameters!EmpID.Value</FilterValue></FilterValues></Filter></Filters></DataSet><DataSet Name="DataSet2"><Query><DataSourceName>SharedDataSource</DataSourceName><CommandText>select E.FirstName+' '+E.LastName as emp_name, E.Id as emp_Id
  from Employee as E left join  EmployeeCard Emp_card on E.id=Emp_card.Employee_id
where E.IsVertualDeleted=0 AND Emp_card.CardStatus=1
ORDER BY emp_name ASC</CommandText></Query><Fields><Field Name="emp_name"><DataField>emp_name</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="emp_Id"><DataField>emp_Id</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field></Fields></DataSet><DataSet Name="DataSet3"><Query><DataSourceName>SharedDataSource</DataSourceName><QueryParameters><QueryParameter Name="@Date"><Value>=Parameters!Date.Value</Value></QueryParameter></QueryParameters><CommandText> select Id ,FirstName+' '+LastName as name from Employee where id not in  (
 select  emp.Id as emp_Id 
  from EntranceExitRecord as Records  left  join Employee as Emp on Records.Employee_id=Emp.Id
  where Emp.IsVertualDeleted=0 and Emp.IsVertualDeleted=0 
  and Records.LogDate=@Date
  )
  order by FirstName+' '+LastName </CommandText></Query><Fields><Field Name="Id"><DataField>Id</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="name"><DataField>name</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field></Fields><Filters><Filter><FilterExpression>=Fields!Id.Value</FilterExpression><Operator>In</Operator><FilterValues><FilterValue>=Parameters!EmpID.Value</FilterValue></FilterValues></Filter></Filters></DataSet></DataSets><ReportParameters><ReportParameter Name="Date"><DataType>DateTime</DataType><Prompt>التاريخ</Prompt></ReportParameter><ReportParameter Name="EmpID"><DataType>Integer</DataType><Prompt>الموظف</Prompt><ValidValues><DataSetReference><DataSetName>DataSet2</DataSetName><ValueField>emp_Id</ValueField><LabelField>emp_name</LabelField></DataSetReference></ValidValues><MultiValue>true</MultiValue></ReportParameter></ReportParameters><Language>en-US</Language><ConsumeContainerWhitespace>true</ConsumeContainerWhitespace><rd:ReportUnitType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">Inch</rd:ReportUnitType><rd:ReportID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">e33b9b13-d1e3-47f0-98be-38a66ba7d1b0</rd:ReportID></Report>