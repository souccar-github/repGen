tyle><Border><Style>None</Style></Border></Style></Tablix></ReportItems><Height>1.5561in</Height><Style /></Body><Width>16.65304in</Width><Page><PageHeight>29.7cm</PageHeight><PageWidth>21cm</PageWidth><LeftMargin>2cm</LeftMargin><RightMargin>2cm</RightMargin><TopMargin>2cm</TopMargin><BottomMargin>2cm</BottomMargin><ColumnSpacing>0.13cm</ColumnSpacing><Style /></Page><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="DataSource1"><DataSourceReference>SharedDataSource</DataSourceReference><rd:SecurityType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">None</rd:SecurityType><rd:DataSourceID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">eab1e084-b509-454d-8fe6-97d24f65d6d8</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="MonthlyEmployeeBenefiDataSet"><Query><DataSourceName>DataSource1</DataSourceName><CommandText>with MonthDetails AS
(
SELECT        
AttendanceMonthlyAdjustment.Id AttendanceMonthlyAdjustmentId,
InitialOvertimeValue, 
FinalOvertimeValue, 
FinalNonAttendanceValue, 
(SELECT TOP 1 N.Name FROM DisciplinarySetting N WHERE N.Id = dbo.AttendanceMonthlyAdjustment.Penalty_id) AS Penalty,
EmployeeAttendanceCard_id,
dbo.AttendanceRecord.Number as MonthNumber ,
dbo.AttendanceRecord.Name MonthName,
dbo.AttendanceRecord.Id MonthId,
dbo.AttendanceMonthlyAdjustmentDetail.WorkHoursValue - dbo.AttendanceMonthlyAdjustmentDetail.VacationValue - dbo.AttendanceMonthlyAdjustmentDetail.MissionValue
AS RequiredHoursValue,
CASE WHEN (IsOffDay=1) AND (IsHoliday=1) THEN 0 ELSE (WorkHoursValue - VacationValue - MissionValue) END RequiredWorkHoursValue,--عدد ساعات العمل المطلوبة لهذا اليوم حسب الوردية
WorkHoursValue,
VacationValue,
MissionValue,
ActualWorkHoursValue,
OvertimeOrderValue
FROM            
dbo.AttendanceMonthlyAdjustment INNER JOIN 
dbo.AttendanceMonthlyAdjustmentDetail on dbo.AttendanceMonthlyAdjustment.Id=dbo.AttendanceMonthlyAdjustmentDetail.AttendanceMonthlyAdjustment_id INNER JOIN 
dbo.AttendanceRecord ON dbo.AttendanceRecord.Id = dbo.AttendanceMonthlyAdjustment.AttendanceRecord_id
where	
AttendanceMonthlyAdjustment.IsVertualDeleted = 0 AND
AttendanceRecord.IsVertualDeleted = 0 
)

select
EmployeeAttendanceCard_id,
AttendanceMonthlyAdjustmentId,
(SELECT dbo.Employee.FirstName + ' ' + dbo.Employee.FatherName + ' ' + dbo.Employee.LastName AS FullName
FROM	dbo.EmployeeCard INNER JOIN	dbo.Employee ON dbo.EmployeeCard.Employee_id = dbo.Employee.Id
WHERE	EmployeeCard.IsVertualDeleted = 0 AND Employee.IsVertualDeleted = 0 AND dbo.EmployeeCard.Id = EmployeeAttendanceCard_id) FullName,
(SELECT dbo.Employee.Id AS EmployeeId
FROM	dbo.EmployeeCard INNER JOIN	dbo.Employee ON dbo.EmployeeCard.Employee_id = dbo.Employee.Id
WHERE	EmployeeCard.IsVertualDeleted = 0 AND Employee.IsVertualDeleted = 0 AND dbo.EmployeeCard.Id = EmployeeAttendanceCard_id) EmployeeId,
MonthNumber,
MonthName,
MonthId,
ROUND(InitialOvertimeValue,2) InitialOvertimeValue, --الاضافي الشهري المحتسب قبل معامل الضرب
ROUND(FinalOvertimeValue,2) FinalOvertimeValue, --الاضافي الشهري المحتسب بعد معامل الضرب
ROUND(FinalNonAttendanceValue,2) FinalNonAttendanceValue, --نقص الدوام بعد الضرب بالمعامل
Penalty,
ROUND(ISNULL(SUM(RequiredHoursValue),0),2) AS SumRequiredHoursValue,
ROUND(ISNULL(SUM(RequiredWorkHoursValue),0),2) AS SumRequiredWorkHoursValue,
ROUND(ISNULL(SUM(WorkHoursValue),0),2) AS SumWorkHoursValue,--عدد ساعات العمل المطلوبة بالشهر حسب الوردية
ROUND(ISNULL(SUM(VacationValue),0),2) AS SumVacationValue,
ROUND(ISNULL(SUM(MissionValue),0),2) AS SumMissionValue,
ROUND(ISNULL(SUM(ActualWorkHoursValue),0),2) AS SumActualWorkHoursValue,--عدد ساعات العمل المحققة بالشهر
ROUND(ISNULL(SUM(OvertimeOrderValue),0),2) AS SumOvertimeOrderValue,--عدد ساعات التكليف
--ISNULL(SUM(RequiredWorkHoursValue),0)AS SumRequiredWorkHoursValue,
ROUND(CASE WHEN ISNULL(SUM(RequiredWorkHoursValue),0) &gt; ISNULL(SUM(ActualWorkHoursValue),0) THEN ISNULL(SUM(RequiredWorkHoursValue),0) - ISNULL(SUM(ActualWorkHoursValue),0) ELSE 0 END,2) InitialNonAttendanceValue,--نقص الدوام
ROUND(CASE WHEN ISNULL(SUM(ActualWorkHoursValue),0) &gt; ISNULL(SUM(RequiredWorkHoursValue),0) THEN ISNULL(SUM(ActualWorkHoursValue),0) - ISNULL(SUM(RequiredWorkHoursValue),0) ELSE 0 END,2) ExpectedOvertimeValue--الاضافي الشهري المحتمل
from   MonthDetails 
group by 
AttendanceMonthlyAdjustmentId,
monthnumber,
MonthName,
MonthId,
EmployeeAttendanceCard_id,
Penalty,
InitialOvertimeValue,
FinalOvertimeValue,
FinalNonAttendanceValue</CommandText><rd:UseGenericDesigner xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">true</rd:UseGenericDesigner></Query><Fields><Field Name="EmployeeAttendanceCard_id"><DataField>EmployeeAttendanceCard_id</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="AttendanceMonthlyAdjustmentId"><DataField>AttendanceMonthlyAdjustmentId</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="FullName"><DataField>FullName</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="EmployeeId"><DataField>EmployeeId</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="MonthNumber"><DataField>MonthNumber</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="MonthName"><DataField>MonthName</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="MonthId"><DataField>MonthId</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="InitialOvertimeValue"><DataField>InitialOvertimeValue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Double</rd:TypeName></Field><Field Name="FinalOvertimeValue"><DataField>FinalOvertimeValue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Double</rd:TypeName></Field><Field Name="FinalNonAttendanceValue"><DataField>FinalNonAttendanceValue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Double</rd:TypeName></Field><Field Name="Penalty"><DataField>Penalty</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="SumRequiredHoursValue"><DataField>SumRequiredHoursValue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Double</rd:TypeName></Field><Field Name="SumRequiredWorkHoursValue"><DataField>SumRequiredWorkHoursValue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Double</rd:TypeName></Field><Field Name="SumWorkHoursValue"><DataField>SumWorkHoursValue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Double</rd:TypeName></Field><Field Name="SumVacationValue"><DataField>SumVacationValue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Double</rd:TypeName></Field><Field Name="SumMissionValue"><DataField>SumMissionValue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Double</rd:TypeName></Field><Field Name="SumActualWorkHoursValue"><DataField>SumActualWorkHoursValue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Double</rd:TypeName></Field><Field Name="SumOvertimeOrderValue"><DataField>SumOvertimeOrderValue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Double</rd:TypeName></Field><Field Name="InitialNonAttendanceValue"><DataField>InitialNonAttendanceValue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Double</rd:TypeName></Field><Field Name="ExpectedOvertimeValue"><DataField>ExpectedOvertimeValue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Double</rd:TypeName></Field></Fields><Filters><Filter><FilterExpression>=Fields!MonthId.Value</FilterExpression><Operator>Equal</Operator><FilterValues><FilterValue>=Parameters!mcid.Value</FilterValue></FilterValues></Filter><Filter><FilterExpression>=Fields!EmployeeId.Value</FilterExpression><Operator>Equal</Operator><FilterValues><FilterValue>=Parameters!empId.Value</FilterValue></FilterValues></Filter></Filters></DataSet></DataSets><ReportParameters><ReportParameter Name="mcid"><DataType>Integer</DataType><Prompt>ReportParameter1</Prompt><Hidden>true</Hidden></ReportParameter><ReportParameter Name="empId"><DataType>Integer</DataType><Prompt>ReportParameter1</Prompt></ReportParameter></ReportParameters><rd:ReportUnitType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">Cm</rd:ReportUnitType><rd:ReportID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">f4004064-847d-4c0b-9251-c6cb910f6fa6</rd:ReportID></Report>