9.7cm</PageHeight><PageWidth>37cm</PageWidth><LeftMargin>0.5cm</LeftMargin><RightMargin>0.5cm</RightMargin><TopMargin>2cm</TopMargin><BottomMargin>2cm</BottomMargin><ColumnSpacing>0.13cm</ColumnSpacing><Style /></Page><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="DataSource"><DataSourceReference>SharedDataSource</DataSourceReference><rd:SecurityType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">None</rd:SecurityType><rd:DataSourceID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">9a350a56-51d6-40e0-b0cf-07e98984a2ce</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="BasicInfoDataSet"><Query><DataSourceName>DataSource</DataSourceName><CommandText>with WithoutDetails AS
(
SELECT        
EmployeeAttendanceCard_id  AS EmpAttCardID,
FinalTotalOvertimeValue,
NormalOvertimeValue,
ParticularOvertimeValue,
HolidayOvertimeValue,
MONTH (dbo.AttendanceRecord.Date ) as monthnumber ,
dbo.AttendanceRecord.Id MonthId,
LatenessPenalty_id,
NonAttendancePenalty_id,
FinalLatenessTotalValue,
LatenessHoursValue,
NonAttendanceHoursValue,
FinalNonAttendanceTotalValue
FROM	dbo.AttendanceWithoutAdjustment 
INNER JOIN dbo.AttendanceWithoutAdjustmentDetail on dbo.AttendanceWithoutAdjustment.Id=dbo.AttendanceWithoutAdjustmentDetail.AttendanceWithoutAdjustment_id
INNER JOIN dbo.AttendanceRecord ON dbo.AttendanceRecord.Id = dbo.AttendanceWithoutAdjustment.AttendanceRecord_id
WHERE	AttendanceWithoutAdjustment.IsVertualDeleted = 0 AND
		AttendanceWithoutAdjustmentDetail.IsVertualDeleted = 0 AND
		AttendanceRecord.IsVertualDeleted = 0
 )
SELECT
EmpAttCardID,
(SELECT dbo.Employee.FirstName + ' ' + dbo.Employee.FatherName + ' ' + dbo.Employee.LastName AS FullName
 FROM	dbo.EmployeeCard INNER JOIN	dbo.Employee ON dbo.EmployeeCard.Employee_id = dbo.Employee.Id
 WHERE	EmployeeCard.IsVertualDeleted = 0 AND Employee.IsVertualDeleted = 0 AND dbo.EmployeeCard.Id = EmpAttCardID) FullName,
monthnumber,
MonthId,
ISNULL((select count(*) from  dbo.AttendanceWithoutAdjustment inner join dbo.AttendanceWithoutAdjustmentDetail on dbo.AttendanceWithoutAdjustment.Id=dbo.AttendanceWithoutAdjustmentDetail.AttendanceWithoutAdjustment_id 
where ActualWorkValue&lt;=0 AND IsOffDay=0 AND HasMission=0 AND HasVacation=0 AND IsHoliday=0
AND AttendanceWithoutAdjustment.EmployeeAttendanceCard_id=EmpAttCardID AND Month(AttendanceWithoutAdjustmentDetail.Date)=monthnumber),0)AS CountAbsence,
ROUND(FinalTotalOvertimeValue,2) FinalTotalOvertimeValue,
LatenessPenalty_id,
(SELECT N.Name FROM DisciplinarySetting N WHERE N.Id = LatenessPenalty_id) AS LatenessPenalty,
NonAttendancePenalty_id,
(SELECT N.Name FROM DisciplinarySetting N WHERE N.Id = NonAttendancePenalty_id) AS NonAttendancePenalty,
ROUND(FinalNonAttendanceTotalValue,2) FinalNonAttendanceTotalValue,
ROUND(FinalLatenessTotalValue,2) FinalLatenessTotalValue, 
ROUND(ISNULL(SUM(NormalOvertimeValue),0),2) AS TotalNormalOvertimeValue,
ROUND(ISNULL(SUM(ParticularOvertimeValue),0),2) AS TotalParticularOvertimeValue,
ROUND(ISNULL(SUM(HolidayOvertimeValue),0),2) AS TotalHolidayOvertimeValue,
ROUND(ISNULL(SUM(LatenessHoursValue),0),2) AS InitialLatenessTotalValue,
ROUND(ISNULL(SUM(NonAttendanceHoursValue),0),2) AS InitialNonAttendanceTotalValue
FROM   WithoutDetails 
GROUP BY 
monthnumber,
EmpAttCardID,
MonthId,
FinalTotalOvertimeValue,
LatenessPenalty_id,
NonAttendancePenalty_id,
FinalNonAttendanceTotalValue,
FinalLatenessTotalValue</CommandText></Query><Fields><Field Name="EmpAttCardID"><DataField>EmpAttCardID</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="FullName"><DataField>FullName</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="monthnumber"><DataField>monthnumber</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="CountAbsence"><DataField>CountAbsence</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="MonthId"><DataField>MonthId</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="FinalTotalOvertimeValue"><DataField>FinalTotalOvertimeValue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Double</rd:TypeName></Field><Field Name="LatenessPenalty_id"><DataField>LatenessPenalty_id</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="LatenessPenalty"><DataField>LatenessPenalty</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="NonAttendancePenalty_id"><DataField>NonAttendancePenalty_id</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="NonAttendancePenalty"><DataField>NonAttendancePenalty</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="FinalNonAttendanceTotalValue"><DataField>FinalNonAttendanceTotalValue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Double</rd:TypeName></Field><Field Name="FinalLatenessTotalValue"><DataField>FinalLatenessTotalValue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Double</rd:TypeName></Field><Field Name="TotalNormalOvertimeValue"><DataField>TotalNormalOvertimeValue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Double</rd:TypeName></Field><Field Name="TotalParticularOvertimeValue"><DataField>TotalParticularOvertimeValue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Double</rd:TypeName></Field><Field Name="TotalHolidayOvertimeValue"><DataField>TotalHolidayOvertimeValue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Double</rd:TypeName></Field><Field Name="InitialLatenessTotalValue"><DataField>InitialLatenessTotalValue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Double</rd:TypeName></Field><Field Name="InitialNonAttendanceTotalValue"><DataField>InitialNonAttendanceTotalValue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Double</rd:TypeName></Field></Fields><Filters><Filter><FilterExpression>=Fields!MonthId.Value</FilterExpression><Operator>In</Operator><FilterValues><FilterValue>=Parameters!MonthName.Value</FilterValue></FilterValues></Filter></Filters></DataSet><DataSet Name="MonthNameDataSet"><Query><DataSourceName>DataSource</DataSourceName><CommandText>SELECT DISTINCT Id, Name AS MonthName
FROM            AttendanceRecord
