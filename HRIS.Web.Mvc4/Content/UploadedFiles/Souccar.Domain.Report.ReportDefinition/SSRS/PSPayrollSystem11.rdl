tRun><Value>= "Page "&amp; Globals!PageNumber &amp; " of " &amp; Globals!TotalPages</Value><Style /></TextRun></TextRuns><Style><TextAlign>Right</TextAlign></Style></Paragraph></Paragraphs><rd:DefaultName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">TotalPages</rd:DefaultName><Top>0.36195cm</Top><Left>21.97805cm</Left><Height>0.6cm</Height><Width>7.66817cm</Width><Style><Border><Style>None</Style></Border><PaddingLeft>2pt</PaddingLeft><PaddingRight>2pt</PaddingRight><PaddingTop>2pt</PaddingTop><PaddingBottom>2pt</PaddingBottom><Direction>RTL</Direction></Style></Textbox></ReportItems><Style><Border><Style>None</Style></Border></Style></PageFooter><PageHeight>32cm</PageHeight><PageWidth>32cm</PageWidth><LeftMargin>0.5cm</LeftMargin><RightMargin>0.5cm</RightMargin><TopMargin>2cm</TopMargin><BottomMargin>2cm</BottomMargin><ColumnSpacing>0.13cm</ColumnSpacing><Style /></Page><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="DataSource1"><DataSourceReference>SharedDataSource</DataSourceReference><rd:SecurityType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">None</rd:SecurityType><rd:DataSourceID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">d5a02870-23e4-4bd7-9a48-107238f7bd9a</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="BasicInfoDataSet"><Query><DataSourceName>DataSource1</DataSourceName><CommandText>SELECT   
MonthlyCard.Id AS MonthlyCardId, 
Employee.Code AS EmployeeCode, 
Employee.FirstName + ' ' +Employee.FatherName +' '+ Employee.LastName AS FullName, 
Month.Name AS MonthName, 
Month.Date AS MonthDate,  
Month.MonthNumber AS MonthNumber,
CASE Month.MonthType WHEN 0 THEN 'اجور و تعويضات' WHEN 1 THEN 'تعويضات فقط' END AS MonthType,
MonthlyCard.Salary AS BasicSalary, 
Employee.Id AS EmployeeId,
Node.Name AS NodeName,

--مجموع تعويضات الموظفين
(Select 
	ISNULL(SUM(MonthlyEmployeeBenefit.InitialValue), 0) 
From 
	MonthlyEmployeeBenefit 
Where 
	MonthlyEmployeeBenefit.MonthlyCard_id = MonthlyCard.ID  AND dbo.MonthlyEmployeeBenefit.IsVertualDeleted=0) As TotalEmployeeBenefits,
--------

--مجموع حسميات القروض للموظفين

((Select 
	ISNULL(SUM(MonthlyEmployeeDeduction.FinalValue), 0) 
From 
	MonthlyEmployeeDeduction 
Where 
	MonthlyEmployeeDeduction.MonthlyCard_id = MonthlyCard.ID  AND dbo.MonthlyEmployeeDeduction.IsVertualDeleted=0)+
	(Select 
	ISNULL(SUM(LoanPayment.PaymentValue), 0) 
From 
	LoanPayment 
Where 
	LoanPayment.MonthlyCard_id = MonthlyCard.ID  AND dbo.LoanPayment.IsVertualDeleted=0) )
	
	 As TotalEmployeeLoansDeductions,
---------

--مجموع الحسميات للموظفين
(Select 
	ISNULL(SUM(MonthlyEmployeeDeduction.FinalValue), 0) 
From 
	MonthlyEmployeeDeduction 
Where 
	MonthlyEmployeeDeduction.MonthlyCard_id = MonthlyCard.ID  AND dbo.MonthlyEmployeeDeduction.IsVertualDeleted=0)

As TotalEmployeeDeductions,
--------------
--مجموع المستحق للموظفين
((MonthlyCard.Salary)-(Select 
	ISNULL(SUM(MED.InitialValue), 0) 
From 
dbo.DeductionCard AS DC
 INNER JOIN
dbo.MonthlyEmployeeDeduction MED ON DC.Id = MED.DeductionCard_id
Where 
	MED.MonthlyCard_id = MonthlyCard.ID  AND MED.IsVertualDeleted = 0)
	+
	(Select 
	ISNULL(SUM(MonthlyEmployeeBenefit.CrossDependencyInitialValue), 0) 
From 
	MonthlyEmployeeBenefit 
Where 
	MonthlyEmployeeBenefit.MonthlyCard_id = MonthlyCard.ID  AND dbo.MonthlyEmployeeBenefit.IsVertualDeleted=0)
	) As TotalEmployeeDeserved,
--------------

--الراتب الصافي للموظف
(Select 
	ISNULL(SUM(LoanPayment.PaymentValue), 0) 
From 
	LoanPayment 
Where 
	LoanPayment.MonthlyCard_id = MonthlyCard.ID  AND dbo.LoanPayment.IsVertualDeleted=0) As TotalEmployeeLoans,
CASE Month.MonthType WHEN 0 THEN MonthlyCard.Salary +
(SELECT     ISNULL(SUM(MonthlyEmployeeBenefit.InitialValue), 0)
 FROM         MonthlyEmployeeBenefit
