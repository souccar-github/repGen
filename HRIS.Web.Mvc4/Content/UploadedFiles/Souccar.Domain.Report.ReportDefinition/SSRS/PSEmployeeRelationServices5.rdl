ingRight><PaddingTop>2pt</PaddingTop><PaddingBottom>2pt</PaddingBottom><Direction>RTL</Direction></Style></Textbox></ReportItems><Style><Border><Style>None</Style></Border></Style></PageFooter><PageHeight>29.7cm</PageHeight><PageWidth>37cm</PageWidth><LeftMargin>0.5cm</LeftMargin><RightMargin>0.5cm</RightMargin><TopMargin>2cm</TopMargin><BottomMargin>2cm</BottomMargin><ColumnSpacing>0.13cm</ColumnSpacing><Style /></Page><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="DataSource"><DataSourceReference>SharedDataSource</DataSourceReference><rd:SecurityType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">None</rd:SecurityType><rd:DataSourceID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">9a350a56-51d6-40e0-b0cf-07e98984a2ce</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="BasicInfoDataSet"><Query><DataSourceName>DataSource</DataSourceName><CommandText>with temp as(
---------------------------------
SELECT     
EmployeePromotion.EmployeeCard_id as EmployeeCardId,
(SELECT EC.Employee_id FROM dbo.EmployeeCard EC where EC.Id = EmployeePromotion.EmployeeCard_id AND EC.IsVertualDeleted=0) as EmployeeId,
(SELECT P.Code FROM dbo.Position P WHERE p.IsVertualDeleted=0 AND p.Id=EmployeePromotion.Position_id) as PositionCode,
(SELECT	J.Name
 FROM	dbo.Position P INNER JOIN JobDescription J ON P.JobDescription_id = J.Id
 WHERE p.IsVertualDeleted = 0 AND J.IsVertualDeleted = 0 AND p.Id=EmployeePromotion.Position_id) as JobDescriptionName,
(SELECT	JT.Name
 FROM	dbo.Position P	INNER JOIN JobDescription JD ON P.JobDescription_id = JD.Id
						INNER JOIN JobTitle JT ON JD.JobTitle_id = JT.Id
 WHERE p.IsVertualDeleted = 0 AND JD.IsVertualDeleted = 0 AND JT.IsVertualDeleted = 0 AND p.Id=EmployeePromotion.Position_id) as JobTitleName,
(SELECT	G.Name
 FROM	dbo.Position P	INNER JOIN JobDescription JD ON P.JobDescription_id = JD.Id
						INNER JOIN JobTitle JT ON JD.JobTitle_id = JT.Id
						INNER JOIN Grade G ON JT.Grade_id = G.Id
 WHERE p.IsVertualDeleted = 0 AND JD.IsVertualDeleted = 0 AND JT.IsVertualDeleted = 0 AND G.IsVertualDeleted = 0 AND p.Id=EmployeePromotion.Position_id) as GradeName,
PositionJoiningDate as StartingDate, 
PromotionReason as Reason, 
Comment,
CreationDate
FROM
dbo.EmployeePromotion
WHERE  EmployeePromotion.IsVertualDeleted=0
---------------------------------
UNION
---------------------------------
SELECT     
EmployeeTransfer.EmployeeCard_id as EmployeeCardId,
(SELECT EC.Employee_id FROM dbo.EmployeeCard EC where EC.Id = EmployeeTransfer.EmployeeCard_id AND EC.IsVertualDeleted=0) as EmployeeId,
(SELECT P.Code FROM dbo.Position P WHERE p.IsVertualDeleted=0 AND p.Id=EmployeeTransfer.DestinationPosition_id) as PositionCode,
(SELECT	J.Name
 FROM	dbo.Position P INNER JOIN JobDescription J ON P.JobDescription_id = J.Id
 WHERE p.IsVertualDeleted = 0 AND J.IsVertualDeleted = 0 AND p.Id=EmployeeTransfer.DestinationPosition_id) as JobDescriptionName,
(SELECT	JT.Name
 FROM	dbo.Position P	INNER JOIN JobDescription JD ON P.JobDescription_id = JD.Id
						INNER JOIN JobTitle JT ON JD.JobTitle_id = JT.Id
 WHERE p.IsVertualDeleted = 0 AND JD.IsVertualDeleted = 0 AND JT.IsVertualDeleted = 0 AND p.Id=EmployeeTransfer.DestinationPosition_id) as JobTitleName,
(SELECT	G.Name
 FROM	dbo.Position P	INNER JOIN JobDescription JD ON P.JobDescription_id = JD.Id
						INNER JOIN JobTitle JT ON JD.JobTitle_id = JT.Id
						INNER JOIN Grade G ON JT.Grade_id = G.Id
 WHERE p.IsVertualDeleted = 0 AND JD.IsVertualDeleted = 0 AND JT.IsVertualDeleted = 0 AND G.IsVertualDeleted = 0 AND p.Id=EmployeeTransfer.DestinationPosition_id) as GradeName,
StartingDate as StartingDate, 
TransferReason as Reason, 
Comment,
CreationDate
FROM      
dbo.EmployeeTransfer
WHERE  EmployeeTransfer.IsVertualDeleted=0
---------------------------------
)
SELECT * FROM temp ORDER BY StartingDate,CreationDate;</CommandText></Query><Fields><Field Name="EmployeeId"><DataField>EmployeeId</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="PositionCode"><DataField>PositionCode</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="EmployeeCardId"><DataField>EmployeeCardId</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="JobDescriptionName"><DataField>JobDescriptionName</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="JobTitleName"><DataField>JobTitleName</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="GradeName"><DataField>GradeName</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="CreationDate"><DataField>CreationDate</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.DateTime</rd:TypeName></Field><Field Name="StartingDate"><DataField>StartingDate</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.DateTime</rd:TypeName></Field><Field Name="Reason"><DataField>Reason</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Comment"><DataField>Comment</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field></Fields><Filters><Filter><FilterExpression>=Fields!EmployeeId.Value</FilterExpression><Operator>In</Operator><FilterValues><FilterValue>=Parameters!Employee.Value</FilterValue></FilterValues></Filter></Filters></DataSet><DataSet Name="EmployeeDataSet"><Query><DataSourceName>DataSource</DataSourceName><CommandText>SELECT        Id, IsVertualDeleted, FirstName + ' ' + FatherName + ' ' + LastName AS FullName, FirstName, LastName, FatherName, MotherName, FirstNameL2, LastNameL2, 
                         FatherNameL2, MotherNameL2, PlaceOfBirthL2, Gender, MaritalStatus, DateOfBirth, CivilRecordPlaceAndNumber, PersonalRecordSource, NationalityIssue, 
                         OtherNationalityIssue, BloodType, Religion, MilitaryStatus, Status, Address, Phone, Mobile, Email, POBox, WebSite, Twitter, Facebook, IdentificationNo, Code, 
                         IsRetired, PhotoId, EmploymentStatus, SalaryStatus, DisabilityExist, PlaceOfBirth_id, CountryOfBirth_id, Nationality_id, OtherNationality_id, DisabilityType_id, 
                         RecruitmentInformation_id
