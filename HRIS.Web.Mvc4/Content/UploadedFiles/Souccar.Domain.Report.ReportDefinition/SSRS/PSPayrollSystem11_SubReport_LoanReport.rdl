LoanPayment.MonthlyCard_id MonthlyCardId,
DonorLoan.Name DonorLoanName,
LoanPayment.PaymentValue,
dbo.EmployeeLoan.TotalAmountOfLoan - (dbo.EmployeeLoan.PrePayed + (SELECT ISNULL(SUM(PaymentValue), 0) AS Expr1 FROM dbo.LoanPayment AS N WHERE (EmployeeLoan_id = dbo.EmployeeLoan.Id) AND (IsVertualDeleted = 0))) AS RemainingValue
From
MonthlyCard Inner Join
LoanPayment On(LoanPayment.MonthlyCard_id = MonthlyCard.Id AND LoanPayment.IsExternalPayment = 0) Inner Join
EmployeeLoan On(EmployeeLoan.Id = LoanPayment.EmployeeLoan_id)Inner Join
DonorLoan On(DonorLoan.Id = EmployeeLoan.DonorLoan_id)
where 
(dbo.LoanPayment.IsVertualDeleted=0) AND
(dbo.MonthlyCard.IsVertualDeleted=0) AND
(dbo.EmployeeLoan.IsVertualDeleted=0) AND
(dbo.DonorLoan.IsVertualDeleted=0) </CommandText><rd:UseGenericDesigner xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">true</rd:UseGenericDesigner></Query><Fields><Field Name="MonthlyCardId"><DataField>MonthlyCardId</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="DonorLoanName"><DataField>DonorLoanName</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="PaymentValue"><DataField>PaymentValue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Double</rd:TypeName></Field><Field Name="RemainingValue"><DataField>RemainingValue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Double</rd:TypeName></Field></Fields><Filters><Filter><FilterExpression>=Fields!MonthlyCardId.Value</FilterExpression><Operator>Equal</Operator><FilterValues><FilterValue>=Parameters!mcid.Value</FilterValue></FilterValues></Filter></Filters></DataSet></DataSets><ReportParameters><ReportParameter Name="mcid"><DataType>Integer</DataType><Prompt>ReportParameter1</Prompt><Hidden>true</Hidden></ReportParameter></ReportParameters><rd:ReportUnitType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">Cm</rd:ReportUnitType><rd:ReportID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">8151fb56-2765-47ca-96f7-97c57fc19434</rd:ReportID></Report>