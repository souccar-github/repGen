TablixMember /><TablixMember /><TablixMember /></TablixMembers></TablixColumnHierarchy><TablixRowHierarchy><TablixMembers><TablixMember><KeepWithGroup>After</KeepWithGroup></TablixMember><TablixMember><Group Name="Details2" /><TablixMembers><TablixMember /><TablixMember /><TablixMember /></TablixMembers></TablixMember><TablixMember><KeepWithGroup>Before</KeepWithGroup></TablixMember></TablixMembers></TablixRowHierarchy><DataSetName>MonthlyEmployeeBenefiDataSet</DataSetName><Top>0cm</Top><Left>0.02646cm</Left><Height>3.9525cm</Height><Width>42.18214cm</Width><Style><Border><Style>None</Style></Border></Style></Tablix></ReportItems><Height>1.5561in</Height><Style /></Body><Width>16.61758in</Width><Page><PageHeight>29.7cm</PageHeight><PageWidth>21cm</PageWidth><LeftMargin>2cm</LeftMargin><RightMargin>2cm</RightMargin><TopMargin>2cm</TopMargin><BottomMargin>2cm</BottomMargin><ColumnSpacing>0.13cm</ColumnSpacing><Style /></Page><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="DataSource1"><DataSourceReference>SharedDataSource</DataSourceReference><rd:SecurityType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">None</rd:SecurityType><rd:DataSourceID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">eab1e084-b509-454d-8fe6-97d24f65d6d8</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="MonthlyEmployeeBenefiDataSet"><Query><DataSourceName>DataSource1</DataSourceName><CommandText>with WithoutDetails AS
(
SELECT        
EmployeeAttendanceCard_id  AS EmpAttCardID,
AttendanceWithoutAdjustment_id,
MONTH (dbo.AttendanceRecord.Date ) as monthnumber ,
FinalTotalOvertimeValue,
NormalOvertimeValue,
ParticularOvertimeValue,
HolidayOvertimeValue,
dbo.AttendanceRecord.Id MonthId,
LatenessPenalty_id,
NonAttendancePenalty_id,
FinalLatenessTotalValue,
LatenessHoursValue,
NonAttendanceHoursValue,
FinalNonAttendanceTotalValue

FROM
dbo.AttendanceWithoutAdjustment 
inner join dbo.AttendanceWithoutAdjustmentDetail ON dbo.AttendanceWithoutAdjustment.Id=dbo.AttendanceWithoutAdjustmentDetail.AttendanceWithoutAdjustment_id
INNER JOIN dbo.AttendanceRecord ON dbo.AttendanceRecord.Id = dbo.AttendanceWithoutAdjustment.AttendanceRecord_id
WHERE	AttendanceWithoutAdjustment.IsVertualDeleted = 0 AND
		AttendanceWithoutAdjustmentDetail.IsVertualDeleted = 0 AND
		AttendanceRecord.IsVertualDeleted = 0
 )

SELECT
EmpAttCardID,
AttendanceWithoutAdjustment_id,
(SELECT dbo.Employee.FirstName + ' ' + dbo.Employee.FatherName + ' ' + dbo.Employee.LastName AS FullName
 FROM	dbo.EmployeeCard INNER JOIN	dbo.Employee ON dbo.EmployeeCard.Employee_id = dbo.Employee.Id
 WHERE	EmployeeCard.IsVertualDeleted = 0 AND Employee.IsVertualDeleted = 0 AND dbo.EmployeeCard.Id = EmpAttCardID) FullName,
(SELECT	dbo.Employee.Id AS EmployeeId
 FROM	dbo.EmployeeCard INNER JOIN	dbo.Employee ON dbo.EmployeeCard.Employee_id = dbo.Employee.Id
 WHERE	EmployeeCard.IsVertualDeleted = 0 AND Employee.IsVertualDeleted = 0 AND dbo.EmployeeCard.Id = EmpAttCardID) EmployeeId,
monthnumber,
MonthId,
ISNULL((SELECT COUNT(*) FROM  dbo.AttendanceWithoutAdjustment 
INNER JOIN dbo.AttendanceWithoutAdjustmentDetail on dbo.AttendanceWithoutAdjustment.Id=dbo.AttendanceWithoutAdjustmentDetail.AttendanceWithoutAdjustment_id 
WHERE ActualWorkValue&lt;=0 AND IsOffDay=0 AND HasMission=0 AND HasVacation=0 AND IsHoliday=0
AND AttendanceWithoutAdjustment.EmployeeAttendanceCard_id=EmpAttCardID AND Month(AttendanceWithoutAdjustmentDetail.Date)=monthnumber),0)AS CountAbsence,
ROUND(FinalTotalOvertimeValue,2) FinalTotalOvertimeValue,
LatenessPenalty_id,
(SELECT N.Name FROM DisciplinarySetting N WHERE N.Id = LatenessPenalty_id) AS LatenessPenalty,
NonAttendancePenalty_id,
(SELECT N.Name FROM DisciplinarySetting N WHERE N.Id = NonAttendancePenalty_id) AS NonAttendancePenalty,
ROUND(FinalNonAttendanceTotalValue,2) FinalNonAttendanceTotalValue,
ROUND(FinalLatenessTotalValue,2) FinalLatenessTotalValue, 
ROUND(ISNULL(SUM(NormalOvertimeValue),0),2) AS TotalNormalOvertimeValue,
ROUND(ISNULL(SUM(ParticularOvertimeValue),0),2) AS TotalParticularOvertimeValue,
ROUND(ISNULL(SUM(HolidayOvertimeValue),0),2) AS TotalHolidayOvertimeValue,
ROUND(ISNULL(SUM(LatenessHoursValue),0),2) AS InitialLatenessTotalValue,
ROUND(ISNULL(SUM(NonAttendanceHoursValue),0),2) AS InitialNonAttendanceTotalValue
FROM   WithoutDetails 
GROUP BY 
monthnumber,
EmpAttCardID,
AttendanceWithoutAdjustment_id,
MonthId,
FinalTotalOvertimeValue,
LatenessPenalty_id,
NonAttendancePenalty_id,
FinalNonAttendanceTotalValue,
FinalLatenessTotalValue</CommandText><rd:UseGenericDesigner xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">true</rd:UseGenericDesigner></Query><Fields><Field Name="EmpAttCardID"><DataField>EmpAttCardID</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="AttendanceWithoutAdjustment_id"><DataField>AttendanceWithoutAdjustment_id</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="FullName"><DataField>FullName</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="EmployeeId"><DataField>EmployeeId</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="monthnumber"><DataField>monthnumber</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="MonthId"><DataField>MonthId</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="CountAbsence"><DataField>CountAbsence</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="FinalTotalOvertimeValue"><DataField>FinalTotalOvertimeValue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Double</rd:TypeName></Field><Field Name="LatenessPenalty_id"><DataField>LatenessPenalty_id</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="LatenessPenalty"><DataField>LatenessPenalty</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="NonAttendancePenalty_id"><DataField>NonAttendancePenalty_id</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="NonAttendancePenalty"><DataField>NonAttendancePenalty</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="FinalNonAttendanceTotalValue"><DataField>FinalNonAttendanceTotalValue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Double</rd:TypeName></Field><Field Name="FinalLatenessTotalValue"><DataField>FinalLatenessTotalValue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Double</rd:TypeName></Field><Field Name="TotalNormalOvertimeValue"><DataField>TotalNormalOvertimeValue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Double</rd:TypeName></Field><Field Name="TotalParticularOvertimeValue"><DataField>TotalParticularOvertimeValue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Double</rd:TypeName></Field><Field Name="TotalHolidayOvertimeValue"><DataField>TotalHolidayOvertimeValue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Double</rd:TypeName></Field><Field Name="InitialLatenessTotalValue"><DataField>InitialLatenessTotalValue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Double</rd:TypeName></Field><Field Name="InitialNonAttendanceTotalValue"><DataField>InitialNonAttendanceTotalValue</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Double</rd:TypeName></Field></Fields><Filters><Filter><FilterExpression>=Fields!MonthId.Value</FilterExpression><Operator>Equal</Operator><FilterValues><FilterValue>=Parameters!mcid.Value</FilterValue></FilterValues></Filter><Filter><FilterExpression>=Fields!EmployeeId.Value</FilterExpression><Operator>Equal</Operator><FilterValues><FilterValue>=Parameters!empId.Value</FilterValue></FilterValues></Filter></Filters></DataSet></DataSets><ReportParameters><ReportParameter Name="mcid"><DataType>Integer</DataType><Prompt>ReportParameter1</Prompt><Hidden>true</Hidden></ReportParameter><ReportParameter Name="empId"><DataType>Integer</DataType><Prompt>ReportParameter1</Prompt></ReportParameter></ReportParameters><rd:ReportUnitType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">Cm</rd:ReportUnitType><rd:ReportID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">f4004064-847d-4c0b-9251-c6cb910f6fa6</rd:ReportID></Report>