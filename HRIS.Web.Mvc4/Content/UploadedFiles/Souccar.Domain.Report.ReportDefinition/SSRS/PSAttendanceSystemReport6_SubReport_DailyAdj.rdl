faultName><Style><Border><Color>LightGrey</Color><Style>Solid</Style></Border><PaddingLeft>2pt</PaddingLeft><PaddingRight>2pt</PaddingRight><PaddingTop>2pt</PaddingTop><PaddingBottom>2pt</PaddingBottom><Direction>RTL</Direction></Style></Textbox></CellContents></TablixCell><TablixCell><CellContents><Textbox Name="Textbox15"><CanGrow>true</CanGrow><KeepTogether>true</KeepTogether><Paragraphs><Paragraph><TextRuns><TextRun><Value>عدد البنود</Value><Style /></TextRun></TextRuns><Style><TextAlign>Right</TextAlign></Style></Paragraph></Paragraphs><rd:DefaultName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">Textbox14</rd:DefaultName><Style><Border><Color>LightGrey</Color><Style>Solid</Style></Border><BackgroundColor>LightGrey</BackgroundColor><PaddingLeft>2pt</PaddingLeft><PaddingRight>2pt</PaddingRight><PaddingTop>2pt</PaddingTop><PaddingBottom>2pt</PaddingBottom><Direction>RTL</Direction></Style></Textbox></CellContents></TablixCell></TablixCells></TablixRow></TablixRows></TablixBody><TablixColumnHierarchy><TablixMembers><TablixMember /><TablixMember /><TablixMember /><TablixMember /><TablixMember /><TablixMember /><TablixMember /></TablixMembers></TablixColumnHierarchy><TablixRowHierarchy><TablixMembers><TablixMember><KeepWithGroup>After</KeepWithGroup></TablixMember><TablixMember><Group Name="Details2" /><TablixMembers><TablixMember /><TablixMember /><TablixMember /></TablixMembers></TablixMember><TablixMember><KeepWithGroup>Before</KeepWithGroup></TablixMember></TablixMembers></TablixRowHierarchy><DataSetName>MonthlyEmployeeBenefiDataSet</DataSetName><Top>0cm</Top><Left>0cm</Left><Height>3.9525cm</Height><Width>42.10276cm</Width><Style><Border><Style>None</Style></Border></Style></Tablix></ReportItems><Height>1.5561in</Height><Style /></Body><Width>16.5759in</Width><Page><PageHeight>29.7cm</PageHeight><PageWidth>21cm</PageWidth><LeftMargin>2cm</LeftMargin><RightMargin>2cm</RightMargin><TopMargin>2cm</TopMargin><BottomMargin>2cm</BottomMargin><ColumnSpacing>0.13cm</ColumnSpacing><Style /></Page><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="DataSource1"><DataSourceReference>SharedDataSource</DataSourceReference><rd:SecurityType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">None</rd:SecurityType><rd:DataSourceID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">eab1e084-b509-454d-8fe6-97d24f65d6d8</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="MonthlyEmployeeBenefiDataSet"><Query><DataSourceName>DataSource1</DataSourceName><CommandText>WITH DailyDetails AS 
(
SELECT	AttendanceDailyAdjustment.EmployeeAttendanceCard_id AS EmpAttCardID, 
		AttendanceDailyAdjustmentDetail.AttendanceDailyAdjustment_id,
		AttendanceDailyAdjustmentDetail.HasVacation, 
		AttendanceDailyAdjustmentDetail.HasMission, 
		AttendanceDailyAdjustmentDetail.IsWorkDay, 
		AttendanceDailyAdjustmentDetail.ActualWorkHoursValue, 
		AttendanceDailyAdjustmentDetail.HolidayOvertimeValue, 
		AttendanceDailyAdjustmentDetail.NormalOvertimeValue, 
		AttendanceDailyAdjustment.FinalOvertimeValue, 
		AttendanceDailyAdjustment.FinalNonAttendanceValue, 
		MONTH(AttendanceRecord.Date) AS monthnumber, 
		AttendanceRecord.Id AS MonthId, 
		AttendanceDailyAdjustmentDetail.WorkHoursValue - AttendanceDailyAdjustmentDetail.VacationValue - AttendanceDailyAdjustmentDetail.MissionValue
		AS RequiredWorkHoursValue, 
		AttendanceDailyAdjustment.Penalty_id
FROM	AttendanceDailyAdjustment 
		INNER JOIN AttendanceDailyAdjustmentDetail ON AttendanceDailyAdjustment.Id = AttendanceDailyAdjustmentDetail.AttendanceDailyAdjustment_id 
		INNER JOIN AttendanceRecord ON AttendanceRecord.Id = AttendanceDailyAdjustment.AttendanceRecord_id
WHERE	(AttendanceDailyAdjustment.IsVertualDeleted = 0) AND 
		(AttendanceDailyAdjustmentDetail.IsVertualDeleted = 0) AND 
		(AttendanceRecord.IsVertualDeleted = 0)
)
SELECT        
ISNULL((SELECT COUNT(*) AS Expr1 
		FROM AttendanceDailyAdjustment AS AttendanceDailyAdjustment_2 
		INNER JOIN AttendanceDailyAdjustmentDetail AS AttendanceDailyAdjustmentDetail_2 
		ON AttendanceDailyAdjustment_2.Id = AttendanceDailyAdjustmentDetail_2.AttendanceDailyAdjustment_id
		WHERE	(AttendanceDailyAdjustment_2.IsVertualDeleted = 0) AND 
				(AttendanceDailyAdjustmentDetail_2.IsVertualDeleted = 0) AND 
				(AttendanceDailyAdjustmentDetail_2.ActualWorkHoursValue = 0) AND 
				(AttendanceDailyAdjustmentDetail_2.IsWorkDay = 1) AND 
				(AttendanceDailyAdjustmentDetail_2.HasMission = 0) AND 
				(AttendanceDailyAdjustmentDetail_2.HasVacation = 0) AND 
				(AttendanceDailyAdjustment_2.EmployeeAttendanceCard_id = DailyDetails_1.EmpAttCardID) AND 
				(MONTH(AttendanceDailyAdjustmentDetail_2.Date) = DailyDetails_1.monthnumber)), 0) AS CountAbsence, 
ROUND(ISNULL((SELECT SUM(AttendanceDailyAdjustmentDetail_1.WorkHoursValue - AttendanceDailyAdjustmentDetail_1.VacationValue - AttendanceDailyAdjustmentDetail_1.MissionValue - AttendanceDailyAdjustmentDetail_1.ActualWorkHoursValue) AS Expr1
		FROM AttendanceDailyAdjustment AS AttendanceDailyAdjustment_1 
		INNER JOIN AttendanceDailyAdjustmentDetail AS AttendanceDailyAdjustmentDetail_1 
		ON AttendanceDailyAdjustment_1.Id = AttendanceDailyAdjustmentDetail_1.AttendanceDailyAdjustment_id
		WHERE	(AttendanceDailyAdjustment_1.IsVertualDeleted = 0) AND 
				(AttendanceDailyAdjustmentDetail_1.IsVertualDeleted = 0) AND 
				(AttendanceDailyAdjustmentDetail_1.WorkHoursValue - AttendanceDailyAdjustmentDetail_1.VacationValue - AttendanceDailyAdjustmentDetail_1.MissionValue - AttendanceDailyAdjustmentDetail_1.ActualWorkHoursValue &gt; 0) AND 
				(AttendanceDailyAdjustment_1.EmployeeAttendanceCard_id = DailyDetails_1.EmpAttCardID) AND 
				(MONTH(AttendanceDailyAdjustmentDetail_1.Date) = DailyDetails_1.monthnumber)), 0),2) AS InitialNonAttendanceValue, 
				MonthId, 
				EmpAttCardID,
				AttendanceDailyAdjustment_id,
		(SELECT	Employee.FirstName + ' ' + Employee.FatherName + ' ' + Employee.LastName AS FullName
		 FROM EmployeeCard INNER JOIN Employee ON EmployeeCard.Employee_id = Employee.Id
		 WHERE (EmployeeCard.IsVertualDeleted = 0) AND 
		 (Employee.IsVertualDeleted = 0) AND 
		 (EmployeeCard.Id = DailyDetails_1.EmpAttCardID)) AS FullName, 
		(SELECT dbo.Employee.Id AS EmployeeId
		 FROM dbo.EmployeeCard INNER JOIN dbo.Employee ON dbo.EmployeeCard.Employee_id = dbo.Employee.Id
		 WHERE EmployeeCard.IsVertualDeleted = 0 AND Employee.IsVertualDeleted = 0 AND dbo.EmployeeCard.Id = EmpAttCardID) EmployeeId,
		monthnumber, 
		ROUND(FinalOvertimeValue,2) FinalOvertimeValue, 
		ROUND(FinalNonAttendanceValue,2) FinalNonAttendanceValue, 
		Penalty_id,
		(SELECT Name
		 FROM DisciplinarySetting AS N
		 WHERE (Id = DailyDetails_1.Penalty_id)) AS Penalty, 
		 ROUND(ISNULL(SUM(HolidayOvertimeValue), 0),2) AS TotalHolidayOvertimeValue, 
		 ROUND(ISNULL(SUM(NormalOvertimeValue), 0),2) AS TotalNormalOvertimeValue
		FROM DailyDetails AS DailyDetails_1
GROUP BY	monthnumber, 
			EmpAttCardID, 
			AttendanceDailyAdjustment_id,
			MonthId, 
			Penalty_id, 
