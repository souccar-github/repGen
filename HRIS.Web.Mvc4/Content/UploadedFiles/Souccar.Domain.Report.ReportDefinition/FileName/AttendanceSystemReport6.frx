      double TotalNormalOvertimeValue=0;
      double FinalTotalOvertimeValue=0;
      while(Without.HasMoreRows)
      {
        int EmpattId = int.Parse( Report.GetColumnValue(&quot;WithoutAdj.EmpCardID&quot;).ToString());
        DateTime withoutdate=(DateTime)Report.GetColumnValue(&quot;WithoutAdj.Date&quot;);
        
        if(  EmpattId==EmpAttCardId)
        {
          if(withoutdate.Year== DateTimePicker1.Value.Year&amp;&amp; withoutdate.Month==DateTimePicker1.Value.Month)
          {   
            flag=true;
            if (int.Parse( Report.GetColumnValue(&quot;WithoutAdj.ActualWorkValue&quot;).ToString()) &lt;= 0 &amp;&amp; !bool.Parse(Report.GetColumnValue(&quot;WithoutAdj.HasMission&quot;).ToString())  &amp;&amp;!bool.Parse(Report.GetColumnValue(&quot;WithoutAdj.HasVacation&quot;).ToString())  &amp;&amp;!bool.Parse(Report.GetColumnValue(&quot;WithoutAdj.IsOffDay&quot;).ToString()) )
            {
              CountAbsence++;
            }
            FinalTotalOvertimeValue=double.Parse( Report.GetColumnValue(&quot;WithoutAdj.FinalTotalOvertimeValue&quot;).ToString());
            TotalNormalOvertimeValue+=double.Parse( Report.GetColumnValue(&quot;WithoutAdj.NormalOvertimeValue&quot;).ToString());
            TotalParticularOvertimeValue+=  double.Parse( Report.GetColumnValue(&quot;WithoutAdj.ParticularOvertimeValue&quot;).ToString());
            TotalHolidayOvertimeValue+=double.Parse( Report.GetColumnValue(&quot;WithoutAdj.HolidayOvertimeValue&quot;).ToString());
            LatenessPenalty=int.Parse(Report.GetColumnValue(&quot;WithoutAdj.LatenessPenalty&quot;).ToString());
            NonAttendancePenalty=int.Parse(Report.GetColumnValue(&quot;WithoutAdj.NonAttendancePenalty&quot;).ToString());
            FinalLatenessTotalValue= double.Parse( Report.GetColumnValue(&quot;WithoutAdj.FinalLatenessTotalValue&quot;).ToString());
            InitialLatenessTotalValue+=  double.Parse( Report.GetColumnValue(&quot;WithoutAdj.LatenessHoursValue&quot;).ToString());
            InitialNonAttendanceTotalValue+= double.Parse( Report.GetColumnValue(&quot;WithoutAdj.NonAttendanceHoursValue&quot;).ToString());
            FinalNonAttendanceTotalValue= double.Parse( Report.GetColumnValue(&quot;WithoutAdj.FinalNonAttendanceTotalValue&quot;).ToString());
          }
        }
        Without.Next();
      }
      if(flag)
      {
          CountAbsenceWithoutText.Text=CountAbsence.ToString();
        FinalLatenessTotalValueText.Text=FinalLatenessTotalValue.ToString();
        InitialLatenessTotalValueWithoutText.Text=InitialLatenessTotalValue.ToString();
        InitialNonAttendanceTotalValueWithouText.Text=InitialNonAttendanceTotalValue.ToString();
        FinalNonAttendanceTotalValueWithoutText.Text=FinalNonAttendanceTotalValue.ToString();
        TotalHolidayOvertimeValueWithoutText.Text=TotalHolidayOvertimeValue.ToString();
        TotalParticularOvertimeValueWithoutText.Text=TotalParticularOvertimeValue.ToString();
        TotalNormalOvertimeValueWithoutText.Text=TotalNormalOvertimeValue.ToString();
        FinalTotalOvertimeValueWithoutText.Text=FinalTotalOvertimeValue.ToString();
        if(LatenessPenalty==0)
          LatenessPenaltyText.Text=&quot;None&quot;;
        if(LatenessPenalty==1)
          LatenessPenaltyText.Text=&quot;Alert&quot;;
        if(LatenessPenalty==2)
          LatenessPenaltyText.Text=&quot;Warning&quot;;
        if(LatenessPenalty==3)
          LatenessPenaltyText.Text=&quot;LayOff&quot;;
        if(NonAttendancePenalty==0)
        NonAttendancePenaltyText.Text=&quot;None&quot;;
        if(NonAttendancePenalty==1)
          NonAttendancePenaltyText.Text=&quot;Alert&quot;;
        if(NonAttendancePenalty==2)
          NonAttendancePenaltyText.Text=&quot;Warning&quot;;
        if(NonAttendancePenalty==3)
          NonAttendancePenaltyText.Text=&quot;LayOff&quot;;
      }
      else
      {
           Data4.Visible=false;
        Data7.Visible=false;
        Data7.Header.Visible=false;
        
      }
    }
    private void GetAttendanceDetails(int EmpAttCardId)
    {
      DataSourceBase Mon = Report.GetDataSource(&quot;MonthAdj&quot;);
      DataSourceBase Dialy = Report.GetDataSource(&quot;DailyAdj&quot;);
      DataSourceBase Without = Report.GetDataSource(&quot;WithoutAdj&quot;);
      Mon.Init();
      if(Mon.RowCount&gt;0)
      {
        MonthAdjAttendanceResult(Mon,EmpAttCardId);
        //return 0;
      }
      //else{
        Dialy.Init();
        if(Dialy.RowCount&gt;0)
        {
          DialyAdjAttendanceResult(Dialy,EmpAttCardId);
        //return 1;
        }
      Without.Init();
      if(Without.RowCount&gt;0)
      {
        WithoutAdjAttendanceResult(Without,EmpAttCardId);
        //return 1;
      }
      
      //return -1;
      //}
    }

    
    private int  GetEmpAttCardId()
    {
      DataSourceBase emp = Report.GetDataSource(&quot;EmpAttCard&quot;);
      emp.Init();
      while(emp.HasMoreRows)
      {
        string fname=Report.GetColumnValue(&quot;EmpAttCard.Fullname&quot;).ToString();
        if (fname==ComboBox1.Text)
        {
          return   int.Parse(Report.GetColumnValue(&quot;EmpAttCard.EmployeeAttendanceCardId&quot;).ToString());
        }
        emp.Next();
      }
      return -1;
    }
    
        
    
  

    private void Data1_BeforePrint(object sender, EventArgs e)
    {
      
      int EmpAttCardId=GetEmpAttCardId();
      GetAttendanceDetails(EmpAttCardId);
