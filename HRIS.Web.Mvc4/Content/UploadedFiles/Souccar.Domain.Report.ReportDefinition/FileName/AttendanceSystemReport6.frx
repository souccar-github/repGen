    private void MonthAdjAttendanceResult(DataSourceBase Mon,int EmpAttCardId)
    {
      int SumWorkHourMonthValue=0;
      int SumVacHourMonthValue=0;
      int SumMisskHourMonthValue=0;
      int SumActualWorkHourValue=0;
      int SumOverTimeHour=0;
      bool flag=false;
      while (Mon.HasMoreRows)
      {
        // get the data column value from the current row
        int EmpattId = int.Parse( Report.GetColumnValue(&quot;MonthAdj.EmpAttCardID&quot;).ToString());
        DateTime mondate=(DateTime)Report.GetColumnValue(&quot;MonthAdj.Date&quot;);
        if(  EmpattId==EmpAttCardId)
        {
          if(mondate.Year== DateTimePicker1.Value.Year&amp;&amp; mondate.Month==DateTimePicker1.Value.Month)
          {
            // Data2.DataSource.FindObject(&quot;MonthAdj&quot;);
            flag=true;
           
            SumWorkHourMonthValue+=int.Parse(Report.GetColumnValue(&quot;MonthAdj.WorkHoursValue&quot;).ToString());
            SumVacHourMonthValue+=int.Parse(Report.GetColumnValue(&quot;MonthAdj.VacationValue&quot;).ToString());
            SumMisskHourMonthValue+= int.Parse(Report.GetColumnValue(&quot;MonthAdj.MissionValue&quot;).ToString());
            SumActualWorkHourValue+= int.Parse(Report.GetColumnValue(&quot;MonthAdj.ActualWorkHoursValue&quot;).ToString());
            SumOverTimeHour+=  int.Parse(Report.GetColumnValue(&quot;MonthAdj.OvertimeOrderValue&quot;).ToString());
            InitialOvertimeValueText.Text=  Report.GetColumnValue(&quot;MonthAdj.InitialOvertimeValue&quot;).ToString();
            FinalOvertimeValueText.Text= Report.GetColumnValue(&quot;MonthAdj.FinalOvertimeValue&quot;).ToString();
            FinalNonAttendanceValueText.Text=Report.GetColumnValue(&quot;MonthAdj.FinalNonAttendanceValue&quot;).ToString();
            PenaltyText.Text= Report.GetColumnValue(&quot;MonthAdj.Penalty&quot;).ToString();
            
          }
         
        }
        // do something with it...
        // ...
        // go next data row
        Mon.Next();
      }
      if(flag)
      {
        int TotalRequiredWorkHour=SumWorkHourMonthValue-SumVacHourMonthValue-SumMisskHourMonthValue;
        int ExpectedOverTime= 0;
        ReqWorkMonthSumValueText.Text=SumWorkHourMonthValue.ToString();
        ReqWorkHoursText.Text=TotalRequiredWorkHour.ToString();
        SumActWorkValueText.Text=SumActualWorkHourValue.ToString();
        if(SumActualWorkHourValue&gt;TotalRequiredWorkHour)
        {
          ExpectedOverTime= SumActualWorkHourValue-TotalRequiredWorkHour;
          StatusText.Text=&quot;OverTime&quot;;
          ExpectedOverTimeText.Text=ExpectedOverTime.ToString();
          InitialNonAttendanceValueText.Text=&quot;0&quot;;
        }
        else
        {
            ExpectedOverTimeText.Text=ExpectedOverTime.ToString();
          if( SumActualWorkHourValue&lt;TotalRequiredWorkHour)
          {
            StatusText.Text=&quot;NonAttendance&quot;;
            InitialNonAttendanceValueText.Text=Convert.ToString(TotalRequiredWorkHour-SumActualWorkHourValue);
          }
          else
            StatusText.Text=&quot;OK&quot;;
        }
        if(ExpectedOverTime&gt;SumOverTimeHour)
        {
             TotalOvertimeOrderValueText.Text=SumOverTimeHour.ToString();
        }
        else
          TotalOvertimeOrderValueText.Text=ExpectedOverTime.ToString();
      }
      else
      {
        Data2.Visible=false;
        Data5.Visible=false;
        Data5.Header.Visible=false;
      }
      
        
     
      
    }
    private void DialyAdjAttendanceResult(DataSourceBase Dialy,int EmpAttCardId)
    {
         //Text81.Text=&quot;DailyAdj&quot;;
      bool flag=false;
      int CountAbsence=0;
      int TotalHolidayOvertimeValue=0;
      int TotalNormalOvertimeValue=0;
      double FinalOvertimeValue=0;
      double InitialNonAttendanceValue=0;
      double FinalNonAttendanceValue=0;
      int Penalty=0;
      while(Dialy.HasMoreRows)
      {
       int EmpattId = int.Parse( Report.GetColumnValue(&quot;DailyAdj.EmpAttCardID&quot;).ToString());
        DateTime dialydate=(DateTime)Report.GetColumnValue(&quot;DailyAdj.Date&quot;);
        
        if(  EmpattId==EmpAttCardId)
        {
          if(dialydate.Year== DateTimePicker1.Value.Year&amp;&amp; dialydate.Month==DateTimePicker1.Value.Month)
          {   
            flag=true;
            if (int.Parse( Report.GetColumnValue(&quot;DailyAdj.ActualWorkHoursValue&quot;).ToString()) == 0 &amp;&amp;bool.Parse(Report.GetColumnValue(&quot;DailyAdj.IsWorkDay&quot;).ToString())  &amp;&amp;!bool.Parse(Report.GetColumnValue(&quot;DailyAdj.HasMission&quot;).ToString())  &amp;&amp;!bool.Parse(Report.GetColumnValue(&quot;DailyAdj.HasVacation&quot;).ToString()) )
            {
              CountAbsence++;
            }
            TotalHolidayOvertimeValue+=int.Parse( Report.GetColumnValue(&quot;DailyAdj.HolidayOvertimeValue&quot;).ToString());
            TotalNormalOvertimeValue+= int.Parse( Report.GetColumnValue(&quot;DailyAdj.NormalOvertimeValue&quot;).ToString());
            FinalOvertimeValue= double.Parse( Report.GetColumnValue(&quot;DailyAdj.FinalOvertimeValue&quot;).ToString());
            if(double.Parse( Report.GetColumnValue(&quot;DailyAdj.RequiredWorkHoursValue&quot;).ToString())-double.Parse( Report.GetColumnValue(&quot;DailyAdj.ActualWorkHoursValue&quot;).ToString())&gt;0)
            {
              InitialNonAttendanceValue+= double.Parse( Report.GetColumnValue(&quot;DailyAdj.RequiredWorkHoursValue&quot;).ToString())-double.Parse( Report.GetColumnValue(&quot;DailyAdj.ActualWorkHoursValue&quot;).ToString());
            }
            FinalNonAttendanceValue= double.Parse( Report.GetColumnValue(&quot;DailyAdj.FinalNonAttendanceValue&quot;).ToString());
            Penalty=int.Parse( Report.GetColumnValue(&quot;DailyAdj.Penalty&quot;).ToString());
            //Text81.Text=&quot;DialyAdj&quot;;
                //Data5.Visible=false;
                //Data5.Header.Visible=false;
                //Data2.Visible=false;
          //Data2.Clear();
          //Data5.Clear();
           // Data4.Visible=false;
            //Data7.Visible=false;
            //Data7.Header.Visible=false;
            //Data3.Visible=true;
            //Data6.Visible=true;
            //Data6.Header.Visible=true;
              
          }
        
        }
        Dialy.Next();
      }
      if(flag)
      {
        CountAbsenceText.Text=CountAbsence.ToString();
        TotalHolidayOvertimeValueText.Text=TotalHolidayOvertimeValue.ToString();
        TotalNormalOvertimeValueText.Text=TotalNormalOvertimeValue.ToString();
        FinalOvertimeValueDailyText.Text=FinalOvertimeValue.ToString();
        InitialNonAttendanceValueDailyText.Text=InitialNonAttendanceValue.ToString();
        FinalNonAttendanceValueDailyText.Text=FinalNonAttendanceValue.ToString();
        if(Penalty==0)
          PenaltyDailyText.Text=&quot;None&quot;;
        if(Penalty==1)
          PenaltyDailyText.Text=&quot;Alert&quot;;
        if(Penalty==2)
          PenaltyDailyText.Text=&quot;Warning&quot;;
        if(Penalty==3)
          PenaltyDailyText.Text=&quot;LayOff&quot;;
        }
      else
      {
        Data3.Visible=false;
        Data6.Visible=false;
        Data6.Header.Visible=false;
      }
        
    }
    private void WithoutAdjAttendanceResult(DataSourceBase Without,int EmpAttCardId)
    {
      bool flag=false;
      int CountAbsence=0;
      double FinalLatenessTotalValue=0;
      double InitialLatenessTotalValue=0;
      double InitialNonAttendanceTotalValue=0;
      double FinalNonAttendanceTotalValue=0;
