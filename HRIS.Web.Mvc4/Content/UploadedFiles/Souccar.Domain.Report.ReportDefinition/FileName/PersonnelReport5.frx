      <TableDataSource Name="Table" Alias="EmployeeListWithReligionInfoDataSource" DataType="System.Int32" Enabled="true" SelectCommand="WITH cte (Id, Name,IdList,FullName)&#13;&#10;AS(&#13;&#10;	select Id,Name,cast(Id as varchar) as IdList,cast(' - '+Name as varchar(255)) as FullName from Node where Parent_Id Is null&#13;&#10;	union all&#13;&#10;	select p.Id,p.Name,cast(c.IdList+' - '+cast(p.Id as varchar) as varchar),cast(c.FullName+' - '+p.Name as varchar(255)) from Node p&#13;&#10;	join cte c on c.Id = p.Parent_Id&#13;&#10;),&#13;&#10;EmployeeReligionInfo AS(&#13;&#10;	SELECT  dbo.JobDescription.Node_id,&#13;&#10;			--cte.FullName,&#13;&#10;			CAST(( SELECT COUNT(DISTINCT dbo.Employee.Id) FROM  dbo.Employee &#13;&#10;				INNER JOIN dbo.AssigningEmployeeToPosition ON dbo.Employee.Id = dbo.AssigningEmployeeToPosition.Employee_id &#13;&#10;				INNER JOIN dbo.Position ON dbo.AssigningEmployeeToPosition.Position_Id = dbo.Position.Id AND  dbo.AssigningEmployeeToPosition.Id = dbo.Position.AssigningEmployeeToPosition_id&#13;&#10;				INNER JOIN dbo.EmployeeCard ON dbo.Employee.Id = dbo.EmployeeCard.Employee_id&#13;&#10;				WHERE	dbo.EmployeeCard.CardStatus = 1 AND  &#13;&#10;						dbo.AssigningEmployeeToPosition.IsPrimary='True' AND &#13;&#10;						Employee.IsVertualDeleted=0 AND &#13;&#10;						AssigningEmployeeToPosition.IsVertualDeleted=0 AND &#13;&#10;						Position.IsVertualDeleted=0 AND &#13;&#10;						EmployeeCard.IsVertualDeleted=0) AS FLOAT) AS AllEmployeeCount,&#13;&#10;			Count(DISTINCT dbo.Employee.Id) AS EmployeeCountWithCurrentReligion,&#13;&#10;			dbo.Employee.Religion,&#13;&#10;			ROUND(Count(DISTINCT dbo.Employee.Id)/ &#13;&#10;			CAST(( SELECT COUNT(DISTINCT dbo.Employee.Id) FROM  dbo.Employee &#13;&#10;				INNER JOIN dbo.AssigningEmployeeToPosition ON dbo.Employee.Id = dbo.AssigningEmployeeToPosition.Employee_id &#13;&#10;				INNER JOIN dbo.Position ON dbo.AssigningEmployeeToPosition.Position_Id = dbo.Position.Id AND  dbo.AssigningEmployeeToPosition.Id = dbo.Position.AssigningEmployeeToPosition_id&#13;&#10;				INNER JOIN dbo.EmployeeCard ON dbo.Employee.Id = dbo.EmployeeCard.Employee_id&#13;&#10;				INNER JOIN dbo.JobDescription JD ON dbo.Position.JobDescription_id = JD.Id&#13;&#10;				WHERE	dbo.EmployeeCard.CardStatus = 1 AND  &#13;&#10;						dbo.AssigningEmployeeToPosition.IsPrimary='True' AND &#13;&#10;						Employee.IsVertualDeleted=0 AND &#13;&#10;						AssigningEmployeeToPosition.IsVertualDeleted=0 AND &#13;&#10;						Position.IsVertualDeleted=0 AND &#13;&#10;						EmployeeCard.IsVertualDeleted=0 AND JD.Node_id=dbo.JobDescription.Node_id) AS FLOAT) * 100,2)&#13;&#10;				AS ReligionPercentage&#13;&#10;			FROM &#13;&#10;			dbo.Employee &#13;&#10;			INNER JOIN dbo.AssigningEmployeeToPosition ON dbo.Employee.Id = dbo.AssigningEmployeeToPosition.Employee_id &#13;&#10;			INNER JOIN dbo.Position ON dbo.AssigningEmployeeToPosition.Position_Id = dbo.Position.Id AND  dbo.AssigningEmployeeToPosition.Id = dbo.Position.AssigningEmployeeToPosition_id&#13;&#10;			INNER JOIN dbo.EmployeeCard ON dbo.Employee.Id = dbo.EmployeeCard.Employee_id&#13;&#10;			INNER JOIN dbo.JobDescription ON dbo.Position.JobDescription_id = dbo.JobDescription.Id&#13;&#10;			INNER JOIN dbo.Node ON dbo.JobDescription.Node_id = dbo.Node.Id&#13;&#10;			INNER JOIN cte ON dbo.Node.Id = cte.Id &#13;&#10;	WHERE		cte.FullName LIKE '%'+ @NodeName +'%' AND&#13;&#10;				dbo.EmployeeCard.CardStatus = 1 AND  &#13;&#10;				dbo.AssigningEmployeeToPosition.IsPrimary='True' AND &#13;&#10;				Employee.IsVertualDeleted=0 AND &#13;&#10;				AssigningEmployeeToPosition.IsVertualDeleted=0 AND &#13;&#10;				Position.IsVertualDeleted=0 AND &#13;&#10;				EmployeeCard.IsVertualDeleted=0&#13;&#10;	GROUP BY Node_id,cte.FullName,dbo.Employee.Religion&#13;&#10;)&#13;&#10;SELECT &#13;&#10;Node_id,&#13;&#10;AllEmployeeCount,&#13;&#10;EmployeeCountWithCurrentReligion,&#13;&#10;ReligionPercentage,&#13;&#10;Religion,&#13;&#10;CASE WHEN Religion = 0 THEN 'مسلم' &#13;&#10;	 WHEN Religion = 1 THEN 'مسيحي'&#13;&#10;	 WHEN Religion = 2 THEN 'يهودي'&#13;&#10;	 WHEN Religion = 3 THEN 'غير ذلك'&#13;&#10;	 END AS ReligionName&#13;&#10;FROM EmployeeReligionInfo&#13;&#10;ORDER BY Node_id,Religion">
        <Column Name="Node_id" DataType="System.Int32"/>
        <Column Name="AllEmployeeCount" DataType="System.Double"/>
        <Column Name="EmployeeCountWithCurrentReligion" DataType="System.Int32"/>
        <Column Name="ReligionPercentage" DataType="System.Double"/>
        <Column Name="Religion" DataType="System.Int32"/>
        <Column Name="ReligionName" DataType="System.String"/>
        <CommandParameter Name="NodeName" DataType="22" Size="255" Expression="NodesComboBox.Text"/>
      </TableDataSource>
      <TableDataSource Name="Node" DataType="System.Int32" Enabled="true" TableName="Node" SelectCommand="SELECT N.Id, N.IsVertualDeleted, N.Name, N.Code, N.IsHistorical, N.Type_id, N.Parent_Id&#13;&#10;FROM &quot;Node&quot; N&#13;&#10;WHERE  N.IsVertualDeleted=0">
        <Column Name="Id" DataType="System.Int32"/>
        <Column Name="IsVertualDeleted" DataType="System.Boolean" BindableControl="CheckBox"/>
