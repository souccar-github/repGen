        <Column Name="Code" DataType="System.String"/>
        <Column Name="IsHistorical" DataType="System.Boolean" BindableControl="CheckBox"/>
        <Column Name="Type_id" DataType="System.Int32"/>
        <Column Name="Parent_Id" DataType="System.Int32"/>
      </TableDataSource>
    </MsSqlDataConnection>
