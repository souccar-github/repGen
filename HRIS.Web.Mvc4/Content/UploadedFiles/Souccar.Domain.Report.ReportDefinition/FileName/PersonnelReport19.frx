      <TableDataSource Name="Table1" Alias="EmployeeInfoDataSourse" DataType="System.Int32" Enabled="true" SelectCommand="WITH EmployeeInfo AS(&#13;&#10;SELECT        &#13;&#10;CAST(( SELECT COUNT(DISTINCT dbo.Employee.Id) FROM  dbo.Employee &#13;&#10;	INNER JOIN dbo.AssigningEmployeeToPosition ON dbo.Employee.Id = dbo.AssigningEmployeeToPosition.Employee_id &#13;&#10;	INNER JOIN dbo.Position ON dbo.AssigningEmployeeToPosition.Position_Id = dbo.Position.Id &#13;&#10;	INNER JOIN dbo.EmployeeCard ON dbo.Employee.Id = dbo.EmployeeCard.Employee_id&#13;&#10;	WHERE	dbo.EmployeeCard.CardStatus = 1 AND  &#13;&#10;			dbo.AssigningEmployeeToPosition.IsPrimary='True' AND &#13;&#10;			Employee.IsVertualDeleted=0 AND &#13;&#10;			AssigningEmployeeToPosition.IsVertualDeleted=0 AND &#13;&#10;			Position.IsVertualDeleted=0 AND &#13;&#10;			EmployeeCard.IsVertualDeleted=0) AS FLOAT) AS AllEmployeeCount,&#13;&#10;Count(DISTINCT dbo.Employee.Id) AS WithDisabilityExistCount,&#13;&#10;dbo.Employee.DisabilityExist,&#13;&#10;ROUND(Count(DISTINCT dbo.Employee.Id)/ &#13;&#10;CAST(( SELECT COUNT(DISTINCT dbo.Employee.Id) FROM  dbo.Employee &#13;&#10;	INNER JOIN dbo.AssigningEmployeeToPosition ON dbo.Employee.Id = dbo.AssigningEmployeeToPosition.Employee_id &#13;&#10;	INNER JOIN dbo.Position ON dbo.AssigningEmployeeToPosition.Position_Id = dbo.Position.Id &#13;&#10;	INNER JOIN dbo.EmployeeCard ON dbo.Employee.Id = dbo.EmployeeCard.Employee_id&#13;&#10;	WHERE	dbo.EmployeeCard.CardStatus = 1 AND  &#13;&#10;			dbo.AssigningEmployeeToPosition.IsPrimary='True' AND &#13;&#10;			Employee.IsVertualDeleted=0 AND &#13;&#10;			AssigningEmployeeToPosition.IsVertualDeleted=0 AND &#13;&#10;			Position.IsVertualDeleted=0 AND &#13;&#10;			EmployeeCard.IsVertualDeleted=0) AS FLOAT) * 100,2)&#13;&#10;	AS DisabilityExistPercentage&#13;&#10;FROM &#13;&#10;dbo.Employee &#13;&#10;INNER JOIN dbo.AssigningEmployeeToPosition ON dbo.Employee.Id = dbo.AssigningEmployeeToPosition.Employee_id &#13;&#10;INNER JOIN dbo.Position ON dbo.AssigningEmployeeToPosition.Position_Id = dbo.Position.Id &#13;&#10;INNER JOIN dbo.EmployeeCard ON dbo.Employee.Id = dbo.EmployeeCard.Employee_id&#13;&#10;WHERE	dbo.EmployeeCard.CardStatus = 1 AND  &#13;&#10;			dbo.AssigningEmployeeToPosition.IsPrimary='True' AND &#13;&#10;			Employee.IsVertualDeleted=0 AND &#13;&#10;			AssigningEmployeeToPosition.IsVertualDeleted=0 AND &#13;&#10;			Position.IsVertualDeleted=0 AND &#13;&#10;			EmployeeCard.IsVertualDeleted=0&#13;&#10;GROUP BY dbo.Employee.DisabilityExist&#13;&#10;)&#13;&#10;SELECT &#13;&#10;AllEmployeeCount,&#13;&#10;WithDisabilityExistCount,&#13;&#10;DisabilityExistPercentage,&#13;&#10;DisabilityExist,&#13;&#10;CASE	WHEN DisabilityExist = 0 THEN 'الموظفين الاصحاء' &#13;&#10;		WHEN DisabilityExist = 1 THEN 'الموظفين ذوي الاحتياجات الخاصة ' &#13;&#10;END AS DisabilityExistName&#13;&#10;FROM EmployeeInfo&#13;&#10;ORDER BY DisabilityExist">
        <Column Name="AllEmployeeCount" DataType="System.Double"/>
        <Column Name="WithDisabilityExistCount" DataType="System.Int32"/>
        <Column Name="DisabilityExistPercentage" DataType="System.Double"/>
        <Column Name="DisabilityExist" DataType="System.Boolean" BindableControl="CheckBox"/>
        <Column Name="DisabilityExistName" DataType="System.String"/>
