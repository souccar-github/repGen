      <TableDataSource Name="Table1" Alias="EmployeeBloodInfoDataSourse" DataType="System.Int32" Enabled="true" SelectCommand="WITH EmployeeBloodInfo AS(&#13;&#10;SELECT        &#13;&#10;CAST(( SELECT COUNT(DISTINCT dbo.Employee.Id) FROM  dbo.Employee &#13;&#10;	INNER JOIN dbo.AssigningEmployeeToPosition ON dbo.Employee.Id = dbo.AssigningEmployeeToPosition.Employee_id &#13;&#10;	INNER JOIN dbo.Position ON dbo.AssigningEmployeeToPosition.Position_Id = dbo.Position.Id&#13;&#10;	INNER JOIN dbo.EmployeeCard ON dbo.Employee.Id = dbo.EmployeeCard.Employee_id&#13;&#10;	WHERE	dbo.EmployeeCard.CardStatus = 1 AND  &#13;&#10;			dbo.AssigningEmployeeToPosition.IsPrimary='True' AND &#13;&#10;			Employee.IsVertualDeleted=0 AND &#13;&#10;			AssigningEmployeeToPosition.IsVertualDeleted=0 AND &#13;&#10;			Position.IsVertualDeleted=0 AND &#13;&#10;			EmployeeCard.IsVertualDeleted=0) AS FLOAT) AS AllEmployeeCount,&#13;&#10;Count(DISTINCT dbo.Employee.Id) AS EmployeeCountWithCurrentBlood,&#13;&#10;dbo.Employee.BloodType,&#13;&#10;ROUND(Count(DISTINCT dbo.Employee.Id)/ &#13;&#10;CAST(( SELECT COUNT(DISTINCT dbo.Employee.Id) FROM  dbo.Employee &#13;&#10;	INNER JOIN dbo.AssigningEmployeeToPosition ON dbo.Employee.Id = dbo.AssigningEmployeeToPosition.Employee_id &#13;&#10;	INNER JOIN dbo.Position ON dbo.AssigningEmployeeToPosition.Position_Id = dbo.Position.Id &#13;&#10;	INNER JOIN dbo.EmployeeCard ON dbo.Employee.Id = dbo.EmployeeCard.Employee_id&#13;&#10;	WHERE	dbo.EmployeeCard.CardStatus = 1 AND  &#13;&#10;			dbo.AssigningEmployeeToPosition.IsPrimary='True' AND &#13;&#10;			Employee.IsVertualDeleted=0 AND &#13;&#10;			AssigningEmployeeToPosition.IsVertualDeleted=0 AND &#13;&#10;			Position.IsVertualDeleted=0 AND &#13;&#10;			EmployeeCard.IsVertualDeleted=0) AS FLOAT) * 100,2)&#13;&#10;	AS BloodTypePercentage&#13;&#10;FROM &#13;&#10;dbo.Employee &#13;&#10;INNER JOIN dbo.AssigningEmployeeToPosition ON dbo.Employee.Id = dbo.AssigningEmployeeToPosition.Employee_id &#13;&#10;INNER JOIN dbo.Position ON dbo.AssigningEmployeeToPosition.Position_Id = dbo.Position.Id&#13;&#10;INNER JOIN dbo.EmployeeCard ON dbo.Employee.Id = dbo.EmployeeCard.Employee_id&#13;&#10;WHERE	dbo.EmployeeCard.CardStatus = 1 AND  &#13;&#10;			dbo.AssigningEmployeeToPosition.IsPrimary='True' AND &#13;&#10;			Employee.IsVertualDeleted=0 AND &#13;&#10;			AssigningEmployeeToPosition.IsVertualDeleted=0 AND &#13;&#10;			Position.IsVertualDeleted=0 AND &#13;&#10;			EmployeeCard.IsVertualDeleted=0&#13;&#10;GROUP BY dbo.Employee.BloodType&#13;&#10;)&#13;&#10;SELECT &#13;&#10;AllEmployeeCount,&#13;&#10;EmployeeCountWithCurrentBlood,&#13;&#10;BloodTypePercentage,&#13;&#10;BloodType,&#13;&#10;CASE WHEN BloodType = 0 THEN 'لايوجد' &#13;&#10;	 WHEN BloodType = 1 THEN 'O+'&#13;&#10;	 WHEN BloodType = 2 THEN 'O-'&#13;&#10;	 WHEN BloodType = 3 THEN 'A+'&#13;&#10;	 WHEN BloodType = 4 THEN 'A-'&#13;&#10;	 WHEN BloodType = 5 THEN 'B+'&#13;&#10;	 WHEN BloodType = 6 THEN 'B-'&#13;&#10;	 WHEN BloodType = 7 THEN 'AB+'&#13;&#10;	 WHEN BloodType = 8 THEN 'AB-'&#13;&#10;	 END AS BloodTypeName&#13;&#10;FROM EmployeeBloodInfo&#13;&#10;ORDER BY BloodType">
