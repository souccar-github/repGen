<script>
    function DisplayReport(reportId) {
        //location.href = "ReportBuilder/DisplayReport?id=" + reportId;

        var url = "ReportBuilder/DisplayReport?id=" + reportId;;
        window.open(
          url,
          '_blank'
        );
    }

    function DisplayReport(reportId) {
        $.ajax(
            {
                url: window.applicationpath + "ReportGenerator/ReportBuilder/DisplayReport",
                type: "POST",
                data: {"id":reportId},
                success: function (response) {
                    console.log(response);
                }
            });
    }
</script>